A_Apres": "A", "divisionRattachee": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "articleAdditionnel": "false", "chapitreAdditionnel": "false", "urlDivisionTexteVise": "/16/textes/2436.asp#D_Article_1er"}, "amendementStandard": {"alinea": {"avant_A_Apres": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "numero": "5", "alineaDesignation": "Alin\u00e9a 5"}}}, "corps": {"cartoucheInformatif": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenuAuteur": {"dispositif": "<p style=\"text-align: justify;\">&#x00C0; l&#x2019;alin&#x00E9;a&nbsp;5, apr&#x00E8;s le mot&nbsp;:</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;int&#x00E9;rieur&nbsp;&#x00BB;,</p><p style=\"text-align: justify;\">ins&#x00E9;rer le mot&nbsp;:</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;national,&nbsp;&#x00BB;.</p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p style=\"text-align: justify;\">Le pr&#x00E9;sent article vise &#x00E0; int&#x00E9;grer la souverainet&#x00E9; alimentaire &#x00E0; la liste des int&#x00E9;r&#x00EA;ts fondamentaux de la Nation, au sein du code rural et de la p&#x00EA;che maritime.</p><p>Dans sa r&#x00E9;daction actuelle, il n'est fait r&#x00E9;f&#x00E9;rence qu'&#x00E0; sa capacit&#x00E9; &#x00E0; assurer son approvisionnement alimentaire dans le cadre du march&#x00E9; int&#x00E9;rieur de l'Union europ&#x00E9;enne. Il convient d'ajouter la dimension nationale pour pouvoir tendre &#x00E0; une v&#x00E9;ritable souverainet&#x00E9; alimentaire dans notre Pays.&nbsp;</p><p>Cet amendement vise donc &#x00E0; ins&#x00E9;rer le mot \"national\" apr&#x00E8;s les mots \"march&#x00E9; int&#x00E9;rieur\".</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2024-04-29", "datePublication": "2024-04-30", "soumisArticle40": "false", "etatDesTraitements": {"etat": {"code": "DI", "libelle": "Discut\u00e9"}, "sousEtat": {"code": "DI", "libelle": "Rejet\u00e9"}}, "dateSort": "2024-05-01T00:00:47+02:00", "sort": "Rejet\u00e9"}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L16PO419610B2436P0D1N003413?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "seanceDiscussionRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "article99": "false", "loiReference": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionCommune": {"idDiscussion": "24441", "typePosition": "Milieu"}, "discussionIdentique": {"idDiscussion": "69157", "typePosition": "Premier"}, "accordGouvernementDepotHorsDelai": "Sans objet"}}