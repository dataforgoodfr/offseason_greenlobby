{"amendement": {"@xmlns": "http://schemas.assemblee-nationale.fr/referentiel", "uid": "AMANR5L16PO419865B2436P0D1N000464", "chronotag": "1714400355496", "legislature": "16", "identification": {"numeroLong": "CD464", "numeroOrdreDepot": "464", "prefixeOrganeExamen": "CION-DVP", "numeroRect": "0"}, "examenRef": "EXANR5L16PO419865B2436P0D1", "texteLegislatifRef": "PRJLANR5L16B2436", "triAmendement": "aeaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaabnzzx", "cardinaliteAmdtMultiples": "1", "amendementParentRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "signataires": {"auteur": {"typeAuteur": "D\u00e9put\u00e9", "gouvernementRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "acteurRef": "PA793664", "groupePolitiqueRef": "PO800526", "auteurRapporteurOrganeRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "cosignataires": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "suffixe": "et les membres du groupe \u00c9cologiste - NUPES", "libelle": "Mme&#160;Pochon et les membres du groupe &#201;cologiste - NUPES"}, "pointeurFragmentTexte": {"partieAmendableRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "division": {"titre": "Article PREMIER", "articleDesignationCourte": "ART. PREMIER", "articleDesignation": "ARTICLE PREMIER", "type": "ARTICLE", "avant_A_Apres": "A", "divisionRattachee": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "articleAdditionnel": "false", "chapitreAdditionnel": "false", "urlDivisionTexteVise": "/16/textes/2436.asp#D_Article_1er"}, "amendementStandard": {"alinea": {"avant_A_Apres": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "numero": "5", "alineaDesignation": "Alin\u00e9a 5"}}}, "corps": {"cartoucheInformatif": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenuAuteur": {"dispositif": "<p style=\"text-align: justify;\">&#x00C0; l&#x2019;alin&#x00E9;a&nbsp;5, apr&#x00E8;s le mot&nbsp;:</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;nutritive,&nbsp;&#x00BB;,&nbsp;</p><p style=\"text-align: justify;\">ins&#x00E9;rer les mots&nbsp;:</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;reposant davantage sur les prot&#x00E9;ines v&#x00E9;g&#x00E9;tales,&nbsp;&#x00BB;.</p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p style=\"text-align: justify;\">Cet amendement int&#x00E8;gre la n&#x00E9;cessit&#x00E9; de d&#x00E9;velopper la consommation de prot&#x00E9;ines v&#x00E9;g&#x00E9;tales et les fili&#x00E8;res alimentaires des prot&#x00E9;ines v&#x00E9;g&#x00E9;tales, concourant ainsi aux objectifs fix&#x00E9;s dans la strat&#x00E9;gie nationale sur les prot&#x00E9;ines v&#x00E9;g&#x00E9;tales port&#x00E9;e par le gouvernement, permettant &#x00E0; la fois aux &#x00E9;leveurs d&#x2019;am&#x00E9;liorer leur autonomie pour l&#x2019;alimentation de leurs animaux, et &#x00E0; encourager les Fran&#x00E7;ais &#x00E0; augmenter leur consommation de prot&#x00E9;ines v&#x00E9;g&#x00E9;tales, pour r&#x00E9;pondre aux nouvelles recommandations nutritionnelles. En d&#x00E9;cembre 2020, le Ministre de l&#x2019;Agriculture de l&#x2019;&#x00E9;poque l&#x2019;affirmait : &#x201C;Ma priorite&#x0301; est claire : nous devons impe&#x0301;rativement regagner en souverainete&#x0301; agroalimentaire, et cela ne peut se faire sans le de&#x0301;veloppement d&#x2019;une production franc&#x0327;aise de prote&#x0301;ines ve&#x0301;ge&#x0301;tales.&#x201D; Il est ainsi n&#x00E9;cessaire de mentionner dans ce projet de loi cet imp&#x00E9;ratif. Tel est l&#x2019;objet de cet amendement.&nbsp;</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2024-04-25", "datePublication": "2024-04-29", "soumisArticle40": "false", "etatDesTraitements": {"etat": {"code": "DI", "libelle": "Discut\u00e9"}, "sousEtat": {"code": "DI", "libelle": "Tomb\u00e9"}}, "dateSort": "2024-04-29T16:18:52+02:00", "sort": "Tomb\u00e9"}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L16PO419865B2436P0D1N000464?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "seanceDiscussionRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "article99": "false", "loiReference": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionCommune": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionIdentique": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "accordGouvernementDepotHorsDelai": "Sans objet"}}