{"amendement": {"@xmlns": "http://schemas.assemblee-nationale.fr/referentiel", "uid": "AMANR5L16PO419610B2436P0D1N002448", "chronotag": "1714658513482", "legislature": "16", "identification": {"numeroLong": "CE2448", "numeroOrdreDepot": "2448", "prefixeOrganeExamen": "CION-ECO", "numeroRect": "0"}, "examenRef": "EXANR5L16PO419610B2436P0D1", "texteLegislatifRef": "PRJLANR5L16B2436", "triAmendement": "aeaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaafzzzzxzzzzzzzzzzzzzz", "cardinaliteAmdtMultiples": "1", "amendementParentRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "signataires": {"auteur": {"typeAuteur": "D\u00e9put\u00e9", "gouvernementRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "acteurRef": "PA607090", "groupePolitiqueRef": "PO800508", "auteurRapporteurOrganeRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "cosignataires": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "suffixe": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "libelle": "M.&#160;Jean-Pierre&#160;Vigier"}, "pointeurFragmentTexte": {"partieAmendableRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "division": {"titre": "Article PREMIER", "articleDesignationCourte": "ART. PREMIER", "articleDesignation": "ARTICLE PREMIER", "type": "ARTICLE", "avant_A_Apres": "A", "divisionRattachee": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "articleAdditionnel": "false", "chapitreAdditionnel": "false", "urlDivisionTexteVise": "/16/textes/2436.asp#D_Article_1er"}, "amendementStandard": {"alinea": {"avant_A_Apres": "Apres", "numero": "11", "alineaDesignation": "Apr\u00e8s l'alin\u00e9a 11"}}}, "corps": {"cartoucheInformatif": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenuAuteur": {"dispositif": "<p style=\"text-align: justify;\">Apr&#x00E8;s l&#x2019;alin&#x00E9;a&nbsp;11, ins&#x00E9;rer les deux alin&#x00E9;as suivants&nbsp;:</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;Apr&#x00E8;s le 12&#x00B0;&nbsp;du I de l&#x2019;article L.&nbsp;1 du Code rural et de la p&#x00EA;che maritime, il est ins&#x00E9;r&#x00E9; un 12&#x00B0;&nbsp;<i>bis</i> ainsi r&#x00E9;dig&#x00E9;&nbsp;:</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;12&#x00B0;&nbsp;<i>bis</i>&nbsp;De pr&#x00E9;server et d&#x2019;am&#x00E9;liorer la souverainet&#x00E9; agricole du pays, li&#x00E9;e &#x00E0; la production durable de biomasse sur le territoire et &#x00E0; la contribution du secteur &#x00E0; la d&#x00E9;carbonation de l&#x2019;&#x00E9;conomie via notamment ses productions &#x00E9;nerg&#x00E9;tiques actuelles et en d&#x00E9;veloppement&nbsp;;&nbsp;&#x00BB;.</p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p style=\"text-align: justify;\">La protection de la souverainet&#x00E9; agricole de notre pays vise avant tout &#x00E0; prot&#x00E9;ger notre souverainet&#x00E9; alimentaire mais &#x00E9;galement la contribution de l&#x2019;agriculture dans la production d&#x2019;&#x00E9;nergie renouvelable&nbsp;: Biocarburants de 1<sup>&#x00E8;re</sup> g&#x00E9;n&#x00E9;ration et avanc&#x00E9;s, biogaz, &#x00E9;lectricit&#x00E9; via la cog&#x00E9;n&#x00E9;ration. Ces productions contribuent significativement au mix-&#x00E9;nerg&#x00E9;tique national et permettent de produire &#x00E9;galement des mati&#x00E8;res riches en prot&#x00E9;ines (tourteaux, dr&#x00E8;ches, &#x2026;) &#x00E0; destination de l&#x2019;&#x00E9;levage et limitent notre d&#x00E9;pendance aux importations.</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2024-04-26", "datePublication": "2024-04-29", "soumisArticle40": "false", "etatDesTraitements": {"etat": {"code": "DI", "libelle": "Discut\u00e9"}, "sousEtat": {"code": "DI", "libelle": "Non soutenu"}}, "dateSort": "2024-05-02T16:01:42+02:00", "sort": "Non soutenu"}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L16PO419610B2436P0D1N002448?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "seanceDiscussionRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "article99": "false", "loiReference": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionCommune": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionIdentique": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "accordGouvernementDepotHorsDelai": "Sans objet"}}