{"amendement": {"@xmlns": "http://schemas.assemblee-nationale.fr/referentiel", "uid": "AMANR5L16PO419610B2436P0D1N002382", "chronotag": "1714776027882", "legislature": "16", "identification": {"numeroLong": "CE2382", "numeroOrdreDepot": "2382", "prefixeOrganeExamen": "CION-ECO", "numeroRect": "0"}, "examenRef": "EXANR5L16PO419610B2436P0D1", "texteLegislatifRef": "PRJLANR5L16B2436", "triAmendement": "iaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaczzzzzzi", "cardinaliteAmdtMultiples": "1", "amendementParentRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "signataires": {"auteur": {"typeAuteur": "D\u00e9put\u00e9", "gouvernementRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "acteurRef": "PA722202", "groupePolitiqueRef": "PO800502", "auteurRapporteurOrganeRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "cosignataires": {"acteurRef": ["PA267306", "PA720838", "PA796106", "PA795868", "PA795192", "PA718926", "PA721896", "PA795998", "PA795184", "PA774962", "PA335612", "PA796010", "PA793174", "PA795820", "PA721270", "PA795240", "PA795876", "PA720692", "PA605745", "PA794650", "PA720992"]}, "suffixe": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "libelle": "M.&#160;Jumel,  M.&#160;Chassaigne, Mme&#160;Bourouaha, M.&#160;B&#233;nard, M.&#160;Castor, M.&#160;Chailloux, M.&#160;Dharr&#233;ville, Mme&#160;Faucillon, Mme&#160;K/Bidi, M.&#160;Le&#160;Gayic, Mme&#160;Lebon, M.&#160;Lecoq, M.&#160;Maillot, M.&#160;Monnet, M.&#160;Nadeau, M.&#160;Peu, Mme&#160;Reid&#160;Arbelot, M.&#160;Rimane, M.&#160;Roussel, M.&#160;Sansu, M.&#160;Tellier et M.&#160;William"}, "pointeurFragmentTexte": {"partieAmendableRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "division": {"titre": "Article 12", "articleDesignationCourte": "ART. 12", "articleDesignation": "ARTICLE 12", "type": "ARTICLE", "avant_A_Apres": "A", "divisionRattachee": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "articleAdditionnel": "false", "chapitreAdditionnel": "false", "urlDivisionTexteVise": "/16/textes/2436.asp#D_Article_12"}, "amendementStandard": {"alinea": {"avant_A_Apres": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "numero": "20", "alineaDesignation": "Alin\u00e9a 20"}}}, "corps": {"cartoucheInformatif": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenuAuteur": {"dispositif": "<p style=\"text-align: justify;\">Compl&#x00E9;ter l&#x2019;alin&#x00E9;a&nbsp;20 par les mots&nbsp;:&nbsp;</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;et il pr&#x00E9;cise les modalit&#x00E9;s de valorisation et r&#x00E9;&#x00E9;valuation de ces actifs&nbsp;&#x00BB; .</p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p style=\"text-align: justify;\">Le d&#x00E9;veloppement des GFAI va conduire &#x00E0; la mise en place d&#x2019;un march&#x00E9; des parts de GFA, dans un premier temps lors de la cr&#x00E9;ation du GFAI puis lors des cessions de parts sociales au cours de la vie du GFAI. En l&#x2019;absence de r&#x00E9;gulation du march&#x00E9; des parts de GFAI, le risque de sp&#x00E9;culation&nbsp; sur terre agricole sera majeur comme cela est d&#x2019;ores et d&#x00E9;j&#x00E0; observ&#x00E9; en for&#x00EA;t. Ainsi, il y a un risque de d&#x00E9;stabilisation des march&#x00E9;s fonciers &#x00E0; la hausse en cas de surcro&#x00EE;t d&#x2019;int&#x00E9;r&#x00EA;t de la part des investisseurs ou &#x00E0; la baisse dans le cas contraire. Pour &#x00E9;viter un risque de survalorisation des actifs dans un objectif de rendement financier, il est indispensable de soumettre la valorisation des actifs et notamment leur &#x00E9;valuation annuelle &#x00E0; l&#x2019;avis d&#x2019;un tiers (expert foncier, SAFER, chambre d&#x2019;agriculture&#x2026;).</p><p style=\"text-align: justify;\">Cet amendement issu des propositions des Jeunes Agriculteurs vise d&#x00E9;finir par d&#x00E9;cret les modalit&#x00E9;s de valorisation et de r&#x00E9;&#x00E9;valuation des actifs.</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2024-04-26", "datePublication": "2024-04-30", "soumisArticle40": "false", "etatDesTraitements": {"etat": {"code": "DI", "libelle": "Discut\u00e9"}, "sousEtat": {"code": "DI", "libelle": "Tomb\u00e9"}}, "dateSort": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "sort": "Tomb\u00e9"}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L16PO419610B2436P0D1N002382?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "seanceDiscussionRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "article99": "false", "loiReference": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionCommune": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionIdentique": {"idDiscussion": "69246", "typePosition": "Dernier"}, "accordGouvernementDepotHorsDelai": "Sans objet"}}