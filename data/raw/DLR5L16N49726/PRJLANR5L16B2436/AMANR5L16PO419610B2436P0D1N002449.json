{"amendement": {"@xmlns": "http://schemas.assemblee-nationale.fr/referentiel", "uid": "AMANR5L16PO419610B2436P0D1N002449", "chronotag": "1714664153426", "legislature": "16", "identification": {"numeroLong": "CE2449", "numeroOrdreDepot": "2449", "prefixeOrganeExamen": "CION-ECO", "numeroRect": "0"}, "examenRef": "EXANR5L16PO419610B2436P0D1", "texteLegislatifRef": "PRJLANR5L16B2436", "triAmendement": "aeaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaafzzzzznzzzxo", "cardinaliteAmdtMultiples": "1", "amendementParentRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "signataires": {"auteur": {"typeAuteur": "D\u00e9put\u00e9", "gouvernementRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "acteurRef": "PA607090", "groupePolitiqueRef": "PO800508", "auteurRapporteurOrganeRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "cosignataires": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "suffixe": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "libelle": "M.&#160;Jean-Pierre&#160;Vigier"}, "pointeurFragmentTexte": {"partieAmendableRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "division": {"titre": "Article PREMIER", "articleDesignationCourte": "ART. PREMIER", "articleDesignation": "ARTICLE PREMIER", "type": "ARTICLE", "avant_A_Apres": "A", "divisionRattachee": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "articleAdditionnel": "false", "chapitreAdditionnel": "false", "urlDivisionTexteVise": "/16/textes/2436.asp#D_Article_1er"}, "amendementStandard": {"alinea": {"avant_A_Apres": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "numero": "14", "alineaDesignation": "Alin\u00e9a 14"}}}, "corps": {"cartoucheInformatif": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenuAuteur": {"dispositif": "<p style=\"text-align: justify;\">R&#x00E9;diger ainsi l&#x2019;alin&#x00E9;a&nbsp;14&nbsp;:</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;&#x00C0; ce titre, elle oriente en priorit&#x00E9; l&#x2019;installation en agriculture vers des syst&#x00E8;mes de production strat&#x00E9;giques pour la souverainet&#x00E9; alimentaire et les transitions &#x00E9;cologique et climatique, adapt&#x00E9;s aux enjeux de chaque territoire, dans une logique de diversification, et notamment vers l&#x2019;agro&#x00E9;cologie dont l&#x2019;agriculture biologique, &#x00E0; travers des mesures visant &#x00E0;&nbsp;:&nbsp;&#x00BB;<br><br></p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p style=\"text-align: justify;\">La r&#x00E9;daction actuelle de l&#x2019;alin&#x00E9;a&nbsp;14 de l&#x2019;article&nbsp;1 introduit la notion d&#x2019;orientation &#x00AB;&nbsp;prioritaire&nbsp;&#x00BB; de la politique d&#x2019;installation et de transmission au profit de &#x00AB;&nbsp;secteurs strat&#x00E9;giques pour la souverainet&#x00E9; alimentaire&nbsp;&#x00BB;. Une telle d&#x00E9;marche de priorisation pourrait aboutir &#x00E0; des situations de blocage pour des exploitation pourtant viable &#x00E9;conomiquement. De plus, le march&#x00E9; ne permet pas de prioriser un secteur agricole au d&#x00E9;triment d&#x2019;un autre. Cet amendement revient donc sur cette notion, tout en confortant le lien entre la politique d&#x2019;installation et la n&#x00E9;cessaire pr&#x00E9;servation et am&#x00E9;lioration de la souverainet&#x00E9; alimentaire et &#x00E9;nerg&#x00E9;tique.</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2024-04-26", "datePublication": "2024-04-29", "soumisArticle40": "false", "etatDesTraitements": {"etat": {"code": "DI", "libelle": "Discut\u00e9"}, "sousEtat": {"code": "DI", "libelle": "Non soutenu"}}, "dateSort": "2024-05-02T17:35:48+02:00", "sort": "Non soutenu"}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L16PO419610B2436P0D1N002449?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "seanceDiscussionRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "article99": "false", "loiReference": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionCommune": {"idDiscussion": "24463", "typePosition": "Milieu"}, "discussionIdentique": {"idDiscussion": "69548", "typePosition": "Milieu"}, "accordGouvernementDepotHorsDelai": "Sans objet"}}