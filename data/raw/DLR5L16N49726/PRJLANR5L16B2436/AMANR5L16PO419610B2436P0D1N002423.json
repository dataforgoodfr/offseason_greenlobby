{"amendement": {"@xmlns": "http://schemas.assemblee-nationale.fr/referentiel", "uid": "AMANR5L16PO419610B2436P0D1N002423", "chronotag": "1714767657054", "legislature": "16", "identification": {"numeroLong": "CE2423", "numeroOrdreDepot": "2423", "prefixeOrganeExamen": "CION-ECO", "numeroRect": "0"}, "examenRef": "EXANR5L16PO419610B2436P0D1", "texteLegislatifRef": "PRJLANR5L16B2436", "triAmendement": "faaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaado", "cardinaliteAmdtMultiples": "1", "amendementParentRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "signataires": {"auteur": {"typeAuteur": "D\u00e9put\u00e9", "gouvernementRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "acteurRef": "PA721678", "groupePolitiqueRef": "PO800538", "auteurRapporteurOrganeRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "cosignataires": {"acteurRef": ["PA794658", "PA795528", "PA794974", "PA643184", "PA712014", "PA720054", "PA794878", "PA718962", "PA719100", "PA793500", "PA719550", "PA721170", "PA721632", "PA793194", "PA794486", "PA794386", "PA345722"]}, "suffixe": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "libelle": "M.&#160;Reda,  M.&#160;Fait, M.&#160;Rodwell, M.&#160;Margueritte, Mme&#160;Klinkert, M.&#160;Ledoux, M.&#160;Haury, M.&#160;Mournet, M.&#160;Zulesi, M.&#160;Ardouin, M.&#160;Bordat, Mme&#160;Liliana&#160;Tanguy, M.&#160;Vojetta, Mme&#160;Lemoine, M.&#160;Vuibert, Mme&#160;Spillebout, Mme&#160;M&#233;tayer et Mme&#160;Le&#160;Grip"}, "pointeurFragmentTexte": {"partieAmendableRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "division": {"titre": "Article 10", "articleDesignationCourte": "ART. 10", "articleDesignation": "ARTICLE 10", "type": "ARTICLE", "avant_A_Apres": "A", "divisionRattachee": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "articleAdditionnel": "false", "chapitreAdditionnel": "false", "urlDivisionTexteVise": "/16/textes/2436.asp#D_Article_10"}, "amendementStandard": {"alinea": {"avant_A_Apres": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "numero": "9", "alineaDesignation": "Alin\u00e9a 9"}}}, "corps": {"cartoucheInformatif": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenuAuteur": {"dispositif": "<p style=\"text-align: justify;\">&#x00C0; l&#x2019;alin&#x00E9;a&nbsp;9, substituer aux mots&nbsp;:</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;l&#x2019;&#x00E9;tablissement public local d&#x2019;enseignement et de formation professionnelle agricole&nbsp;&#x00BB;</p><p style=\"text-align: justify;\">les mots&nbsp;:</p><p style=\"text-align: justify;\">&#x00AB; l&#x2019;un des &#x00E9;tablissements d&#x2019;enseignement et de formation professionnelle agricole d&#x00E9;finis aux articles L.&nbsp;811&#x2011;1 et L.&nbsp;813&#x2011;1 le plus adapt&#x00E9; au projet du candidat&nbsp;&#x00BB;.</p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p style=\"text-align: justify;\">Cet amendement propose de reconna&#x00EE;tre la diversit&#x00E9; des besoins et des aspirations des &#x00E9;tudiants en mati&#x00E8;re de formation agricole. En permettant aux candidats de choisir l&#x2019;&#x00E9;tablissement le plus adapt&#x00E9; &#x00E0; leur projet professionnel, on augmente la probabilit&#x00E9; de r&#x00E9;ussite et de satisfaction dans leur parcours de formation. Cette approche individualis&#x00E9;e aide &#x00E0; mieux aligner les comp&#x00E9;tences acquises avec les exigences du march&#x00E9; du travail et les sp&#x00E9;cificit&#x00E9;s r&#x00E9;gionales.<br>&nbsp;<br>&nbsp;Cet amendement permet &#x00E9;galement une meilleure utilisation des ressources et des comp&#x00E9;tences disponibles dans diff&#x00E9;rents &#x00E9;tablissements. En diversifiant les options de formation pour les &#x00E9;tudiants, on encourage les &#x00E9;tablissements &#x00E0; se sp&#x00E9;cialiser et &#x00E0; innover dans leurs domaines de comp&#x00E9;tence.<br>&nbsp;<br>Cet amendement vise donc &#x00E0; moderniser et &#x00E0; personnaliser l&#x2019;enseignement agricole en France, en accordant aux &#x00E9;tudiants la libert&#x00E9; de choisir l&#x2019;&#x00E9;tablissement qui correspond le mieux &#x00E0; leurs objectifs professionnels et personnels, tout en renfor&#x00E7;ant la sp&#x00E9;cialisation et l&#x2019;innovation au sein des &#x00E9;tablissements d&#x2019;enseignement et de formation professionnelle agricole.</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2024-04-26", "datePublication": "2024-04-30", "soumisArticle40": "false", "etatDesTraitements": {"etat": {"code": "DI", "libelle": "Discut\u00e9"}, "sousEtat": {"code": "DI", "libelle": "Tomb\u00e9"}}, "dateSort": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "sort": "Tomb\u00e9"}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L16PO419610B2436P0D1N002423?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "seanceDiscussionRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "article99": "false", "loiReference": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionCommune": {"idDiscussion": "24547", "typePosition": "Milieu"}, "discussionIdentique": {"idDiscussion": "69489", "typePosition": "Milieu"}, "accordGouvernementDepotHorsDelai": "Sans objet"}}