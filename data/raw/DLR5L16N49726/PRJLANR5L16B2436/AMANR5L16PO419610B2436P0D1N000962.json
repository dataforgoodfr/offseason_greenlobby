{"amendement": {"@xmlns": "http://schemas.assemblee-nationale.fr/referentiel", "uid": "AMANR5L16PO419610B2436P0D1N000962", "chronotag": "1714507973449", "legislature": "16", "identification": {"numeroLong": "CE962", "numeroOrdreDepot": "962", "prefixeOrganeExamen": "CION-ECO", "numeroRect": "0"}, "examenRef": "EXANR5L16PO419610B2436P0D1", "texteLegislatifRef": "PRJLANR5L16B2436", "triAmendement": "aeaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaafyx", "cardinaliteAmdtMultiples": "1", "amendementParentRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "signataires": {"auteur": {"typeAuteur": "D\u00e9put\u00e9", "gouvernementRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "acteurRef": "PA793476", "groupePolitiqueRef": "PO800508", "auteurRapporteurOrganeRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "cosignataires": {"acteurRef": ["PA718838", "PA712015", "PA719272", "PA793094", "PA794178", "PA721314", "PA346054", "PA721442"]}, "suffixe": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "libelle": "M.&#160;Dubois,  M.&#160;Fabrice&#160;Brun, M.&#160;Dive, Mme&#160;Fr&#233;d&#233;rique&#160;Meunier, M.&#160;Ray, M.&#160;Taite, M.&#160;Schellenberger, M.&#160;Emmanuel&#160;Maquet et Mme&#160;Petex"}, "pointeurFragmentTexte": {"partieAmendableRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "division": {"titre": "Article PREMIER", "articleDesignationCourte": "ART. PREMIER", "articleDesignation": "ARTICLE PREMIER", "type": "ARTICLE", "avant_A_Apres": "A", "divisionRattachee": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "articleAdditionnel": "false", "chapitreAdditionnel": "false", "urlDivisionTexteVise": "/16/textes/2436.asp#D_Article_1er"}, "amendementStandard": {"alinea": {"avant_A_Apres": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "numero": "3", "alineaDesignation": "Alin\u00e9a 3"}}}, "corps": {"cartoucheInformatif": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenuAuteur": {"dispositif": "<p style=\"text-align: justify;\">&#x00C0; l&#x2019;alin&#x00E9;a 3, apr&#x00E8;s les mots&nbsp;:&nbsp;</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;l&#x2019;agriculture&nbsp;&#x00BB;,</p><p>ins&#x00E9;rer les mots et la ponctuation&nbsp;:&nbsp;</p><p>&#x00AB;,&nbsp;l&#x2019;&#x00E9;levage&nbsp;&#x00BB;.</p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p style=\"text-align: justify;\">Cet article vise &#x00E0; int&#x00E9;grer la souverainet&#x00E9; alimentaire &#x00E0; la liste des int&#x00E9;r&#x00EA;ts fondamentaux de la Nation, au sein du code rural et de la p&#x00EA;che maritime.</p><p style=\"text-align: justify;\">Or la France est aujourd&#x2019;hui le premier producteur europ&#x00E9;en de viande bovine, le second producteur laitier europ&#x00E9;en, le 3eme producteur europ&#x00E9;en de volailles. Elle&nbsp;poss&#x00E8;de &#x00E9;galement le troisi&#x00E8;me cheptel porcin.</p><p style=\"text-align: justify;\">Au total, l&#x2019;&#x00E9;levage en France repr&#x00E9;sente pr&#x00E8;s de 40&nbsp;% des exploitations agricoles. C&#x2019;est une agriculture d&#x2019;excellence, labellis&#x00E9;e, durable et respectueuse des paysages et de l&#x2019;environnement.</p><p style=\"text-align: justify;\">&nbsp;</p><p style=\"text-align: justify;\">A ce titre, il est essentiel d&#x2019;ajouter explicitement dans cette liste l&#x2019;&#x00E9;levage, fleuron national fran&#x00E7;ais. Tel est le sens de cet amendement.</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2024-04-23", "datePublication": "2024-04-30", "soumisArticle40": "false", "etatDesTraitements": {"etat": {"code": "DI", "libelle": "Discut\u00e9"}, "sousEtat": {"code": "DI", "libelle": "Rejet\u00e9"}}, "dateSort": "2024-04-30T22:12:27+02:00", "sort": "Rejet\u00e9"}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L16PO419610B2436P0D1N000962?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "seanceDiscussionRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "article99": "false", "loiReference": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionCommune": {"idDiscussion": "24417", "typePosition": "Milieu"}, "discussionIdentique": {"idDiscussion": "69442", "typePosition": "Milieu"}, "accordGouvernementDepotHorsDelai": "Sans objet"}}