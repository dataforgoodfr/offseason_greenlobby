{"amendement": {"@xmlns": "http://schemas.assemblee-nationale.fr/referentiel", "uid": "AMANR5L16PO419610B2436P0D1N001227", "chronotag": "1714734053442", "legislature": "16", "identification": {"numeroLong": "CE1227", "numeroOrdreDepot": "1227", "prefixeOrganeExamen": "CION-ECO", "numeroRect": "0"}, "examenRef": "EXANR5L16PO419610B2436P0D1", "texteLegislatifRef": "PRJLANR5L16B2436", "triAmendement": "daaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaai", "cardinaliteAmdtMultiples": "1", "amendementParentRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "signataires": {"auteur": {"typeAuteur": "D\u00e9put\u00e9", "gouvernementRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "acteurRef": "PA330909", "groupePolitiqueRef": "PO800508", "auteurRapporteurOrganeRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "cosignataires": {"acteurRef": ["PA718838", "PA266793", "PA721410", "PA720586", "PA794178", "PA795406", "PA721442", "PA643134", "PA342240", "PA793094", "PA721474", "PA719272", "PA712015", "PA608416", "PA722390", "PA793476", "PA608826", "PA719968", "PA718850"]}, "suffixe": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "libelle": "M.&#160;Descoeur,  M.&#160;Fabrice&#160;Brun, M.&#160;Bony, Mme&#160;Bonnivard, M.&#160;Vatin, M.&#160;Taite, Mme&#160;P&#233;rigault, Mme&#160;Petex, Mme&#160;Valentin, M.&#160;Bourgeaux, M.&#160;Ray, M.&#160;Viry, Mme&#160;Fr&#233;d&#233;rique&#160;Meunier, M.&#160;Dive, M.&#160;Hetzel, Mme&#160;Corneloup, M.&#160;Dubois, Mme&#160;Duby-Muller, Mme&#160;Bonnet et M.&#160;Cordier"}, "pointeurFragmentTexte": {"partieAmendableRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "division": {"titre": "TITRE III", "articleDesignationCourte": "AVANT ART. 8", "articleDesignation": "AVANT L'ARTICLE 8", "type": "CHAPITRE", "avant_A_Apres": "A", "divisionRattachee": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "articleAdditionnel": "false", "chapitreAdditionnel": "false", "urlDivisionTexteVise": "/16/textes/2436.asp#D_TITRE_III_13"}, "amendementStandard": {"alinea": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "corps": {"cartoucheInformatif": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenuAuteur": {"dispositif": "<p style=\"text-align: justify;\">Compl&#x00E9;ter l&#x2019;intitul&#x00E9; du titre III par les mots&nbsp;: &#x00AB;&nbsp;et leurs revenus&nbsp;&#x00BB;.</p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p style=\"text-align: justify;\">La principale revendication exprim&#x00E9;e par les agriculteurs lors des manifestations de ce d&#x00E9;but d&#x2019;ann&#x00E9;e porte sur le revenu agricole. Or, ce projet de loi -qui se veut pourtant d&#x2019;orientation- n&#x2019;&#x00E9;voque &#x00E0; aucun moment la question du revenu des agriculteurs, pourtant essentielle &#x00E0; l&#x2019;atteinte de l&#x2019;objectif de souverainet&#x00E9; alimentaire. Sans revenus qui assurent aux agriculteurs une juste r&#x00E9;mun&#x00E9;ration et une capacit&#x00E9; &#x00E0; investir sur leur exploitation, le renouvellement des g&#x00E9;n&#x00E9;rations ne pourra pas &#x00EA;tre assur&#x00E9; et le nombre d&#x2019;exploitations agricoles continuera de diminuer, compromettant l&#x2019;objectif de souverainet&#x00E9; alimentaire affich&#x00E9; par ce projet de loi. C&#x2019;est pourquoi le pr&#x00E9;sent amendement vise &#x00E0; inscrire l&#x2019;objectif d&#x2019;augmentation du revenu des agriculteurs en titre III.&nbsp;</p><p>&nbsp;</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2024-04-24", "datePublication": "2024-04-29", "soumisArticle40": "false", "etatDesTraitements": {"etat": {"code": "DI", "libelle": "Discut\u00e9"}, "sousEtat": {"code": "DI", "libelle": "Rejet\u00e9"}}, "dateSort": "2024-05-03T13:00:45+02:00", "sort": "Rejet\u00e9"}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L16PO419610B2436P0D1N001227?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "seanceDiscussionRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "article99": "false", "loiReference": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionCommune": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionIdentique": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "accordGouvernementDepotHorsDelai": "Sans objet"}}