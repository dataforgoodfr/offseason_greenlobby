{"amendement": {"@xmlns": "http://schemas.assemblee-nationale.fr/referentiel", "uid": "AMANR5L16PO419610B2436P0D1N002361", "chronotag": "1714817603414", "legislature": "16", "identification": {"numeroLong": "CE2361", "numeroOrdreDepot": "2361", "prefixeOrganeExamen": "CION-ECO", "numeroRect": "0"}, "examenRef": "EXANR5L16PO419610B2436P0D1", "texteLegislatifRef": "PRJLANR5L16B2436", "triAmendement": "jiaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaanyjh", "cardinaliteAmdtMultiples": "1", "amendementParentRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "signataires": {"auteur": {"typeAuteur": "D\u00e9put\u00e9", "gouvernementRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "acteurRef": "PA794206", "groupePolitiqueRef": "PO800538", "auteurRapporteurOrganeRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "cosignataires": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "suffixe": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "libelle": "M.&#160;Marion"}, "pointeurFragmentTexte": {"partieAmendableRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "division": {"titre": "Article 14", "articleDesignationCourte": "ART. 14", "articleDesignation": "ARTICLE 14", "type": "ARTICLE", "avant_A_Apres": "A", "divisionRattachee": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "articleAdditionnel": "false", "chapitreAdditionnel": "false", "urlDivisionTexteVise": "/16/textes/2436.asp#D_Article_14"}, "amendementStandard": {"alinea": {"avant_A_Apres": "Apres", "numero": "24", "alineaDesignation": "Apr\u00e8s l'alin\u00e9a 24"}}}, "corps": {"cartoucheInformatif": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenuAuteur": {"dispositif": "<p style=\"text-align: justify;\">Apr&#x00E8;s l&#x2019;alin&#x00E9;a&nbsp;24, ins&#x00E9;rer les alin&#x00E9;as suivants&nbsp;:</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;11&#x00B0;&nbsp;Autorisation ou absence d&#x2019;opposition en cas de n&#x00E9;cessit&#x00E9; de maintenir une visibilit&#x00E9; des r&#x00E9;seaux d&#x2019;&#x00E9;nergie et de communication ou de r&#x00E9;tablir un acc&#x00E8;s aux services &#x00E0; la suite d'une coupure&nbsp;;</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;12&#x00B0;&nbsp;Autorisation ou absence d&#x2019;opposition pour travaux en dehors des p&#x00E9;riodes l&#x00E9;gales en cas de catastrophe naturelle ou de force majeure.&nbsp;&#x00BB;&nbsp;</p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p style=\"text-align: justify;\">Cet amendement pr&#x00E9;voit d&#x2019;autoriser la taille des haies, au-del&#x00E0; de la p&#x00E9;riode possible du 16 mars au 15 ao&#x00FB;t, pour celles relevant de la BCAE8 dans deux situations concr&#x00E8;tes : lorsqu'il est n&#x00E9;cessaire de maintenir une visibilit&#x00E9; des r&#x00E9;seaux d&#x2019;&#x00E9;nergie et de communication ou de r&#x00E9;tablir un acc&#x00E8;s aux services &#x00E0; la suite d'une coupure ou lorsqu'une catastrophe naturelle ou un cas de force majeure l'impose.</p><p style=\"text-align: justify;\">Cet amendement a &#x00E9;t&#x00E9; travaill&#x00E9; avec la F&#x00E9;d&#x00E9;ration des Entrepreneurs des Territoires de Loir-et-Cher.&nbsp;</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2024-04-26", "datePublication": "2024-05-02", "soumisArticle40": "false", "etatDesTraitements": {"etat": {"code": "DI", "libelle": "Discut\u00e9"}, "sousEtat": {"code": "DI", "libelle": "Non soutenu"}}, "dateSort": "2024-05-04T12:13:11+02:00", "sort": "Non soutenu"}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutR