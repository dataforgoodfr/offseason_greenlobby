{"amendement": {"@xmlns": "http://schemas.assemblee-nationale.fr/referentiel", "uid": "AMANR5L16PO419865B2436P0D1N000487", "chronotag": "1714402545366", "legislature": "16", "identification": {"numeroLong": "CD487", "numeroOrdreDepot": "487", "prefixeOrganeExamen": "CION-DVP", "numeroRect": "0"}, "examenRef": "EXANR5L16PO419865B2436P0D1", "texteLegislatifRef": "PRJLANR5L16B2436", "triAmendement": "aeaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaabnzzzzzzzy", "cardinaliteAmdtMultiples": "1", "amendementParentRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "signataires": {"auteur": {"typeAuteur": "D\u00e9put\u00e9", "gouvernementRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "acteurRef": "PA795362", "groupePolitiqueRef": "PO800526", "auteurRapporteurOrganeRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "cosignataires": {"acteurRef": ["PA793664", "PA793780", "PA335999", "PA795454", "PA794008", "PA793980", "PA795010", "PA794022", "PA794146", "PA795636", "PA795794", "PA793708", "PA794154", "PA794778", "PA795076", "PA610002", "PA795808", "PA720952", "PA736201", "PA793948"]}, "suffixe": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "libelle": "Mme&#160;Belluco,  Mme&#160;Pochon, Mme&#160;Arrighi, Mme&#160;Batho, M.&#160;Ben&#160;Cheikh, Mme&#160;Chatelain, M.&#160;Fournier, Mme&#160;Garin, M.&#160;Iordanoff, Mme&#160;Laernoes, M.&#160;Lucas-Lundy, Mme&#160;Pasquini, M.&#160;Peytavie, M.&#160;Raux, Mme&#160;Regol, Mme&#160;Rousseau, Mme&#160;Sas, Mme&#160;Sebaihi, M.&#160;Tach&#233;, Mme&#160;Taill&#233;-Polian et M.&#160;Thierry"}, "pointeurFragmentTexte": {"partieAmendableRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "division": {"titre": "Article PREMIER", "articleDesignationCourte": "ART. PREMIER", "articleDesignation": "ARTICLE PREMIER", "type": "ARTICLE", "avant_A_Apres": "A", "divisionRattachee": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "articleAdditionnel": "false", "chapitreAdditionnel": "false", "urlDivisionTexteVise": "/16/textes/2436.asp#D_Article_1er"}, "amendementStandard": {"alinea": {"avant_A_Apres": "Apres", "numero": "9", "alineaDesignation": "Apr\u00e8s l'alin\u00e9a 9"}}}, "corps": {"cartoucheInformatif": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenuAuteur": {"dispositif": "<p style=\"text-align: justify;\">Apr&#x00E8;s l&#x2019;alin&#x00E9;a&nbsp;9, ins&#x00E9;rer l&#x2019;alin&#x00E9;a suivant&nbsp;:</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;&#x2013; pr&#x00E9;server les terres agricoles, en contribuant &#x00E0; atteindre l&#x2019;objectif inscrit &#x00E0; l&#x2019;article&nbsp;191 de la loi n&#x00B0;&nbsp;2021&#x2011;1104 du 22 ao&#x00FB;t 2021 portant lutte contre le d&#x00E9;r&#x00E8;glement climatique et renforcement de la r&#x00E9;silience face &#x00E0; ses effets&nbsp;;&nbsp;&#x00BB;</p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p style=\"text-align: justify;\">L'objectif de cet amendement est de mettre en coh&#x00E9;rence cette d&#x00E9;finition de la souverainet&#x00E9; alimentaire et l'objectif ZAN. La protection des terres agricoles est en effet une condition de possibilit&#x00E9; pour conserver les moyens de cultiver les aliments dont nous avons besoin. Une d&#x00E9;finition de la souverainet&#x00E9; alimentaire ne peut faire l'&#x00E9;conomie d'une r&#x00E9;f&#x00E9;rence &#x00E0; cet objectif, comme indiqu&#x00E9; par les SAFER en audition &#x00E0; l'Assembl&#x00E9;e nationale.</p><p>Tel est l'objet de cet amendement.</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2024-04-25", "datePublication": "2024-04-29", "soumisArticle40": "false", "etatDesTraitements": {"etat": {"code": "DI", "libelle": "Discut\u00e9"}, "sousEtat": {"code": "DI", "libelle": "Rejet\u00e9"}}, "dateSort": "2024-04-29T16:55:33+02:00", "sort": "Rejet\u00e9"}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L16PO419865B2436P0D1N000487?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "seanceDiscussionRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "article99": "false", "loiReference": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionCommune": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionIdentique": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "accordGouvernementDepotHorsDelai": "Sans objet"}}