{"amendement": {"@xmlns": "http://schemas.assemblee-nationale.fr/referentiel", "uid": "AMANR5L16PO419610B2436P0D1N001426", "chronotag": "1714724123383", "legislature": "16", "identification": {"numeroLong": "CE1426", "numeroOrdreDepot": "1426", "prefixeOrganeExamen": "CION-ECO", "numeroRect": "0"}, "examenRef": "EXANR5L16PO419610B2436P0D1", "texteLegislatifRef": "PRJLANR5L16B2436", "triAmendement": "bcaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaatf", "cardinaliteAmdtMultiples": "1", "amendementParentRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "signataires": {"auteur": {"typeAuteur": "D\u00e9put\u00e9", "gouvernementRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "acteurRef": "PA793182", "groupePolitiqueRef": "PO800520", "auteurRapporteurOrganeRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "cosignataires": {"acteurRef": ["PA793238", "PA793158", "PA720668", "PA794222", "PA793964", "PA642988", "PA793146", "PA793246", "PA794418", "PA795540", "PA794354", "PA793904", "PA794590", "PA794530", "PA793314", "PA719436", "PA793852", "PA794894", "PA793298", "PA794946"]}, "suffixe": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "libelle": "M.&#160;Bovet,  M.&#160;Barth&#232;s, M.&#160;Beaurain, M.&#160;Blairy, Mme&#160;Cousin, M.&#160;de&#160;Fournas, M.&#160;de&#160;L&#233;pinau, M.&#160;Dragon, M.&#160;Falcon, Mme&#160;Florence&#160;Goulet, M.&#160;Grenon, Mme&#160;Laporte, M.&#160;Lopez-Liguori, M.&#160;Loubet, M.&#160;Marchio, Mme&#160;Alexandra&#160;Masson, M.&#160;Meizonnet, M.&#160;Meurin, Mme&#160;Sabatini, M.&#160;Tivoli et M.&#160;Villedieu"}, "pointeurFragmentTexte": {"partieAmendableRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "division": {"titre": "Article 3", "articleDesignationCourte": "ART. 3", "articleDesignation": "ARTICLE 3", "type": "ARTICLE", "avant_A_Apres": "A", "divisionRattachee": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "articleAdditionnel": "false", "chapitreAdditionnel": "false", "urlDivisionTexteVise": "/16/textes/2436.asp#D_Article_3"}, "amendementStandard": {"alinea": {"avant_A_Apres": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "numero": "14", "alineaDesignation": "Alin\u00e9a 14"}}}, "corps": {"cartoucheInformatif": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenuAuteur": {"dispositif": "<p style=\"text-align: justify;\">&#x00C0; l&#x2019;alin&#x00E9;a&nbsp;14, substituer aux mots&nbsp;:&nbsp;</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;sont associ&#x00E9;es&nbsp;&#x00BB;, </p><p style=\"text-align: justify;\">les mots&nbsp;:&nbsp;</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;et les d&#x00E9;partements sont associ&#x00E9;s&nbsp;&#x00BB;. </p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p style=\"text-align: justify;\">Cet amendement vise &#x00E0; ce que les d&#x00E9;partements soient associ&#x00E9;s &#x00E0; la mise en oeuvre des missions des<br>&#x00E9;tablissements les enseignements et les formations professionnelles.</p><p>&nbsp;<br>En effet, le d&#x00E9;partement peut participer, comme financer, &#x00E0; divers programmes de formation. Par<br>ailleurs, le d&#x00E9;partement peut attribuer des subventions aux &#x00E9;tablissements scolaires et notamment<br>aux coll&#x00E8;ges.</p><p>&nbsp;<br>On peut imaginer que pour mener &#x00E0; bien les missions d'enseignement, les classes de coll&#x00E8;ge fassent<br>des sorties p&#x00E9;dagogiques qui pourraient &#x00EA;tre financ&#x00E9;es par les d&#x00E9;partements. C'est pour cela que le<br>d&#x00E9;partement doit avoir un droit de regard sur les missions mises en &#x0153;uvre dans le cadre de ce<br>programme.</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2024-04-25", "datePublication": "2024-04-29", "soumisArticle40": "false", "etatDesTraitements": {"etat": {"code": "DI", "libelle": "Discut\u00e9"}, "sousEtat": {"code": "DI", "libelle": "Rejet\u00e9"}}, "dateSort": "2024-05-03T10:15:07+02:00", "sort": "Rejet\u00e9"}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L16PO419610B2436P0D1N001426?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "seanceDiscussionRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "article99": "false", "loiReference": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionCommune": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionIdentique": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "accordGouvernementDepotHorsDelai": "Sans objet"}}