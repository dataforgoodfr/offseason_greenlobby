{"amendement": {"@xmlns": "http://schemas.assemblee-nationale.fr/referentiel", "uid": "AMANR5L16PO419610B2436P0D1N003686", "chronotag": "1714767443432", "legislature": "16", "identification": {"numeroLong": "CE3686", "numeroOrdreDepot": "3686", "prefixeOrganeExamen": "CION-ECO", "numeroRect": "0"}, "examenRef": "EXANR5L16PO419610B2436P0D1", "texteLegislatifRef": "PRJLANR5L16B2436", "triAmendement": "faaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaafir", "cardinaliteAmdtMultiples": "1", "amendementParentRef": "AMANR5L16PO419610B2436P0D1N003573", "signataires": {"auteur": {"typeAuteur": "D\u00e9put\u00e9", "gouvernementRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "acteurRef": "PA607553", "groupePolitiqueRef": "PO830170", "auteurRapporteurOrganeRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "cosignataires": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "suffixe": "et les membres du groupe Socialistes et apparent\u00e9s", "libelle": "M.&#160;Potier et les membres du groupe Socialistes et apparent&#233;s"}, "pointeurFragmentTexte": {"partieAmendableRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "division": {"titre": "Article 10", "articleDesignationCourte": "ART. 10", "articleDesignation": "ARTICLE 10", "type": "ARTICLE", "avant_A_Apres": "A", "divisionRattachee": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "articleAdditionnel": "false", "chapitreAdditionnel": "false", "urlDivisionTexteVise": "/16/textes/2436.asp#D_Article_10"}, "amendementStandard": {"alinea": {"avant_A_Apres": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "numero": "2", "alineaDesignation": "Alin\u00e9a 2"}}}, "corps": {"cartoucheInformatif": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenuAuteur": {"dispositif": "<p style=\"text-align: justify;\">Apr&#x00E8;s le neuvi&#x00E8;me alin&#x00E9;a, ins&#x00E9;rer l&#x2019;alin&#x00E9;a suivant&nbsp;:</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;Le point d&#x2019;accueil d&#x00E9;partemental unique informe&nbsp;les candidats &#x00E0; l&#x2019;installation du sch&#x00E9;ma directeur r&#x00E9;gional des exploitations agricoles et veillent &#x00E0; ce que le projet d&#x2019;installation s&#x2019;inscrive dans les priorit&#x00E9;s dudit sch&#x00E9;ma.&nbsp;&#x00BB;</p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p style=\"text-align: justify;\">Ce sous amendement du groupe Socialistes et apparent&#x00E9;s&nbsp;vise &#x00E0; informer les candidats des crit&#x00E8;res du SDREA et n&#x2019;accompagner que des projets conformes au SDREA.</p><p style=\"text-align: justify;\">L&#x2019;accompagnement des c&#x00E9;dants et des candidats &#x00E0; l&#x2019;installation et &#x00E0; un agrandissement mesur&#x00E9; devra tenir compte des crit&#x00E8;res de priorit&#x00E9;s des SDREA. Les structures agr&#x00E9;&#x00E9;es par l&#x2019;&#x00C9;tat pour accompagner les candidats &#x00E0; l&#x2019;installation doivent informer les candidats des crit&#x00E8;res du SDREA et n&#x2019;accompagner que des projets conformes au SDREA.</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2024-05-03", "datePublication": "2024-05-03", "soumisArticle40": "false", "etatDesTraitements": {"etat": {"code": "DI", "libelle": "Discut\u00e9"}, "sousEtat": {"code": "DI", "libelle": "Rejet\u00e9"}}, "dateSort": "2024-05-03T22:16:58+02:00", "sort": "Rejet\u00e9"}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmln