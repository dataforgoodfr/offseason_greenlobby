{"amendement": {"@xmlns": "http://schemas.assemblee-nationale.fr/referentiel", "uid": "AMANR5L16PO419610B2436P0D1N000915", "chronotag": "1714514453391", "legislature": "16", "identification": {"numeroLong": "CE915", "numeroOrdreDepot": "915", "prefixeOrganeExamen": "CION-ECO", "numeroRect": "0"}, "examenRef": "EXANR5L16PO419610B2436P0D1", "texteLegislatifRef": "PRJLANR5L16B2436", "triAmendement": "aeaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaafzzzzvro", "cardinaliteAmdtMultiples": "1", "amendementParentRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "signataires": {"auteur": {"typeAuteur": "D\u00e9put\u00e9", "gouvernementRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "acteurRef": "PA793476", "groupePolitiqueRef": "PO800508", "auteurRapporteurOrganeRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "cosignataires": {"acteurRef": ["PA719968", "PA266793", "PA342240", "PA793508", "PA718838", "PA330909", "PA608416", "PA267901", "PA719272", "PA795406", "PA793094", "PA794178", "PA721474", "PA722390", "PA608016", "PA721314", "PA643134", "PA607090", "PA346054", "PA608826", "PA721442", "PA712015"]}, "suffixe": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "libelle": "M.&#160;Dubois,  Mme&#160;Bonnet, M.&#160;Bony, M.&#160;Bourgeaux, M.&#160;Brigand, M.&#160;Fabrice&#160;Brun, M.&#160;Descoeur, M.&#160;Hetzel, M.&#160;Kamardine, Mme&#160;Fr&#233;d&#233;rique&#160;Meunier, Mme&#160;P&#233;rigault, M.&#160;Ray, M.&#160;Taite, M.&#160;Viry, Mme&#160;Corneloup, Mme&#160;Louwagie, M.&#160;Schellenberger, Mme&#160;Valentin, M.&#160;Jean-Pierre&#160;Vigier, M.&#160;Emmanuel&#160;Maquet, Mme&#160;Duby-Muller, Mme&#160;Petex et M.&#160;Dive"}, "pointeurFragmentTexte": {"partieAmendableRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "division": {"titre": "Article PREMIER", "articleDesignationCourte": "ART. PREMIER", "articleDesignation": "ARTICLE PREMIER", "type": "ARTICLE", "avant_A_Apres": "A", "divisionRattachee": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "articleAdditionnel": "false", "chapitreAdditionnel": "false", "urlDivisionTexteVise": "/16/textes/2436.asp#D_Article_1er"}, "amendementStandard": {"alinea": {"avant_A_Apres": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "numero": "5", "alineaDesignation": "Alin\u00e9a 5"}}}, "corps": {"cartoucheInformatif": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenuAuteur": {"dispositif": "<p style=\"text-align: justify;\">&#x00C0; l&#x2019;alin&#x00E9;a&nbsp;5, substituer aux mots&nbsp;:</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;dans le cadre&nbsp;du march&#x00E9; int&#x00E9;rieur de l&#x2019;Union europ&#x00E9;enne et de ses engagements internationaux&nbsp;&#x00BB;&nbsp;</p><p style=\"text-align: justify;\">les mots&nbsp;:</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;sans concurrence d&#x00E9;loyale au niveau europ&#x00E9;en et international&nbsp;&#x00BB;.</p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p style=\"text-align: justify;\">Aujourd&#x2019;hui, de tr&#x00E8;s nombreux agriculteurs fran&#x00E7;ais subissent de plein fouet une concurrence d&#x00E9;loyale des pays &#x00E9;trangers. Les normes fran&#x00E7;aises sont tr&#x00E8;s strictes, mais dans les rayons des supermarch&#x00E9;s, les consommateurs peuvent acheter biens moins chers des aliments produits ailleurs qui ne respectent pas ces r&#x00E8;gles.</p><p style=\"text-align: justify;\">Afin de prot&#x00E9;ger notre agriculture, il est imp&#x00E9;ratif d&#x2019;inscrire dans la loi que notre agriculture ne doit pas subir de concurrence d&#x00E9;loyale au niveau europ&#x00E9;en ou international. Le but n&#x2019;est pas de p&#x00E9;naliser les importations de produits agricoles, mais il faut que celles-ci soient soumises aux m&#x00EA;mes normes que les n&#x00F4;tres (clauses miroirs).</p><p style=\"text-align: justify;\">Sans quoi nous ne pourrons pas prot&#x00E9;ger nos agriculteurs et atteindre notre souverainet&#x00E9; alimentaire.</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2024-04-23", "datePublication": "2024-04-30", "soumisArticle40": "false", "etatDesTraitements": {"etat": {"code": "DI", "libelle": "Discut\u00e9"}, "sousEtat": {"code": "DI", "libelle": "Rejet\u00e9"}}, "dateSort": "2024-05-01T00:00:25+02:00", "sort": "Rejet\u00e9"}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L16PO419610B2436P0D1N000915?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "seanceDiscussionRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "article99": "false", "loiReference": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionCommune": {"idDiscussion": "24441", "typePosition": "Milieu"}, "discussionIdentique": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "accordGouvernementDepotHorsDelai": "Sans objet"}}