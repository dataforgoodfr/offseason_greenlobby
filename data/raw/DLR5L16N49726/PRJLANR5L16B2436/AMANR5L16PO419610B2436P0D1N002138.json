{"amendement": {"@xmlns": "http://schemas.assemblee-nationale.fr/referentiel", "uid": "AMANR5L16PO419610B2436P0D1N002138", "chronotag": "1714767658128", "legislature": "16", "identification": {"numeroLong": "CE2138", "numeroOrdreDepot": "2138", "prefixeOrganeExamen": "CION-ECO", "numeroRect": "0"}, "examenRef": "EXANR5L16PO419610B2436P0D1", "texteLegislatifRef": "PRJLANR5L16B2436", "triAmendement": "faaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaafbq", "cardinaliteAmdtMultiples": "1", "amendementParentRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "signataires": {"auteur": {"typeAuteur": "D\u00e9put\u00e9", "gouvernementRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "acteurRef": "PA795270", "groupePolitiqueRef": "PO800514", "auteurRapporteurOrganeRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "cosignataires": {"acteurRef": ["PA719382", "PA267780", "PA720230"]}, "suffixe": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "libelle": "Mme&#160;Poussier-Winsback,  M.&#160;Lamirault, Mme&#160;Firmin&#160;Le&#160;Bodo et Mme&#160;Magnier"}, "pointeurFragmentTexte": {"partieAmendableRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "division": {"titre": "Article 10", "articleDesignationCourte": "ART. 10", "articleDesignation": "ARTICLE 10", "type": "ARTICLE", "avant_A_Apres": "A", "divisionRattachee": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "articleAdditionnel": "false", "chapitreAdditionnel": "false", "urlDivisionTexteVise": "/16/textes/2436.asp#D_Article_10"}, "amendementStandard": {"alinea": {"avant_A_Apres": "Apres", "numero": "11", "alineaDesignation": "Apr\u00e8s l'alin\u00e9a 11"}}}, "corps": {"cartoucheInformatif": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenuAuteur": {"dispositif": "<p style=\"text-align: justify;\">Apre&#x0300;s l&#x2019;aline&#x0301;a 11, inse&#x0301;rer l&#x2019;aline&#x0301;a suivant&nbsp;:</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;Le repr&#x00E9;sentant de l&#x2019;&#x00C9;tat dans le d&#x00E9;partement met en place un comite&#x0301; de suivi de&#x0301;partemental France services agriculture selon des modalite&#x0301;s de&#x0301;finies par de&#x0301;cret, pour veiller &#x00E0; la mise en place du dispositif d&#x2019;accompagnement et de conseil par les chambres d&#x2019;agriculture.&nbsp;&#x00BB;</p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p style=\"text-align: justify;\">Au m&#x00EA;me titre que les comit&#x00E9;s d&#x00E9;partementaux d&#x2019;acc&#x00E8;s aux services publics r&#x00E9;unis par les pr&#x00E9;fets de D&#x00E9;partement, les services d&#x00E9;concentr&#x00E9;s de l&#x2019;&#x00C9;tat sont charg&#x00E9;s du suivi de la mise en place de France Services Agriculture afin de garantir et d&#x2019;&#x00E9;valuer l&#x2019;efficacit&#x00E9; et la repr&#x00E9;sentativit&#x00E9; de ce nouveau dispositif.</p><p style=\"text-align: justify;\">&#x00C0; terme, une fois pass&#x00E9;e la phase d&#x2019;amor&#x00E7;age et de mise en place des France Services Agriculture, ce comit&#x00E9; doit pouvoir se fondre dans les comit&#x00E9;s d&#x00E9;partementaux d&#x2019;acc&#x00E8;s aux services publics.&nbsp;</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2024-04-26", "datePublication": "2024-05-01", "soumisArticle40": "false", "etatDesTraitements": {"etat": {"code": "DI", "libelle": "Discut\u00e9"}, "sousEtat": {"code": "DI", "libelle": "Tomb\u00e9"}}, "dateSort": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "sort": "Tomb\u00e9"}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L16PO419610B2436P0D1N002138?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "seanceDiscussionRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "article99": "false", "loiReference": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionCommune": {"idDiscussion": "24582", "typePosition": "Premier"}, "discussionIdentique": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "accordGouvernementDepotHorsDelai": "Sans objet"}}