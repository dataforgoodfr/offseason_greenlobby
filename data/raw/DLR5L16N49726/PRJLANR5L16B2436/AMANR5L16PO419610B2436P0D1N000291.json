{"amendement": {"@xmlns": "http://schemas.assemblee-nationale.fr/referentiel", "uid": "AMANR5L16PO419610B2436P0D1N000291", "chronotag": "1714776025913", "legislature": "16", "identification": {"numeroLong": "CE291", "numeroOrdreDepot": "291", "prefixeOrganeExamen": "CION-ECO", "numeroRect": "0"}, "examenRef": "EXANR5L16PO419610B2436P0D1", "texteLegislatifRef": "PRJLANR5L16B2436", "triAmendement": "iaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaczzi", "cardinaliteAmdtMultiples": "1", "amendementParentRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "signataires": {"auteur": {"typeAuteur": "D\u00e9put\u00e9", "gouvernementRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "acteurRef": "PA712015", "groupePolitiqueRef": "PO800508", "auteurRapporteurOrganeRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "cosignataires": {"acteurRef": ["PA793476", "PA718838", "PA266793", "PA605991", "PA795406", "PA342240", "PA720586", "PA794038", "PA793094", "PA721442", "PA607090", "PA794178", "PA721474", "PA719318", "PA794410", "PA608416", "PA719272", "PA721314", "PA1327", "PA722390", "PA346054", "PA718850", "PA719968", "PA608826", "PA266808"]}, "suffixe": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "libelle": "M.&#160;Dive,  M.&#160;Dubois, M.&#160;Fabrice&#160;Brun, M.&#160;Bony, Mme&#160;Genevard, Mme&#160;P&#233;rigault, M.&#160;Bourgeaux, M.&#160;Vatin, M.&#160;Neuder, M.&#160;Ray, Mme&#160;Petex, M.&#160;Jean-Pierre&#160;Vigier, M.&#160;Taite, M.&#160;Viry, Mme&#160;Anthoine, M.&#160;Seitlinger, M.&#160;Hetzel, Mme&#160;Fr&#233;d&#233;rique&#160;Meunier, M.&#160;Schellenberger, M.&#160;Forissier, Mme&#160;Corneloup, M.&#160;Emmanuel&#160;Maquet, M.&#160;Cordier, Mme&#160;Bonnet, Mme&#160;Duby-Muller et M.&#160;Rolland"}, "pointeurFragmentTexte": {"partieAmendableRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "division": {"titre": "Article 12", "articleDesignationCourte": "ART. 12", "articleDesignation": "ARTICLE 12", "type": "ARTICLE", "avant_A_Apres": "A", "divisionRattachee": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "articleAdditionnel": "false", "chapitreAdditionnel": "false", "urlDivisionTexteVise": "/16/textes/2436.asp#D_Article_12"}, "amendementStandard": {"alinea": {"avant_A_Apres": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "numero": "7", "alineaDesignation": "Alin\u00e9a 7"}}}, "corps": {"cartoucheInformatif": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenuAuteur": {"dispositif": "<p style=\"text-align: justify;\">R&#x00E9;diger ainsi la troisi&#x00E8;me phrase de l&#x2019;alin&#x00E9;a&nbsp;7&nbsp;:&nbsp;</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;Ces statuts pr&#x00E9;voient un droit de priorit&#x00E9; aux preneurs des baux, qu&#x2019;ils soient associ&#x00E9;s ou non du groupement.&nbsp;&#x00BB;.</p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p style=\"text-align: justify;\">Le preneur en place b&#x00E9;n&#x00E9;ficie d&#x2019;un droit de pr&#x00E9;emption des terres qu&#x2019;il exploite d&#x00E8;s lors que celles-ci sont en vente. Dans le cadre d&#x2019;un GFAI l&#x2019;ali&#x00E9;nation porte davantage sur des parts que sur du foncier. Face &#x00E0; cette situation, le fermier ne dispose d&#x2019;aucun moyen juridique d&#x2019;avoir une priorit&#x00E9; d&#x2019;achat dans l&#x2019;acquisition des parts du GFAI. Cet amendement permet au preneur de b&#x00E9;n&#x00E9;ficier d&#x2019;une priorit&#x00E9; d&#x2019;achat en cas de cession de parts de GFAI dans lequel il loue par bail rural des terres. Ainsi, la priorit&#x00E9; n&#x2019;est pas limit&#x00E9;e aux associ&#x00E9;s participant &#x00E0; l&#x2019;exploitation du foncier. L&#x2019;int&#x00E9;r&#x00EA;t de mettre en place un droit de priorit&#x00E9; pour le fermier en place est de lui offrir un moyen d&#x2019;int&#x00E9;grer le GFAI.</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2024-04-17", "datePublication": "2024-04-30", "soumisArticle40": "false", "etatDesTraitements": {"etat": {"code": "DI", "libelle": "Discut\u00e9"}, "sousEtat": {"code": "DI", "libelle": "Tomb\u00e9"}}, "dateSort": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "sort": "Tomb\u00e9"}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L16PO419610B2436P0D1N000291?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "seanceDiscussionRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "article99": "false", "loiReference": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionCommune": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionIdentique": {"idDiscussion": "69478", "typePosition": "Premier"}, "accordGouvernementDepotHorsDelai": "Sans objet"}}