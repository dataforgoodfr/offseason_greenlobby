{"amendement": {"@xmlns": "http://schemas.assemblee-nationale.fr/referentiel", "uid": "AMANR5L16PO419610B2436P0D1N001576", "chronotag": "1714640693644", "legislature": "16", "identification": {"numeroLong": "CE1576", "numeroOrdreDepot": "1576", "prefixeOrganeExamen": "CION-ECO", "numeroRect": "0"}, "examenRef": "EXANR5L16PO419610B2436P0D1", "texteLegislatifRef": "PRJLANR5L16B2436", "triAmendement": "aeaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaafzzzzxzzzl", "cardinaliteAmdtMultiples": "1", "amendementParentRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "signataires": {"auteur": {"typeAuteur": "D\u00e9put\u00e9", "gouvernementRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "acteurRef": "PA794946", "groupePolitiqueRef": "PO800520", "auteurRapporteurOrganeRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "cosignataires": {"acteurRef": ["PA793238", "PA793158", "PA720668", "PA793182", "PA794222", "PA793964", "PA642988", "PA793146", "PA794718", "PA793246", "PA794418", "PA795540", "PA794354", "PA793904", "PA794590", "PA794530", "PA793314", "PA719436", "PA793852", "PA794894", "PA793298"]}, "suffixe": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "libelle": "M.&#160;Villedieu,  M.&#160;Barth&#232;s, M.&#160;Beaurain, M.&#160;Blairy, M.&#160;Bovet, Mme&#160;Cousin, M.&#160;de&#160;Fournas, M.&#160;de&#160;L&#233;pinau, M.&#160;Dragon, Mme&#160;Engrand, M.&#160;Falcon, Mme&#160;Florence&#160;Goulet, M.&#160;Grenon, Mme&#160;Laporte, M.&#160;Lopez-Liguori, M.&#160;Loubet, M.&#160;Marchio, Mme&#160;Alexandra&#160;Masson, M.&#160;Meizonnet, M.&#160;Meurin, Mme&#160;Sabatini et M.&#160;Tivoli"}, "pointeurFragmentTexte": {"partieAmendableRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "division": {"titre": "Article PREMIER", "articleDesignationCourte": "ART. PREMIER", "articleDesignation": "ARTICLE PREMIER", "type": "ARTICLE", "avant_A_Apres": "A", "divisionRattachee": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "articleAdditionnel": "false", "chapitreAdditionnel": "false", "urlDivisionTexteVise": "/16/textes/2436.asp#D_Article_1er"}, "amendementStandard": {"alinea": {"avant_A_Apres": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "numero": "7", "alineaDesignation": "Alin\u00e9a 7"}}}, "corps": {"cartoucheInformatif": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenuAuteur": {"dispositif": "<p style=\"text-align: justify;\">&#x00C0; l&#x2019;alin&#x00E9;a&nbsp;7, substituer aux mots&nbsp;:</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;la d&#x00E9;carbonation de l&#x2019;&#x00E9;conomie&nbsp;&#x00BB;&nbsp;</p><p style=\"text-align: justify;\">les mots&nbsp;:</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;assurer durablement les besoins alimentaires de la Nation&nbsp;&#x00BB;.</p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p style=\"text-align: justify;\">L&#x2019;amendement propose d&#x2019;&#x00E9;tablir la souverainet&#x00E9; agricole comme un moyen d&#x2019;assurer la souverainet&#x00E9; alimentaire en permettant &#x00E0; nos agriculteurs de produire localement pour assurer les besoins alimentaires de la Nation. Actuellement, ces derniers sont s&#x00E9;v&#x00E8;rement impact&#x00E9;s par l&#x2019;ivresse normative en provenance de l&#x2019;Union europ&#x00E9;enne, notamment en mati&#x00E8;re de transition &#x00E9;nerg&#x00E9;tique. Le secteur agricole, qui est soumis &#x00E0; des contraintes multiples dans ce domaine, ne cesse de d&#x00E9;ployer des efforts pour se plier aux objectifs de r&#x00E9;duction de l&#x2019;empreinte carbone. Afin qu&#x2019;il ne devienne pas une variable d&#x2019;ajustement dans la poursuite de ces objectifs, il est essentiel de pr&#x00E9;ciser qu&#x2019;il doit conserver sa vocation essentielle, celle de produire une alimentation destin&#x00E9;e &#x00E0; assouvir la demande fran&#x00E7;aise.</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2024-04-25", "datePublication": "2024-04-29", "soumisArticle40": "false", "etatDesTraitements": {"etat": {"code": "DI", "libelle": "Discut\u00e9"}, "sousEtat": {"code": "DI", "libelle": "Tomb\u00e9"}}, "dateSort": "2024-05-02T11:04:35+02:00", "sort": "Tomb\u00e9"}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L16PO419610B2436P0D1N001576?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "seanceDiscussionRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "article99": "false", "loiReference": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionCommune": {"idDiscussion": "24446", "typePosition": "Milieu"}, "discussionIdentique": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "accordGouvernementDepotHorsDelai": "Sans objet"}}