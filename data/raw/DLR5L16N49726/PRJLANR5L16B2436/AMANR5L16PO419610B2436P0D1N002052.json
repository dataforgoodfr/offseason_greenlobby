{"amendement": {"@xmlns": "http://schemas.assemblee-nationale.fr/referentiel", "uid": "AMANR5L16PO419610B2436P0D1N002052", "chronotag": "1714671833399", "legislature": "16", "identification": {"numeroLong": "CE2052", "numeroOrdreDepot": "2052", "prefixeOrganeExamen": "CION-ECO", "numeroRect": "0"}, "examenRef": "EXANR5L16PO419610B2436P0D1", "texteLegislatifRef": "PRJLANR5L16B2436", "triAmendement": "asaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaabaal", "cardinaliteAmdtMultiples": "1", "amendementParentRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "signataires": {"auteur": {"typeAuteur": "D\u00e9put\u00e9", "gouvernementRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "acteurRef": "PA793796", "groupePolitiqueRef": "PO800532", "auteurRapporteurOrganeRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "cosignataires": {"acteurRef": ["PA720976", "PA794626", "PA607619", "PA266788"]}, "suffixe": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "libelle": "M.&#160;Taupiac,  M.&#160;Mathiasin, M.&#160;Saint-Huile, M.&#160;Molac et M.&#160;Morel-&#192;-L&apos;Huissier"}, "pointeurFragmentTexte": {"partieAmendableRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "division": {"titre": "Article 2", "articleDesignationCourte": "ART. 2", "articleDesignation": "ARTICLE 2", "type": "ARTICLE", "avant_A_Apres": "A", "divisionRattachee": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "articleAdditionnel": "false", "chapitreAdditionnel": "false", "urlDivisionTexteVise": "/16/textes/2436.asp#D_Article_2"}, "amendementStandard": {"alinea": {"avant_A_Apres": "Apres", "numero": "1", "alineaDesignation": "Apr\u00e8s l'alin\u00e9a 1"}}}, "corps": {"cartoucheInformatif": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenuAuteur": {"dispositif": "<p style=\"text-align: justify;\">Apr&#x00E8;s l&#x2019;alin&#x00E9;a&nbsp;1, ins&#x00E9;rer les alin&#x00E9;as suivants&nbsp;:&nbsp;</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;Elles visent d&#x2019;ici 2030 &#x00E0;&nbsp;:</p><p style=\"text-align: justify;\">1&#x00B0;&nbsp;une augmentation de 30&nbsp;% du nombre d&#x2019;apprenants dans les formations de l&#x2019;enseignement agricole technique qui pr&#x00E9;parent aux m&#x00E9;tiers de l&#x2019;agriculture et de l&#x2019;agroalimentaire par rapport &#x00E0; 2022&nbsp;;</p><p style=\"text-align: justify;\">2&#x00B0;&nbsp;une augmentation de 75&nbsp;% du nombre de v&#x00E9;t&#x00E9;rinaires form&#x00E9;s en France par rapport &#x00E0; 2017&nbsp;;</p><p style=\"text-align: justify;\">3&#x00B0;&nbsp;une augmentation de 30&nbsp;% du nombre d&#x2019;ing&#x00E9;nieurs agronomes form&#x00E9;s par rapport &#x00E0; 2017.&nbsp;&#x00BB;</p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p style=\"text-align: justify;\">Cet amendement vise &#x00E0; fixer des objectifs chiffr&#x00E9;s d'augmentation du nombre de&nbsp;personnes form&#x00E9;es pour les m&#x00E9;tiers de l&#x2019;agriculture, de<br>l&#x2019;agroalimentaire et des autres m&#x00E9;tiers du vivant afin d&#x2019;assurer la souverainet&#x00E9; alimentaire et le renouvellement des g&#x00E9;n&#x00E9;rations en agriculture.&nbsp;</p><p style=\"text-align: justify;\">Elles pr&#x00E9;voient ainsi:</p><p style=\"text-align: justify;\">1&#x00B0; une augmentation de 30 % du nombre d&#x2019;apprenants dans les formations de l&#x2019;enseignement agricole technique qui pr&#x00E9;parent aux m&#x00E9;tiers de l&#x2019;agriculture et de l&#x2019;agroalimentaire par rapport &#x00E0; 2022 ;<br>2&#x00B0; une augmentation de 75 % du nombre de v&#x00E9;t&#x00E9;rinaires form&#x00E9;s en France par rapport &#x00E0; 2017 ;<br>3&#x00B0; une augmentation de 30 % du nombre d&#x2019;ing&#x00E9;nieurs agronomes form&#x00E9;s par rapport &#x00E0; 2017.</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2024-04-26", "datePublication": "2024-04-30", "soumisArticle40": "false", "etatDesTraitements": {"etat": {"code": "DI", "libelle": "Discut\u00e9"}, "sousEtat": {"code": "DI", "libelle": "Adopt\u00e9"}}, "dateSort": "2024-05-02T19:43:42+02:00", "sort": "Adopt\u00e9"}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L16PO419610B2436P0D1N002052?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "seanceDiscussionRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "article99": "false", "loiReference": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionCommune": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionIdentique": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "accordGouvernementDepotHorsDelai": "Sans objet"}}