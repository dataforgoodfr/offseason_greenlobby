{"amendement": {"@xmlns": "http://schemas.assemblee-nationale.fr/referentiel", "uid": "AMANR5L16PO419610B2436P0D1N002056", "chronotag": "1714690613430", "legislature": "16", "identification": {"numeroLong": "CE2056", "numeroOrdreDepot": "2056", "prefixeOrganeExamen": "CION-ECO", "numeroRect": "0"}, "examenRef": "EXANR5L16PO419610B2436P0D1", "texteLegislatifRef": "PRJLANR5L16B2436", "triAmendement": "bcaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaq", "cardinaliteAmdtMultiples": "1", "amendementParentRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "signataires": {"auteur": {"typeAuteur": "D\u00e9put\u00e9", "gouvernementRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "acteurRef": "PA720696", "groupePolitiqueRef": "PO800532", "auteurRapporteurOrganeRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "cosignataires": {"acteurRef": ["PA942", "PA793796", "PA720976", "PA794626", "PA607619", "PA266788"]}, "suffixe": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "libelle": "Mme&#160;Descamps,  M.&#160;de&#160;Courson, M.&#160;Taupiac, M.&#160;Mathiasin, M.&#160;Saint-Huile, M.&#160;Molac et M.&#160;Morel-&#192;-L&apos;Huissier"}, "pointeurFragmentTexte": {"partieAmendableRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "division": {"titre": "Article 3", "articleDesignationCourte": "ART. 3", "articleDesignation": "ARTICLE 3", "type": "ARTICLE", "avant_A_Apres": "A", "divisionRattachee": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "articleAdditionnel": "false", "chapitreAdditionnel": "false", "urlDivisionTexteVise": "/16/textes/2436.asp#D_Article_3"}, "amendementStandard": {"alinea": {"avant_A_Apres": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "numero": "9", "alineaDesignation": "Alin\u00e9a 9"}}}, "corps": {"cartoucheInformatif": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenuAuteur": {"dispositif": "<p style=\"text-align: justify;\">R&#x00E9;diger ainsi l&#x2019;alin&#x00E9;a&nbsp;9&nbsp;:&nbsp;</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;2&#x00B0;&nbsp;Ils contribuent au d&#x00E9;veloppement personnel des &#x00E9;l&#x00E8;ves, &#x00E9;tudiants, apprentis et stagiaires, &#x00E0; l&#x2019;insertion scolaire, sociale et professionnelle des jeunes, &#x00E0; l&#x2019;&#x00E9;l&#x00E9;vation et &#x00E0; l&#x2019;adaptation de leurs qualifications, et &#x00E0; l&#x2019;insertion sociale et professionnelles des adultes, en participant &#x00E0; leur orientation&nbsp;;&nbsp;&#x00BB;</p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p style=\"text-align: justify;\">Le pr&#x00E9;sent amendement vise &#x00E0; compl&#x00E9;ter l'alin&#x00E9;a 9 de l'article 3 en reprenant une partie de l'article 811-1 actuellement en vigueur. Il vise en particulier&nbsp;&#x00E0; maintenir la derni&#x00E8;re phrase du premier alin&#x00E9;a de l'article susmentionn&#x00E9;, en l'adaptant avec la r&#x00E9;daction de l'alin&#x00E9;a 9 de l'article 3 du projet de loi.&nbsp;</p><p>Il semble en effet n&#x00E9;cessaire de maintenir dans la loi que l'enseignement et la formation professionnelle aux m&#x00E9;tiers de l'agriculture favorisent le d&#x00E9;veloppement personnel des &#x00E9;l&#x00E8;ves et assure l'&#x00E9;l&#x00E9;vation et l'adaptation de leurs qualifications.</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2024-04-26", "datePublication": "2024-04-29", "soumisArticle40": "false", "etatDesTraitements": {"etat": {"code": "DI", "libelle": "Discut\u00e9"}, "sousEtat": {"code": "DI", "libelle": "Rejet\u00e9"}}, "dateSort": "2024-05-03T00:56:50+02:00", "sort": "Rejet\u00e9"}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L16PO419610B2436P0D1N002056?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "seanceDiscussionRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "article99": "false", "loiReference": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionCommune": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionIdentique": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "accordGouvernementDepotHorsDelai": "Sans objet"}}