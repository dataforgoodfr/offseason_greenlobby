{"amendement": {"@xmlns": "http://schemas.assemblee-nationale.fr/referentiel", "uid": "AMANR5L16PO419610B2436P0D1N000190", "chronotag": "1714667333436", "legislature": "16", "identification": {"numeroLong": "CE190", "numeroOrdreDepot": "190", "prefixeOrganeExamen": "CION-ECO", "numeroRect": "0"}, "examenRef": "EXANR5L16PO419610B2436P0D1", "texteLegislatifRef": "PRJLANR5L16B2436", "triAmendement": "aeaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaafzzzzznzzzzzzzzzzc", "cardinaliteAmdtMultiples": "1", "amendementParentRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "signataires": {"auteur": {"typeAuteur": "D\u00e9put\u00e9", "gouvernementRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "acteurRef": "PA605991", "groupePolitiqueRef": "PO800508", "auteurRapporteurOrganeRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "cosignataires": {"acteurRef": ["PA793094", "PA721474", "PA712015", "PA793322", "PA608416", "PA608016", "PA718850", "PA719968", "PA642847", "PA793508", "PA794058", "PA720386", "PA719318", "PA719272", "PA607090", "PA267901", "PA794038", "PA608826", "PA722390", "PA721442", "PA793476"]}, "suffixe": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "libelle": "Mme&#160;Genevard,  M.&#160;Ray, M.&#160;Viry, M.&#160;Dive, Mme&#160;D&apos;Intorni, M.&#160;Hetzel, Mme&#160;Louwagie, M.&#160;Cordier, Mme&#160;Bonnet, M.&#160;Bazin, M.&#160;Brigand, Mme&#160;Gruet, M.&#160;Di&#160;Filippo, Mme&#160;Anthoine, Mme&#160;Fr&#233;d&#233;rique&#160;Meunier, M.&#160;Jean-Pierre&#160;Vigier, M.&#160;Kamardine, M.&#160;Neuder, Mme&#160;Duby-Muller, Mme&#160;Corneloup, Mme&#160;Petex et M.&#160;Dubois"}, "pointeurFragmentTexte": {"partieAmendableRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "division": {"titre": "Article PREMIER", "articleDesignationCourte": "ART. PREMIER", "articleDesignation": "ARTICLE PREMIER", "type": "ARTICLE", "avant_A_Apres": "A", "divisionRattachee": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "articleAdditionnel": "false", "chapitreAdditionnel": "false", "urlDivisionTexteVise": "/16/textes/2436.asp#D_Article_1er"}, "amendementStandard": {"alinea": {"avant_A_Apres": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "numero": "21", "alineaDesignation": "Alin\u00e9a 21"}}}, "corps": {"cartoucheInformatif": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenuAuteur": {"dispositif": "<p style=\"text-align: justify;\">Compl&#x00E9;ter l&#x2019;alin&#x00E9;a&nbsp;21 par les mots&nbsp;:&nbsp;</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;et au maintien d'un r&#x00E9;seau de services dans le monde rural favorisant l&#x2019;installation de jeunes.&nbsp;&#x00BB;</p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p style=\"text-align: justify;\">Le d&#x00E9;veloppement d&#x2019;un r&#x00E9;seau de services complets et de qualit&#x00E9; en milieu rural est essentiel pour permettre l&#x2019;installation de jeunes agriculteurs. Ces services, en r&#x00E9;pondant aux besoins et aux aspirations des futurs agriculteurs, contribuent &#x00E0; faire des territoires ruraux des lieux de vie d&#x00E9;sirables, offrant des services dans le domaine de la sant&#x00E9;, du social, de l&#x2019;enseignement, de l&#x2019;encadrement, de l&#x2019;animation&#x2026; Dans l&#x2019;enseignement agricole priv&#x00E9;, les fili&#x00E8;res services et animation des territoires ruraux repr&#x00E9;sentent plus de 30&nbsp;% des effectifs d&#x2019;apprenants. Ces fili&#x00E8;res forment &#x00E0; des m&#x00E9;tiers vari&#x00E9;s tels que charg&#x00E9; de d&#x00E9;veloppement local, charg&#x00E9; de promotion du patrimoine, conseiller en d&#x00E9;veloppement territorial, animateur de territoire rural au sein de collectivit&#x00E9;s locales, d&#x2019;associations de d&#x00E9;veloppement ou bien d&#x2019;entreprises.</p><p style=\"text-align: justify;\">Contribuer au renouvellement des g&#x00E9;n&#x00E9;rations d&#x2019;agriculteurs implique aussi de soutenir le d&#x00E9;veloppement des services de proximit&#x00E9; pour les actifs agricoles dans lesquels les conjoints d&#x2019;agriculteurs sont tr&#x00E8;s souvent investis. Cet amendement vise &#x00E0; ainsi &#x00E0; mettre en lumi&#x00E8;re l&#x2019;importance de cette fili&#x00E8;re de formation qui joue un r&#x00F4;le cl&#x00E9; dans la vitalit&#x00E9; des zones rurales et appelle donc le soutien de l&#x2019;&#x00C9;tat &#x00E0; celle-ci.</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2024-04-17", "datePublication": "2024-04-30", "soumisArticle40": "false", "etatDesTraitements": {"etat": {"code": "DI", "libelle": "Discut\u00e9"}, "sousEtat": {"code": "DI", "libelle": "Adopt\u00e9"}}, "dateSort": "2024-05-02T18:28:40+02:00", "sort": "Adopt\u00e9"}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L16PO419610B2436P0D1N000190?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "seanceDiscussionRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "article99": "false", "loiReference": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionCommune": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionIdentique": {"idDiscussion": "69211", "typePosition": "Milieu"}, "accordGouvernementDepotHorsDelai": "Sans objet"}}