@xsi:nil": "true"}, "division": {"titre": "Article 12", "articleDesignationCourte": "ART. 12", "articleDesignation": "ARTICLE 12", "type": "ARTICLE", "avant_A_Apres": "A", "divisionRattachee": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "articleAdditionnel": "false", "chapitreAdditionnel": "false", "urlDivisionTexteVise": "/16/textes/2436.asp#D_Article_12"}, "amendementStandard": {"alinea": {"avant_A_Apres": "Apres", "numero": "15", "alineaDesignation": "Apr\u00e8s l'alin\u00e9a 15"}}}, "corps": {"cartoucheInformatif": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenuAuteur": {"dispositif": "<p style=\"text-align: justify;\">Apr&#x00E8;s l&#x2019;alin&#x00E9;a&nbsp;15, ins&#x00E9;rer les deux alin&#x00E9;as suivants&nbsp;:&nbsp;</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;II <i>bis</i>.&nbsp;&#x2013;&nbsp;Apr&#x00E8;s le huiti&#x00E8;me alin&#x00E9;a de l&#x2019;article L.&nbsp;143&#x2011;1 du code rural et de la p&#x00EA;che maritime, il est ins&#x00E9;r&#x00E9; un alin&#x00E9;a ainsi r&#x00E9;dig&#x00E9;&nbsp;:</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;Les soci&#x00E9;t&#x00E9;s d&#x2019;am&#x00E9;nagement foncier et d&#x2019;&#x00E9;tablissement rural peuvent exercer leur droit de pr&#x00E9;emption en cas de cession partielle des parts ou actions d&#x2019;un groupement foncier agricole d&#x2019;investissement.&nbsp;&#x00BB;</p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p style=\"text-align: justify;\">Le pr&#x00E9;sent amendement pr&#x00E9;voit d&#x2019;ouvrir la possibilit&#x00E9; pour les Safer de pr&#x00E9;empter des cessions partielles de parts de groupements fonciers agricoles d&#x2019;investissement. Cette nouvelle voie de pr&#x00E9;emption constituerait le corollaire d'un dispositif visant &#x00E0; pr&#x00E9;server le foncier agricole et &#x00E0; contribuer au maintien de la surface agricole utile, difficilement tenu depuis 20 ans. Cet amendement permet &#x00E0; la SAFER de pr&#x00E9;empter sur une cession partielle des parts de GFAI.</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2024-04-17", "datePublication": "2024-05-02", "soumisArticle40": "false", "etatDesTraitements": {"etat": {"code": "DI", "libelle": "Discut\u00e9"}, "sousEtat": {"code": "DI", "libelle": "Tomb\u00e9"}}, "dateSort": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "sort": "Tomb\u00e9"}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L16PO419610B2436P0D1N000289?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "seanceDiscussionRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "article99": "false", "loiReference": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionCommune": {"idDiscussion": "24573", "typePosition": "Premier"}, "discussionIdentique": {"idDiscussion": "69244", "typePosition": "Premier"}, "accordGouvernementDepotHorsDelai": "Sans objet"}}