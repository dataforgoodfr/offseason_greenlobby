{"amendement": {"@xmlns": "http://schemas.assemblee-nationale.fr/referentiel", "uid": "AMANR5L16PO419610B2436P0D1N001937", "chronotag": "1714733273450", "legislature": "16", "identification": {"numeroLong": "CE1937", "numeroOrdreDepot": "1937", "prefixeOrganeExamen": "CION-ECO", "numeroRect": "0"}, "examenRef": "EXANR5L16PO419610B2436P0D1", "texteLegislatifRef": "PRJLANR5L16B2436", "triAmendement": "coaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaabu", "cardinaliteAmdtMultiples": "1", "amendementParentRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "signataires": {"auteur": {"typeAuteur": "D\u00e9put\u00e9", "gouvernementRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "acteurRef": "PA266788", "groupePolitiqueRef": "PO800532", "auteurRapporteurOrganeRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "cosignataires": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "suffixe": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "libelle": "M.&#160;Morel-&#192;-L&apos;Huissier"}, "pointeurFragmentTexte": {"partieAmendableRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "division": {"titre": "Article 7", "articleDesignationCourte": "ART. 7", "articleDesignation": "ARTICLE 7", "type": "ARTICLE", "avant_A_Apres": "A", "divisionRattachee": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "articleAdditionnel": "false", "chapitreAdditionnel": "false", "urlDivisionTexteVise": "/16/textes/2436.asp#D_Article_7"}, "amendementStandard": {"alinea": {"avant_A_Apres": "A", "numero": "5", "alineaDesignation": "Alin\u00e9a 5"}}}, "corps": {"cartoucheInformatif": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenuAuteur": {"dispositif": "<p style=\"text-align: justify;\">Substituer aux&nbsp;alin&#x00E9;as 5&nbsp;et&nbsp;6 les deux alin&#x00E9;as ainsi r&#x00E9;dig&#x00E9;s&nbsp;:</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;Les &#x00E9;leveurs sont habilit&#x00E9;s &#x00E0; r&#x00E9;aliser certains actes v&#x00E9;t&#x00E9;rinaires d&#x2019;urgence, attestant d&#x2019;une formation pr&#x00E9;alable, dans l&#x2019;attente de l&#x2019;intervention d&#x2019;une personne habilit&#x00E9;e sur les exploitations agricoles ou dans une structure v&#x00E9;t&#x00E9;rinaire au titre de la liste fix&#x00E9;e au pr&#x00E9;sent article.</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;Les actes v&#x00E9;t&#x00E9;rinaires concern&#x00E9;s sont fix&#x00E9;s par d&#x00E9;cret&nbsp;&#x00BB;.</p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p style=\"text-align: justify;\">Il est propos&#x00E9; de permettre aux &#x00AB;&nbsp;&#x00E9;leveurs soigneurs&nbsp;&#x00BB; de r&#x00E9;aliser certains actes v&#x00E9;t&#x00E9;rinaires d&#x2019;urgence, list&#x00E9;s par d&#x00E9;cret, et apr&#x00E8;s formation. Actuellement, les &#x00E9;leveurs doivent attendre l&#x2019;intervention d&#x2019;une personne habilit&#x00E9;e ce qui engage non seulement des frais suppl&#x00E9;mentaires, mais &#x00E9;galement du temps pr&#x00E9;cieux &#x00E0; faire soigner leurs b&#x00E9;tails.&nbsp;</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2024-04-26", "datePublication": "2024-04-29", "soumisArticle40": "false", "etatDesTraitements": {"etat": {"code": "DI", "libelle": "Discut\u00e9"}, "sousEtat": {"code": "DI", "libelle": "Non soutenu"}}, "dateSort": "2024-05-03T12:47:38+02:00", "sort": "Non soutenu"}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L16PO419610B2436P0D1N001937?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "seanceDiscussionRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "article99": "false", "loiReference": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionCommune": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionIdentique": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "accordGouvernementDepotHorsDelai": "Sans objet"}}