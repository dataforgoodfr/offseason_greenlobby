{"amendement": {"@xmlns": "http://schemas.assemblee-nationale.fr/referentiel", "uid": "AMANR5L16PO419610B2436P0D1N000377", "chronotag": "1714665803558", "legislature": "16", "identification": {"numeroLong": "CE377", "numeroOrdreDepot": "377", "prefixeOrganeExamen": "CION-ECO", "numeroRect": "0"}, "examenRef": "EXANR5L16PO419610B2436P0D1", "texteLegislatifRef": "PRJLANR5L16B2436", "triAmendement": "aeaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaafzzzzznzzzzzw", "cardinaliteAmdtMultiples": "1", "amendementParentRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "signataires": {"auteur": {"typeAuteur": "D\u00e9put\u00e9", "gouvernementRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "acteurRef": "PA605991", "groupePolitiqueRef": "PO800508", "auteurRapporteurOrganeRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "cosignataires": {"acteurRef": ["PA330909", "PA795406", "PA266793", "PA342240", "PA721816", "PA721474", "PA608416", "PA608016", "PA718850", "PA719968", "PA642847", "PA793508", "PA794058", "PA643134", "PA607090", "PA267901", "PA794038", "PA608826", "PA722390", "PA793476"]}, "suffixe": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "libelle": "Mme&#160;Genevard,  M.&#160;Descoeur, Mme&#160;P&#233;rigault, M.&#160;Bony, M.&#160;Bourgeaux, M.&#160;Boucard, M.&#160;Viry, M.&#160;Hetzel, Mme&#160;Louwagie, M.&#160;Cordier, Mme&#160;Bonnet, M.&#160;Bazin, M.&#160;Brigand, Mme&#160;Gruet, Mme&#160;Valentin, M.&#160;Jean-Pierre&#160;Vigier, M.&#160;Kamardine, M.&#160;Neuder, Mme&#160;Duby-Muller, Mme&#160;Corneloup et M.&#160;Dubois"}, "pointeurFragmentTexte": {"partieAmendableRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "division": {"titre": "Article PREMIER", "articleDesignationCourte": "ART. PREMIER", "articleDesignation": "ARTICLE PREMIER", "type": "ARTICLE", "avant_A_Apres": "A", "divisionRattachee": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "articleAdditionnel": "false", "chapitreAdditionnel": "false", "urlDivisionTexteVise": "/16/textes/2436.asp#D_Article_1er"}, "amendementStandard": {"alinea": {"avant_A_Apres": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "numero": "15", "alineaDesignation": "Alin\u00e9a 15"}}}, "corps": {"cartoucheInformatif": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenuAuteur": {"dispositif": "<p style=\"text-align: justify;\">&#x00C0; l&#x2019;alin&#x00E9;a 15, apr&#x00E8;s les mots :&nbsp;</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;d&#x2019;exploitant agricole&nbsp;&#x00BB;,</p><p>ins&#x00E9;rer les mots&nbsp;:&nbsp;</p><p>&#x00AB;&nbsp;et de salari&#x00E9; agricole&nbsp;&#x00BB;.</p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p style=\"text-align: justify;\">L&#x2019;emploi agricole joue un r&#x00F4;le majeur dans les exploitations agricoles, il importe ainsi de faire conna&#x00EE;tre &#x00E9;galement le m&#x00E9;tier de salari&#x00E9; agricole en vue d&#x2019;assurer le renouvellement des g&#x00E9;n&#x00E9;rations. D&#x2019;apr&#x00E8;s le panorama d&#x00E9;mographique et financier du r&#x00E9;gime agricole de la MSA en 2022, 64,4% des actifs relevant du r&#x00E9;gime agricole sont affili&#x00E9;s au r&#x00E9;gime salari&#x00E9;, soit plus de 800 000 emplois, en hausse de 3,7% sur un an. Ces statistiques sont r&#x00E9;v&#x00E9;latrices de l&#x2019;importance du m&#x00E9;tier de salari&#x00E9; agricole, qui contribue &#x00E9;galement au renouvellement des g&#x00E9;n&#x00E9;rations.</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2024-04-19", "datePublication": "2024-04-29", "soumisArticle40": "false", "etatDesTraitements": {"etat": {"code": "DI", "libelle": "Discut\u00e9"}, "sousEtat": {"code": "DI", "libelle": "Adopt\u00e9"}}, "dateSort": "2024-05-02T18:03:00+02:00", "sort": "Adopt\u00e9"}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L16PO419610B2436P0D1N000377?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "seanceDiscussionRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "article99": "false", "loiReference": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionCommune": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionIdentique": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "accordGouvernementDepotHorsDelai": "Sans objet"}}