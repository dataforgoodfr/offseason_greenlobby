": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "division": {"titre": "Article 14", "articleDesignationCourte": "ART. 14", "articleDesignation": "ARTICLE 14", "type": "ARTICLE", "avant_A_Apres": "A", "divisionRattachee": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "articleAdditionnel": "false", "chapitreAdditionnel": "false", "urlDivisionTexteVise": "/16/textes/2436.asp#D_Article_14"}, "amendementStandard": {"alinea": {"avant_A_Apres": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "numero": "38", "alineaDesignation": "Alin\u00e9a 38"}}}, "corps": {"cartoucheInformatif": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenuAuteur": {"dispositif": "<p style=\"text-align: justify;\">Supprimer l'alin&#x00E9;a 38.&nbsp;</p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p style=\"text-align: justify;\">Cet amendement du groupe LR propose de supprimer la sanction pr&#x00E9;vue pour le d&#x00E9;faut d&#x2019;autorisation ou de d&#x00E9;claration pour une destruction de haie.</p><p>En effet, Le Conseil d&#x2019;&#x00C9;tat a estim&#x00E9; que cette sanction, selon l'article L.173-1 du code de l'environnement, &#x00E9;tait trop s&#x00E9;v&#x00E8;re, compte tenu que les r&#x00E8;gles sur les haies ne rel&#x00E8;vent ni des Installations Class&#x00E9;es pour la Protection de l'Environnement (ICPE) ni des Ouvrages, Travaux et Activit&#x00E9;s (IOTA).</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2024-04-26", "datePublication": "2024-05-01", "soumisArticle40": "false", "etatDesTraitements": {"etat": {"code": "DI", "libelle": "Discut\u00e9"}, "sousEtat": {"code": "DI", "libelle": "Rejet\u00e9"}}, "dateSort": "2024-05-04T12:43:12+02:00", "sort": "Rejet\u00e9"}, "representation