{"amendement": {"@xmlns": "http://schemas.assemblee-nationale.fr/referentiel", "uid": "AMANR5L16PO419865B2436P0D1N000178", "chronotag": "1714409325419", "legislature": "16", "identification": {"numeroLong": "CD178", "numeroOrdreDepot": "178", "prefixeOrganeExamen": "CION-DVP", "numeroRect": "0"}, "examenRef": "EXANR5L16PO419865B2436P0D1", "texteLegislatifRef": "PRJLANR5L16B2436", "triAmendement": "aeaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaatcno", "cardinaliteAmdtMultiples": "1", "amendementParentRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "signataires": {"auteur": {"typeAuteur": "D\u00e9put\u00e9", "gouvernementRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "acteurRef": "PA330909", "groupePolitiqueRef": "PO800508", "auteurRapporteurOrganeRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "cosignataires": {"acteurRef": ["PA721410", "PA718838", "PA266793", "PA342240", "PA267901", "PA793508", "PA643134", "PA721442", "PA721474", "PA720586", "PA795406", "PA794178", "PA346054", "PA719272", "PA793094", "PA712015", "PA608416", "PA722390", "PA793476", "PA608826"]}, "suffixe": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "libelle": "M.&#160;Descoeur,  Mme&#160;Bonnivard, M.&#160;Fabrice&#160;Brun, M.&#160;Bony, M.&#160;Bourgeaux, M.&#160;Kamardine, M.&#160;Brigand, Mme&#160;Valentin, Mme&#160;Petex, M.&#160;Viry, M.&#160;Vatin, Mme&#160;P&#233;rigault, M.&#160;Taite, M.&#160;Emmanuel&#160;Maquet, Mme&#160;Fr&#233;d&#233;rique&#160;Meunier, M.&#160;Ray, M.&#160;Dive, M.&#160;Hetzel, Mme&#160;Corneloup, M.&#160;Dubois et Mme&#160;Duby-Muller"}, "pointeurFragmentTexte": {"partieAmendableRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "division": {"titre": "Article PREMIER", "articleDesignationCourte": "ART. PREMIER", "articleDesignation": "ARTICLE PREMIER", "type": "ARTICLE", "avant_A_Apres": "A", "divisionRattachee": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "articleAdditionnel": "false", "chapitreAdditionnel": "false", "urlDivisionTexteVise": "/16/textes/2436.asp#D_Article_1er"}, "amendementStandard": {"alinea": {"avant_A_Apres": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "numero": "21", "alineaDesignation": "Alin\u00e9a 21"}}}, "corps": {"cartoucheInformatif": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenuAuteur": {"dispositif": "<p style=\"text-align: justify;\">&#x00C0; la seconde phrase de l&#x2019;alin&#x00E9;a&nbsp;21, apr&#x00E8;s le mot&nbsp;:</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;agriculture,&nbsp;&#x00BB;,&nbsp;</p><p style=\"text-align: justify;\">ins&#x00E9;rer les mots&nbsp;:</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;de l&#x2019;&#x00E9;levage,&nbsp;&#x00BB;.</p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p style=\"text-align: justify;\">Il convient &#x00E0; travers cet amendement d'&#x00E9;voquer l'&#x00E9;levage, le grand oubli&#x00E9; du projet de loi. Rien n'est pr&#x00E9;vu dans ce texte pour encourager l'investissement et la modernisation des &#x00E9;levages. Alors qu'aujourd'hui, on assiste &#x00E0; un basculement g&#x00E9;n&#x00E9;rationnel tr&#x00E8;s important, qui va entra&#x00EE;ner une baisse de la production, et que le revenu des &#x00E9;leveurs s&#x2019;effrite, rien n'est propos&#x00E9; pour soutenir le secteur de l&#x2019;&#x00E9;levage. Alors que le Ministre nous avait assur&#x00E9; le 7 juin dernier que l&#x2019;&#x00E9;levage figurerait bien parmi les sujets importants du Pacte et de la Loi d&#x2019;orientation et d&#x2019;avenir agricoles (PLOA), aucune mesure sp&#x00E9;cifique n'est propos&#x00E9;e pour cette fili&#x00E8;re, pourtant essentielle pour l&#x2019;am&#x00E9;nagement et l&#x2019;occupation de notre territoire, l&#x2019;&#x00E9;conomie agricole et la souverainet&#x00E9; alimentaire de notre pays.</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2024-04-24", "datePublication": "2024-04-29", "soumisArticle40": "false", "etatDesTraitements": {"etat": {"code": "DI", "libelle": "Discut\u00e9"}, "sousEtat": {"code": "DI", "libelle": "Rejet\u00e9"}}, "dateSort": "2024-04-29T18:48:14+02:00", "sort": "Rejet\u00e9"}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L16PO419865B2436P0D1N000178?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "seanceDiscussionRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "article99": "false", "loiReference": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionCommune": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionIdentique": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "accordGouvernementDepotHorsDelai": "Sans objet"}}