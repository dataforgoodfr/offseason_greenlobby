{"amendement": {"@xmlns": "http://schemas.assemblee-nationale.fr/referentiel", "uid": "AMANR5L16PO419610B2436P0D1N003539", "chronotag": "1714672253374", "legislature": "16", "identification": {"numeroLong": "CE3539", "numeroOrdreDepot": "3539", "prefixeOrganeExamen": "CION-ECO", "numeroRect": "0"}, "examenRef": "EXANR5L16PO419610B2436P0D1", "texteLegislatifRef": "PRJLANR5L16B2436", "triAmendement": "asaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaababi", "cardinaliteAmdtMultiples": "1", "amendementParentRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "signataires": {"auteur": {"typeAuteur": "Rapporteur", "gouvernementRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "acteurRef": "PA720256", "groupePolitiqueRef": "PO800484", "auteurRapporteurOrganeRef": "PO419604"}, "cosignataires": {"acteurRef": ["PA720190", "PA605991", "PA330909", "PA721474", "PA607090", "PA608416", "PA608016", "PA795406", "PA718850", "PA719968", "PA793508", "PA267901", "PA794058", "PA642847", "PA720386", "PA794038", "PA643134", "PA266793", "PA342240", "PA608826", "PA721816", "PA793476"]}, "suffixe": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "libelle": "Mme&#160;Bannier, rapporteure pour avis au nom de la commission des affaires culturelles et de l&apos;&#233;ducation,  M.&#160;Sorre, Mme&#160;Genevard, M.&#160;Descoeur, M.&#160;Viry, M.&#160;Jean-Pierre&#160;Vigier, M.&#160;Hetzel, Mme&#160;Louwagie, Mme&#160;P&#233;rigault, M.&#160;Cordier, Mme&#160;Bonnet, M.&#160;Brigand, M.&#160;Kamardine, Mme&#160;Gruet, M.&#160;Bazin, M.&#160;Di&#160;Filippo, M.&#160;Neuder, Mme&#160;Valentin, M.&#160;Bony, M.&#160;Bourgeaux, Mme&#160;Duby-Muller, M.&#160;Boucard et M.&#160;Dubois"}, "pointeurFragmentTexte": {"partieAmendableRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "division": {"titre": "Article 2", "articleDesignationCourte": "ART. 2", "articleDesignation": "ARTICLE 2", "type": "ARTICLE", "avant_A_Apres": "A", "divisionRattachee": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "articleAdditionnel": "false", "chapitreAdditionnel": "false", "urlDivisionTexteVise": "/16/textes/2436.asp#D_Article_2"}, "amendementStandard": {"alinea": {"avant_A_Apres": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "numero": "2", "alineaDesignation": "Alin\u00e9a 2"}}}, "corps": {"cartoucheInformatif": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenuAuteur": {"dispositif": "<p style=\"text-align: justify;\">I.&nbsp;&#x2013;&nbsp;&#x00C0; l&#x2019;alin&#x00E9;a&nbsp;2, apr&#x00E8;s le mot&nbsp;:&nbsp;</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;int&#x00E9;ress&#x00E9;es&nbsp;&#x00BB;,</p><p style=\"text-align: justify;\">ins&#x00E9;rer les mots&nbsp;:&nbsp;</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;, en concertation avec les branches professionnelles concern&#x00E9;es&nbsp;&#x00BB;.</p><p style=\"text-align: justify;\">II.&nbsp;&#x2013;&nbsp;En cons&#x00E9;quence, &#x00E0; la premi&#x00E8;re phrase de l&#x2019;alin&#x00E9;a&nbsp;7, apr&#x00E8;s le mot&nbsp;:&nbsp;</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;r&#x00E9;gions&nbsp;&#x00BB;</p><p style=\"text-align: justify;\">ins&#x00E9;rer les mots&nbsp;:&nbsp;</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;et les branches professionnelles concern&#x00E9;es&nbsp;&#x00BB;.</p><p style=\"text-align: justify;\">III.&nbsp;&#x2013;&nbsp;En cons&#x00E9;quence, &#x00E0; la premi&#x00E8;re phrase de l&#x2019;alin&#x00E9;a&nbsp;8, proc&#x00E9;der &#x00E0; la m&#x00EA;me insertion.</p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p style=\"text-align: justify;\">Cet amendement vise &#x00E0; rendre obligatoire l&#x2019;implication des branches professionnelles concern&#x00E9;es dans l&#x2019;&#x00E9;tablissement du programme national d&#x2019;orientation et de d&#x00E9;couverte des m&#x00E9;tiers de l&#x2019;agriculture et de l&#x2019;alimentation et dans la mise en &#x0153;uvre d&#x2019;un programme national triennal de formation aux comp&#x00E9;tences en mati&#x00E8;re de transitions agro&#x00E9;cologique et climatique. L&#x2019;association des organismes professionnels ne peut se limiter aux concertations r&#x00E9;gionales pr&#x00E9;c&#x00E9;dant la pr&#x00E9;sentation du projet de loi d&#x2019;orientation agricole mais doit &#x00EA;tre une partie prenante structurelle des politiques publiques de formation, de recherche et d&#x2019;innovation.</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2024-04-30", "datePublication": "2024-04-30", "soumisArticle40": "false", "etatDesTraitements": {"etat": {"code": "DI", "libelle": "Discut\u00e9"}, "sousEtat": {"code": "DI", "libelle": "Retir\u00e9"}}, "dateSort": "2024-05-02T19:50:47+02:00", "sort": "Retir\u00e9"}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L16PO419610B2436P0D1N003539?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "seanceDiscussionRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "article99": "false", "loiReference": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionCommune": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionIdentique": {"idDiscussion": "69739", "typePosition": "Premier"}, "accordGouvernementDepotHorsDelai": "Sans objet"}}