{"amendement": {"@xmlns": "http://schemas.assemblee-nationale.fr/referentiel", "uid": "AMANR5L16PO419610B2436P0D1N001022", "chronotag": "1714681343477", "legislature": "16", "identification": {"numeroLong": "CE1022", "numeroOrdreDepot": "1022", "prefixeOrganeExamen": "CION-ECO", "numeroRect": "0"}, "examenRef": "EXANR5L16PO419610B2436P0D1", "texteLegislatifRef": "PRJLANR5L16B2436", "triAmendement": "asaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaabgo", "cardinaliteAmdtMultiples": "1", "amendementParentRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "signataires": {"auteur": {"typeAuteur": "D\u00e9put\u00e9", "gouvernementRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "acteurRef": "PA712015", "groupePolitiqueRef": "PO800508", "auteurRapporteurOrganeRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "cosignataires": {"acteurRef": ["PA719318", "PA719968", "PA718884", "PA266793", "PA342240", "PA793508", "PA718838", "PA793476", "PA722390", "PA718850", "PA330909", "PA608826", "PA1327", "PA605991", "PA608416", "PA719272", "PA794038", "PA721442", "PA795406", "PA793094", "PA266808", "PA721314", "PA794410", "PA794178", "PA720586", "PA607090", "PA721474"]}, "suffixe": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "libelle": "M.&#160;Dive,  Mme&#160;Anthoine, Mme&#160;Bonnet, Mme&#160;Bazin-Malgras, M.&#160;Bony, M.&#160;Bourgeaux, M.&#160;Brigand, M.&#160;Fabrice&#160;Brun, M.&#160;Dubois, Mme&#160;Corneloup, M.&#160;Cordier, M.&#160;Descoeur, Mme&#160;Duby-Muller, M.&#160;Forissier, Mme&#160;Genevard, M.&#160;Hetzel, Mme&#160;Fr&#233;d&#233;rique&#160;Meunier, M.&#160;Neuder, Mme&#160;Petex, Mme&#160;P&#233;rigault, M.&#160;Ray, M.&#160;Rolland, M.&#160;Schellenberger, M.&#160;Seitlinger, M.&#160;Taite, M.&#160;Vatin, M.&#160;Jean-Pierre&#160;Vigier et M.&#160;Viry"}, "pointeurFragmentTexte": {"partieAmendableRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "division": {"titre": "Article 2", "articleDesignationCourte": "ART. 2", "articleDesignation": "ARTICLE 2", "type": "ARTICLE", "avant_A_Apres": "A", "divisionRattachee": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "articleAdditionnel": "false", "chapitreAdditionnel": "false", "urlDivisionTexteVise": "/16/textes/2436.asp#D_Article_2"}, "amendementStandard": {"alinea": {"avant_A_Apres": "Apres", "numero": "6", "alineaDesignation": "Apr\u00e8s l'alin\u00e9a 6"}}}, "corps": {"cartoucheInformatif": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenuAuteur": {"dispositif": "<p style=\"text-align: justify;\">Apr&#x00E8;s l&#x2019;alin&#x00E9;a&nbsp;6, ins&#x00E9;rer l&#x2019;alin&#x00E9;a suivant&nbsp;:&nbsp;</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;5&#x00B0;&nbsp;D&#x2019;informer et de diffuser l&#x2019;avancement des nouvelles techniques de s&#x00E9;lection et d&#x2019;am&#x00E9;lioration des cultures, &#x00E0; destination des actifs des secteurs de l&#x2019;agriculture et de l&#x2019;agroalimentaire et des citoyens.&nbsp;&#x00BB;</p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p style=\"text-align: justify;\">La recherche et l'innovation jouent un r&#x00F4;le central dans l'am&#x00E9;lioration des pratiques agricoles et alimentaires, notamment &#x00E0; travers l'introduction de nouvelles techniques de s&#x00E9;lection et d'am&#x00E9;lioration des cultures. Ces avanc&#x00E9;es, souvent regroup&#x00E9;es repr&#x00E9;sentent un potentiel consid&#x00E9;rable pour accro&#x00EE;tre la r&#x00E9;silience des cultures face aux d&#x00E9;fis climatiques et r&#x00E9;duire l'utilisation des intrants chimiques. Cependant, l'acceptabilit&#x00E9; sociale et la compr&#x00E9;hension publique de ces techniques sont des &#x00E9;l&#x00E9;ments essentiels pour leur adoption r&#x00E9;ussie. C'est pourquoi cet amendement propose d'informer et de sensibiliser tant les acteurs des secteurs agricoles et agroalimentaires que le grand public sur l'&#x00E9;tat de l'art des nouvelles techniques de s&#x00E9;lection et d'am&#x00E9;lioration des cultures. Cette d&#x00E9;marche vise &#x00E0; favoriser un dialogue ouvert et inform&#x00E9;.&nbsp;</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2024-04-24", "datePublication": "2024-04-29", "soumisArticle40": "false", "etatDesTraitements": {"etat": {"code": "DI", "libelle": "Discut\u00e9"}, "sousEtat": {"code": "DI", "libelle": "Retir\u00e9"}}, "dateSort": "2024-05-02T22:22:14+02:00", "sort": "Retir\u00e9"}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L16PO419610B2436P0D1N001022?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "seanceDiscussionRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "article99": "false", "loiReference": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionCommune": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionIdentique": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "accordGouvernementDepotHorsDelai": "Sans objet"}}