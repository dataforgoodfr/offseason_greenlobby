{"amendement": {"@xmlns": "http://schemas.assemblee-nationale.fr/referentiel", "uid": "AMANR5L16PO419610B2436P0D1N000961", "chronotag": "1714688093381", "legislature": "16", "identification": {"numeroLong": "CE961", "numeroOrdreDepot": "961", "prefixeOrganeExamen": "CION-ECO", "numeroRect": "0"}, "examenRef": "EXANR5L16PO419610B2436P0D1", "texteLegislatifRef": "PRJLANR5L16B2436", "triAmendement": "auaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaac", "cardinaliteAmdtMultiples": "1", "amendementParentRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "signataires": {"auteur": {"typeAuteur": "D\u00e9put\u00e9", "gouvernementRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "acteurRef": "PA793476", "groupePolitiqueRef": "PO800508", "auteurRapporteurOrganeRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "cosignataires": {"acteurRef": ["PA719968", "PA266793", "PA342240", "PA793508", "PA718838", "PA267901", "PA330909", "PA712015", "PA608416", "PA719272", "PA795406", "PA793094", "PA794178", "PA643134", "PA722390", "PA608016", "PA721314", "PA607090", "PA721474", "PA346054", "PA608826", "PA721442"]}, "suffixe": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "libelle": "M.&#160;Dubois,  Mme&#160;Bonnet, M.&#160;Bony, M.&#160;Bourgeaux, M.&#160;Brigand, M.&#160;Fabrice&#160;Brun, M.&#160;Kamardine, M.&#160;Descoeur, M.&#160;Dive, M.&#160;Hetzel, Mme&#160;Fr&#233;d&#233;rique&#160;Meunier, Mme&#160;P&#233;rigault, M.&#160;Ray, M.&#160;Taite, Mme&#160;Valentin, Mme&#160;Corneloup, Mme&#160;Louwagie, M.&#160;Schellenberger, M.&#160;Jean-Pierre&#160;Vigier, M.&#160;Viry, M.&#160;Emmanuel&#160;Maquet, Mme&#160;Duby-Muller et Mme&#160;Petex"}, "pointeurFragmentTexte": {"partieAmendableRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "division": {"titre": "Article 2", "articleDesignationCourte": "APR\u00c8S ART. 2", "articleDesignation": "APR\u00c8S L'ARTICLE 2, ins\u00e9rer l'article suivant:", "type": "ARTICLE", "avant_A_Apres": "Apr\u00e8s", "divisionRattachee": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "articleAdditionnel": "true", "chapitreAdditionnel": "false", "urlDivisionTexteVise": "/16/textes/2436.asp#D_Article_2"}, "amendementStandard": {"alinea": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "corps": {"cartoucheInformatif": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenuAuteur": {"dispositif": "<p style=\"text-align: justify;\">La section 9 <i>bis</i> du chapitre II du titre I<sup>er</sup> du livre III de la deuxi&#x00E8;me partie du code de l&#x2019;&#x00E9;ducation est compl&#x00E9;t&#x00E9;e par un article L.&nbsp;312&#x2011;17&#x2011;3 ainsi r&#x00E9;dig&#x00E9;&nbsp;:&nbsp;</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;<i>Art.&nbsp;L.&nbsp;312&#x2011;17&#x2011;3&nbsp;</i>.&nbsp;&#x2013;&nbsp;D&#x00E8;s l&#x2019;&#x00E9;cole primaire, des modules d&#x2019;information et de d&#x00E9;couverte de l&#x2019;agriculture et des modes de productions agricoles sont dispens&#x00E9;s aux &#x00E9;l&#x00E8;ves afin de les sensibiliser &#x00E0; la r&#x00E9;alit&#x00E9; du monde agricole&nbsp;et de leur transmettre des connaissances et des savoirs relatifs &#x00E0; la nature, &#x00E0; la culture, &#x00E0; une nutrition saine et &#x00E0; la n&#x00E9;cessit&#x00E9; de prot&#x00E9;ger notre souverainet&#x00E9; alimentaire et agricole.&nbsp;&#x00BB;</p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p style=\"text-align: justify;\">En coh&#x00E9;rence avec les orientations du programme national relatif &#x00E0; la nutrition et &#x00E0; la sant&#x00E9;&nbsp;et avec le programme national pour l'alimentation, il para&#x00EE;t indispensable de dispenser &#x00E0; nos &#x00E9;l&#x00E8;ves, d&#x00E8;s l'&#x00E9;cole primaire, une information et une &#x00E9;ducation &#x00E0; l&#x2019;agriculture et aux modes de productions agricoles afin de sensibiliser les jeunes g&#x00E9;n&#x00E9;rations aux enjeux li&#x00E9;s &#x00E0; notre agriculture fran&#x00E7;aise, et ainsi pr&#x00E9;server notre souverainet&#x00E9; alimentaire et agricole.<br><br></p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2024-04-23", "datePublication": "2024-04-30", "soumisArticle40": "false", "etatDesTraitements": {"etat": {"code": "DI", "libelle": "Discut\u00e9"}, "sousEtat": {"code": "DI", "libelle": "Adopt\u00e9"}}, "dateSort": "2024-05-03T00:14:39+02:00", "sort": "Adopt\u00e9"}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L16PO419610B2436P0D1N000961?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "seanceDiscussionRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "article99": "false", "loiReference": {"codeLoi": "c \u00e9ducation", "divisionCodeLoi": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "discussionCommune": {"idDiscussion": "24358", "typePosition": "Premier"}, "discussionIdentique": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "accordGouvernementDepotHorsDelai": "Sans objet"}}