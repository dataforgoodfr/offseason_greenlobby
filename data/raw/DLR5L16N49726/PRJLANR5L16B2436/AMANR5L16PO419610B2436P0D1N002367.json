{"amendement": {"@xmlns": "http://schemas.assemblee-nationale.fr/referentiel", "uid": "AMANR5L16PO419610B2436P0D1N002367", "chronotag": "1714636793383", "legislature": "16", "identification": {"numeroLong": "CE2367", "numeroOrdreDepot": "2367", "prefixeOrganeExamen": "CION-ECO", "numeroRect": "0"}, "examenRef": "EXANR5L16PO419610B2436P0D1", "texteLegislatifRef": "PRJLANR5L16B2436", "triAmendement": "aeaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaafzzzzxzb", "cardinaliteAmdtMultiples": "1", "amendementParentRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "signataires": {"auteur": {"typeAuteur": "D\u00e9put\u00e9", "gouvernementRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "acteurRef": "PA722202", "groupePolitiqueRef": "PO800502", "auteurRapporteurOrganeRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "cosignataires": {"acteurRef": ["PA267306", "PA720838", "PA796106", "PA795868", "PA795192", "PA718926", "PA721896", "PA795998", "PA795184", "PA774962", "PA335612", "PA796010", "PA793174", "PA795820", "PA721270", "PA795240", "PA795876", "PA720692", "PA605745", "PA794650", "PA720992"]}, "suffixe": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "libelle": "M.&#160;Jumel,  M.&#160;Chassaigne, Mme&#160;Bourouaha, M.&#160;B&#233;nard, M.&#160;Castor, M.&#160;Chailloux, M.&#160;Dharr&#233;ville, Mme&#160;Faucillon, Mme&#160;K/Bidi, M.&#160;Le&#160;Gayic, Mme&#160;Lebon, M.&#160;Lecoq, M.&#160;Maillot, M.&#160;Monnet, M.&#160;Nadeau, M.&#160;Peu, Mme&#160;Reid&#160;Arbelot, M.&#160;Rimane, M.&#160;Roussel, M.&#160;Sansu, M.&#160;Tellier et M.&#160;William"}, "pointeurFragmentTexte": {"partieAmendableRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "division": {"titre": "Article PREMIER", "articleDesignationCourte": "ART. PREMIER", "articleDesignation": "ARTICLE PREMIER", "type": "ARTICLE", "avant_A_Apres": "A", "divisionRattachee": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "articleAdditionnel": "false", "chapitreAdditionnel": "false", "urlDivisionTexteVise": "/16/textes/2436.asp#D_Article_1er"}, "amendementStandard": {"alinea": {"avant_A_Apres": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "numero": "6", "alineaDesignation": "Alin\u00e9a 6"}}}, "corps": {"cartoucheInformatif": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenuAuteur": {"dispositif": "<p style=\"text-align: justify;\">&#x00C0; l&#x2019;alin&#x00E9;a&nbsp;6, apr&#x00E8;s les mots&nbsp;:&nbsp;</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;sa capacit&#x00E9;&nbsp;&#x00BB;,&nbsp;</p><p style=\"text-align: justify;\">ins&#x00E9;rer les mots&nbsp;:</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;&#x00E0; anticiper et s&#x2019;adapter aux cons&#x00E9;quences du changement climatique compte tenu de la trajectoire du r&#x00E9;chauffement de r&#x00E9;f&#x00E9;rence, &#x00E0; en att&#x00E9;nuer les effets et&nbsp;&#x00BB;.</p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p style=\"text-align: justify;\">Le pr&#x00E9;sent projet de loi introduit dans le code rural un nouvel article L. 1 A propre aux orientations des politiques publiques associ&#x00E9;es &#x00E0; la protection de la souverainet&#x00E9; alimentaire. La r&#x00E9;daction propos&#x00E9;e est toutefois d&#x00E9;faillante et ne fait en particulier nulle r&#x00E9;f&#x00E9;rence aux implications commerciales, aux droits et aux revenus des agriculteurs. Le pr&#x00E9;sent amendement tend en cons&#x00E9;quence &#x00E0; rappeler que la souverainet&#x00E9; alimentaire ne peut &#x00EA;tre conquise sans une politique volontariste d&#x2019;anticipation des cons&#x00E9;quences du d&#x00E9;r&#x00E8;glement climatique et de planification &#x00E9;cologique.</p><p style=\"text-align: justify;\">Cet amendement s'inspire d'une proposition du collectif Nourrir.</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2024-04-26", "datePublication": "2024-04-29", "soumisArticle40": "false", "etatDesTraitements": {"etat": {"code": "DI", "libelle": "Discut\u00e9"}, "sousEtat": {"code": "DI", "libelle": "Non soutenu"}}, "dateSort": "2024-05-02T09:59:26+02:00", "sort": "Non soutenu"}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L16PO419610B2436P0D1N002367?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "seanceDiscussionRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "article99": "false", "loiReference": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionCommune": {"idDiscussion": "24443", "typePosition": "Milieu"}, "discussionIdentique": {"idDiscussion": "69515", "typePosition": "Milieu"}, "accordGouvernementDepotHorsDelai": "Sans objet"}}