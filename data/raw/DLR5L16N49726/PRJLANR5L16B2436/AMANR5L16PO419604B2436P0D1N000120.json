{"amendement": {"@xmlns": "http://schemas.assemblee-nationale.fr/referentiel", "uid": "AMANR5L16PO419604B2436P0D1N000120", "chronotag": "1714396995377", "legislature": "16", "identification": {"numeroLong": "AC120", "numeroOrdreDepot": "120", "prefixeOrganeExamen": "CION-CEDU", "numeroRect": "0"}, "examenRef": "EXANR5L16PO419604B2436P0D1", "texteLegislatifRef": "PRJLANR5L16B2436", "triAmendement": "boaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaxrr", "cardinaliteAmdtMultiples": "1", "amendementParentRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "signataires": {"auteur": {"typeAuteur": "Rapporteur", "gouvernementRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "acteurRef": "PA720256", "groupePolitiqueRef": "PO800484", "auteurRapporteurOrganeRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "cosignataires": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "suffixe": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "libelle": "Mme&#160;Bannier, rapporteure"}, "pointeurFragmentTexte": {"partieAmendableRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "division": {"titre": "Article 5", "articleDesignationCourte": "ART. 5", "articleDesignation": "ARTICLE 5", "type": "ARTICLE", "avant_A_Apres": "A", "divisionRattachee": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "articleAdditionnel": "false", "chapitreAdditionnel": "false", "urlDivisionTexteVise": "/16/textes/2436.asp#D_Article_5"}, "amendementStandard": {"alinea": {"avant_A_Apres": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "numero": "3", "alineaDesignation": "Alin\u00e9a 3"}}}, "corps": {"cartoucheInformatif": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenuAuteur": {"dispositif": "<p style=\"text-align: justify;\">&#x00C0; la derni&#x00E8;re phrase de l&#x2019;alin&#x00E9;a&nbsp;3, substituer aux mots&nbsp;:</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;d&#x00E9;nomm&#x00E9; &#x00AB;&nbsp;Bachelor Agro&nbsp;&#x00BB;&nbsp;&#x00BB;</p><p style=\"text-align: justify;\">les mots&nbsp;:</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;une licence professionnelle&nbsp;&#x00BB;.</p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p style=\"text-align: justify;\">Cet amendement vise &#x00E0; clarifier la nature du dipl&#x00F4;me d&#x00E9;nomm&#x00E9; &#x00AB;&nbsp;Bachelor&nbsp;&#x00BB;. Il s&#x2019;agit bel et bien d&#x2019;une licence. Il est important que les jeunes qui s&#x2019;engagent dans une voie professionnelle agricole via ce dipl&#x00F4;me appel&#x00E9; &#x00AB;&nbsp;Bachelor&nbsp;&#x00BB; voit ce dipl&#x00F4;me reconnu &#x00E0; sa juste valeur qui ne saurait &#x00EA;tre inf&#x00E9;rieur &#x00E0; une licence. C&#x2019;est pourquoi la mention explicite du mot &#x00AB;&nbsp;licence&nbsp;&#x00BB; appara&#x00EE;t comme une reconnaissance de la qualit&#x00E9; de la formation propos&#x00E9;e.&nbsp;</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2024-04-25", "datePublication": "2024-04-26", "soumisArticle40": "false", "etatDesTraitements": {"etat": {"code": "RT", "libelle": "Retir\u00e9"}, "sousEtat": {"code": "RT", "libelle": "Retir\u00e9 apr\u00e8s publication"}}, "dateSort": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "sort": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L16PO419604B2436P0D1N000120?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "seanceDiscussionRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "article99": "false", "loiReference": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionCommune": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionIdentique": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "accordGouvernementDepotHorsDelai": "Sans objet"}}