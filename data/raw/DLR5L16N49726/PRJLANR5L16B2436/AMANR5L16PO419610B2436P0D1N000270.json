{"amendement": {"@xmlns": "http://schemas.assemblee-nationale.fr/referentiel", "uid": "AMANR5L16PO419610B2436P0D1N000270", "chronotag": "1714641984092", "legislature": "16", "identification": {"numeroLong": "CE270", "numeroOrdreDepot": "270", "prefixeOrganeExamen": "CION-ECO", "numeroRect": "0"}, "examenRef": "EXANR5L16PO419610B2436P0D1", "texteLegislatifRef": "PRJLANR5L16B2436", "triAmendement": "aeaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaafzzzzxzzzyzwbi", "cardinaliteAmdtMultiples": "1", "amendementParentRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "signataires": {"auteur": {"typeAuteur": "D\u00e9put\u00e9", "gouvernementRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "acteurRef": "PA712015", "groupePolitiqueRef": "PO800508", "auteurRapporteurOrganeRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "cosignataires": {"acteurRef": ["PA718838", "PA605991", "PA793476", "PA266793", "PA1327", "PA342240", "PA720586", "PA794038", "PA793094", "PA721442", "PA721474", "PA719318", "PA794410", "PA608416", "PA719272", "PA721314", "PA722390", "PA346054", "PA718850", "PA719968", "PA608826", "PA266808"]}, "suffixe": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "libelle": "M.&#160;Dive,  M.&#160;Fabrice&#160;Brun, Mme&#160;Genevard, M.&#160;Dubois, M.&#160;Bony, M.&#160;Forissier, M.&#160;Bourgeaux, M.&#160;Vatin, M.&#160;Neuder, M.&#160;Ray, Mme&#160;Petex, M.&#160;Viry, Mme&#160;Anthoine, M.&#160;Seitlinger, M.&#160;Hetzel, Mme&#160;Fr&#233;d&#233;rique&#160;Meunier, M.&#160;Schellenberger, Mme&#160;Corneloup, M.&#160;Emmanuel&#160;Maquet, M.&#160;Cordier, Mme&#160;Bonnet, Mme&#160;Duby-Muller et M.&#160;Rolland"}, "pointeurFragmentTexte": {"partieAmendableRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "division": {"titre": "Article PREMIER", "articleDesignationCourte": "ART. PREMIER", "articleDesignation": "ARTICLE PREMIER", "type": "ARTICLE", "avant_A_Apres": "A", "divisionRattachee": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "articleAdditionnel": "false", "chapitreAdditionnel": "false", "urlDivisionTexteVise": "/16/textes/2436.asp#D_Article_1er"}, "amendementStandard": {"alinea": {"avant_A_Apres": "Apres", "numero": "7", "alineaDesignation": "Apr\u00e8s l'alin\u00e9a 7"}}}, "corps": {"cartoucheInformatif": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenuAuteur": {"dispositif": "<p style=\"text-align: justify;\">Apr&#x00E8;s l'alin&#x00E9;a 7, ins&#x00E9;rer l'alin&#x00E9;a suivant :</p><p>&#x00AB;&nbsp;&#x2013;&nbsp;sa capacit&#x00E9; &#x00E0; faciliter le renouvellement des g&#x00E9;n&#x00E9;rations en agriculture. &#x00BB;</p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p style=\"text-align: justify;\">L&#x2019;int&#x00E9;gration de l&#x2019;objectif du renouvellement des g&#x00E9;n&#x00E9;rations en agriculture au premier rang des politiques publiques agricoles, est indispensable. L&#x2019;enjeu d&#x00E9;mographique est en effet au fondement m&#x00EA;me de l&#x2019;exercice de l&#x2019;activit&#x00E9; agricole dans son ensemble, qui d&#x00E9;terminera, entre autres, l&#x2019;avenir de l&#x2019;agriculture fran&#x00E7;aise. Ainsi, l&#x2019;accent doit &#x00EA;tre mis sur les leviers &#x00E0; mettre en &#x0153;uvre au sein des politiques publiques, pour favoriser ce renouvellement g&#x00E9;n&#x00E9;rationnel, cela justifiant son inscription dans la liste des objectifs politiques &#x00E0; poursuivre, dans la poursuite de l&#x2019;objectif de souverainet&#x00E9; alimentaire.</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2024-04-17", "datePublication": "2024-04-30", "soumisArticle40": "false", "etatDesTraitements": {"etat": {"code": "DI", "libelle": "Discut\u00e9"}, "sousEtat": {"code": "DI", "libelle": "Adopt\u00e9"}}, "dateSort": "2024-05-02T11:26:10+02:00", "sort": "Adopt\u00e9"}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L16PO419610B2436P0D1N000270?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "seanceDiscussionRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "article99": "false", "loiReference": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionCommune": {"idDiscussion": "24567", "typePosition": "Milieu"}, "discussionIdentique": {"idDiscussion": "69519", "typePosition": "Milieu"}, "accordGouvernementDepotHorsDelai": "Sans objet"}}