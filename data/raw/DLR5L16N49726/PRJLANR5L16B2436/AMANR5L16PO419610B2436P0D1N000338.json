{"amendement": {"@xmlns": "http://schemas.assemblee-nationale.fr/referentiel", "uid": "AMANR5L16PO419610B2436P0D1N000338", "chronotag": "1714817993389", "legislature": "16", "identification": {"numeroLong": "CE338", "numeroOrdreDepot": "338", "prefixeOrganeExamen": "CION-ECO", "numeroRect": "0"}, "examenRef": "EXANR5L16PO419610B2436P0D1", "texteLegislatifRef": "PRJLANR5L16B2436", "triAmendement": "jiaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaanylo", "cardinaliteAmdtMultiples": "1", "amendementParentRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "signataires": {"auteur": {"typeAuteur": "D\u00e9put\u00e9", "gouvernementRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "acteurRef": "PA712015", "groupePolitiqueRef": "PO800508", "auteurRapporteurOrganeRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "cosignataires": {"acteurRef": ["PA719318", "PA719968", "PA266793", "PA718838", "PA722390", "PA718850", "PA330909", "PA608826", "PA1327", "PA605991", "PA608416", "PA346054", "PA719272", "PA794038", "PA721442", "PA795406", "PA793094", "PA721314", "PA794410", "PA794178", "PA720586", "PA721474"]}, "suffixe": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "libelle": "M.&#160;Dive,  Mme&#160;Anthoine, Mme&#160;Bonnet, M.&#160;Bony, M.&#160;Fabrice&#160;Brun, Mme&#160;Corneloup, M.&#160;Cordier, M.&#160;Descoeur, Mme&#160;Duby-Muller, M.&#160;Forissier, Mme&#160;Genevard, M.&#160;Hetzel, M.&#160;Emmanuel&#160;Maquet, Mme&#160;Fr&#233;d&#233;rique&#160;Meunier, M.&#160;Neuder, Mme&#160;Petex, Mme&#160;P&#233;rigault, M.&#160;Ray, M.&#160;Schellenberger, M.&#160;Seitlinger, M.&#160;Taite, M.&#160;Vatin et M.&#160;Viry"}, "pointeurFragmentTexte": {"partieAmendableRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "division": {"titre": "Article 14", "articleDesignationCourte": "ART. 14", "articleDesignation": "ARTICLE 14", "type": "ARTICLE", "avant_A_Apres": "A", "divisionRattachee": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "articleAdditionnel": "false", "chapitreAdditionnel": "false", "urlDivisionTexteVise": "/16/textes/2436.asp#D_Article_14"}, "amendementStandard": {"alinea": {"avant_A_Apres": "Apres", "numero": "24", "alineaDesignation": "Apr\u00e8s l'alin\u00e9a 24"}}}, "corps": {"cartoucheInformatif": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenuAuteur": {"dispositif": "<p style=\"text-align: justify;\">Apr&#x00E8;s l&#x2019;alin&#x00E9;a&nbsp;24, ins&#x00E9;rer les trois alin&#x00E9;as suivants&nbsp;:&nbsp;</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;11&#x00B0;&nbsp;Autorisation sp&#x00E9;ciale au titre des abords des monuments historiques en application de l&#x2019;article L.&nbsp;621&#x2011;32 du code du patrimoine&nbsp;; &nbsp;</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;12&#x00B0;&nbsp;Autorisation sp&#x00E9;ciale au titre des sites patrimoniaux remarquables en application de l&#x2019;article L.&nbsp;632&#x2011;1 du code du patrimoine&nbsp;; &nbsp;</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;13&#x00B0;&nbsp;D&#x00E9;claration pr&#x00E9;alable au titre des sites inscrits en application du troisi&#x00E8;me alin&#x00E9;a de l&#x2019;article L.&nbsp;341&#x2011;1 du code de l&#x2019;environnement.&nbsp;&#x00BB; &nbsp;</p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p style=\"text-align: justify;\">Cet amendement a pour objectif d&#x2019;int&#x00E9;grer d&#x2019;autres r&#x00E8;glementations pouvant s&#x2019;appliquer &#x00E0; une haie. Le but de ce m&#x00E9;canisme &#x00E9;tant de simplifier les d&#x00E9;marches pour les p&#x00E9;titionnaires, la d&#x00E9;marche d&#x2019;autorisation se doit d&#x2019;&#x00EA;tre exhaustive pour ne pas avoir de proc&#x00E9;dure administrative en parall&#x00E8;le. C&#x2019;est pourquoi int&#x00E9;grer les deux autorisations concernant les monuments historiques, les sites patrimoniaux et la d&#x00E9;claration concernant les sites inscrits permet de r&#x00E9;pondre &#x00E0; l&#x2019;objectif de cette nouvelle section relative &#x00E0; la protection des haies.&nbsp;</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2024-04-18", "datePublication": "2024-04-30", "soumisArticle40": "false", "etatDesTraitements": {"etat": {"code": "DI", "libelle": "Discut\u00e9"}, "sousEtat": {"code": "DI", "libelle": "Adopt\u00e9"}}, "dateSort": "2024-05-04T12:19:34+02:00", "sort": "Adopt\u00e9"}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L16PO419610B2436P0D1N000338?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "seanceDiscussionRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "article99": "false", "loiReference": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionCommune": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionIdentique": {"idDiscussion": "69273", "typePosition": "Premier"}, "accordGouvernementDepotHorsDelai": "Sans objet"}}