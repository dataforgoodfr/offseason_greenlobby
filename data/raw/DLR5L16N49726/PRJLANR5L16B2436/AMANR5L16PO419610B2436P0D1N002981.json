{"amendement": {"@xmlns": "http://schemas.assemblee-nationale.fr/referentiel", "uid": "AMANR5L16PO419610B2436P0D1N002981", "chronotag": "1714770383384", "legislature": "16", "identification": {"numeroLong": "CE2981", "numeroOrdreDepot": "2981", "prefixeOrganeExamen": "CION-ECO", "numeroRect": "0"}, "examenRef": "EXANR5L16PO419610B2436P0D1", "texteLegislatifRef": "PRJLANR5L16B2436", "triAmendement": "faaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaro", "cardinaliteAmdtMultiples": "1", "amendementParentRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "signataires": {"auteur": {"typeAuteur": "D\u00e9put\u00e9", "gouvernementRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "acteurRef": "PA793664", "groupePolitiqueRef": "PO800526", "auteurRapporteurOrganeRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "cosignataires": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "suffixe": "et les membres du groupe \u00c9cologiste - NUPES", "libelle": "Mme&#160;Pochon et les membres du groupe &#201;cologiste - NUPES"}, "pointeurFragmentTexte": {"partieAmendableRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "division": {"titre": "Article 10", "articleDesignationCourte": "ART. 10", "articleDesignation": "ARTICLE 10", "type": "ARTICLE", "avant_A_Apres": "A", "divisionRattachee": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "articleAdditionnel": "false", "chapitreAdditionnel": "false", "urlDivisionTexteVise": "/16/textes/2436.asp#D_Article_10"}, "amendementStandard": {"alinea": {"avant_A_Apres": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "numero": "21", "alineaDesignation": "Alin\u00e9a 21"}}}, "corps": {"cartoucheInformatif": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenuAuteur": {"dispositif": "<p style=\"text-align: justify;\">Apr&#x00E8;s l&#x2019;alin&#x00E9;a&nbsp;21, ins&#x00E9;rer l&#x2019;alin&#x00E9;a suivant&nbsp;:</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;4&#x00B0;&nbsp;<i>bis</i> Le m&#x00EA;me sixi&#x00E8;me alin&#x00E9;a est compl&#x00E9;t&#x00E9; par deux phrases ainsi r&#x00E9;dig&#x00E9;es&nbsp;:&nbsp;&#x00AB;&nbsp;Cet observatoire renseigne des indicateurs de suivi qui sont d&#x00E9;finis par d&#x00E9;cret apr&#x00E8;s avis d&#x2019;une&nbsp;instance nationale de pilotage de la politique d&#x2019;installation et de transmission et en publie une&nbsp;synth&#x00E8;se annuelle. L&#x2019;ensemble de ses donn&#x00E9;es sont publiques.&nbsp;&#x00BB;&nbsp;&#x00BB;</p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p style=\"text-align: justify;\">Cet amendement propose de concr&#x00E9;tiser l'observatoire national de l'installation en pr&#x00E9;voyant des indicateurs de suivi d&#x00E9;finis par d&#x00E9;cret et la publication d'une synth&#x00E8;se annuelle publiquement.&nbsp;</p><p style=\"text-align: justify;\">Cet amendement est propos&#x00E9; par la FADEAR, la FNAB, le MIRAMAP, le RENETA, le R&#x00E9;seau CIVAM, SOL, Terre de Liens et le p&#x00F4;le INPACT. Aux c&#x00F4;t&#x00E9;s des chambres d'agriculture, les organisations membres de cette coalition repr&#x00E9;sentent des acteurs diversifi&#x00E9;s et compl&#x00E9;mentaires participant activement &#x00E0; la mise en &#x0153;uvre des politiques et dispositifs d&#x2019;accompagnement &#x00E0; l&#x2019;installation-transmission sur les territoires. Les associations et r&#x00E9;seaux paysans et citoyens d&#x2019;accompagnement &#x00E0; l&#x2019;installation-transmission accompagnent aujourd&#x2019;hui plus de 10 000 candidats et 2 000 c&#x00E9;dants par an, avec &#x00E0; la cl&#x00E9; plus d&#x2019;un tiers des installations annuelles.</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2024-04-26", "datePublication": "2024-04-29", "soumisArticle40": "false", "etatDesTraitements": {"etat": {"code": "DI", "libelle": "Discut\u00e9"}, "sousEtat": {"code": "DI", "libelle": "Rejet\u00e9"}}, "dateSort": "2024-05-03T23:06:11+02:00", "sort": "Rejet\u00e9"}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L16PO419610B2436P0D1N002981?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "seanceDiscussionRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "article99": "false", "loiReference": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionCommune": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionIdentique": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "accordGouvernementDepotHorsDelai": "Sans objet"}}