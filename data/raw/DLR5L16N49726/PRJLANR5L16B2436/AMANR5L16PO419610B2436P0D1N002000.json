{"amendement": {"@xmlns": "http://schemas.assemblee-nationale.fr/referentiel", "uid": "AMANR5L16PO419610B2436P0D1N002000", "chronotag": "1714640723552", "legislature": "16", "identification": {"numeroLong": "CE2000", "numeroOrdreDepot": "2000", "prefixeOrganeExamen": "CION-ECO", "numeroRect": "0"}, "examenRef": "EXANR5L16PO419610B2436P0D1", "texteLegislatifRef": "PRJLANR5L16B2436", "triAmendement": "aeaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaafzzzzxzzzx", "cardinaliteAmdtMultiples": "1", "amendementParentRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "signataires": {"auteur": {"typeAuteur": "D\u00e9put\u00e9", "gouvernementRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "acteurRef": "PA793964", "groupePolitiqueRef": "PO800520", "auteurRapporteurOrganeRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "cosignataires": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "suffixe": "et les membres du groupe Rassemblement National", "libelle": "M.&#160;de&#160;Fournas et les membres du groupe Rassemblement National"}, "pointeurFragmentTexte": {"partieAmendableRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "division": {"titre": "Article PREMIER", "articleDesignationCourte": "ART. PREMIER", "articleDesignation": "ARTICLE PREMIER", "type": "ARTICLE", "avant_A_Apres": "A", "divisionRattachee": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "articleAdditionnel": "false", "chapitreAdditionnel": "false", "urlDivisionTexteVise": "/16/textes/2436.asp#D_Article_1er"}, "amendementStandard": {"alinea": {"avant_A_Apres": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "numero": "7", "alineaDesignation": "Alin\u00e9a 7"}}}, "corps": {"cartoucheInformatif": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenuAuteur": {"dispositif": "<p style=\"text-align: justify;\">Compl&#x00E9;ter l&#x2019;alin&#x00E9;a&nbsp;7 par la phrase suivante&nbsp;:&nbsp;</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;L&#x2019;objectif de d&#x00E9;carbonation est subordonn&#x00E9; &#x00E0; l&#x2019;imp&#x00E9;ratif de pr&#x00E9;servation de la souverainet&#x00E9; alimentaire&nbsp;&#x00BB;. &nbsp;</p><p style=\"text-align: justify;\">&nbsp;</p><p style=\"text-align: justify;\">&nbsp;</p><p style=\"text-align: justify;\">&nbsp;</p><p style=\"text-align: justify;\">&nbsp;</p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p style=\"text-align: justify;\">La contribution de l&#x2019;agriculture &#x00E0; la d&#x00E9;carbonation de l&#x2019;&#x00E9;conomie est &#x00E9;videmment souhaitable mais celle-ci ne peut se faire au d&#x00E9;triment des capacit&#x00E9;s de production agricoles de la France, essentielles afin de restaurer notre souverainet&#x00E9; alimentaire.</p><p style=\"text-align: justify;\">Les capacit&#x00E9;s de production de notre pays sont d&#x00E9;j&#x00E0;&#x0300; consid&#x00E9;rablement mises a&#x0300; mal par l&#x2019;inflation normative et la surr&#x00E8;glementation par rapport aux normes europ&#x00E9;ennes qui asphyxie les fili&#x00E8;res agricoles fran&#x00E7;aises.</p><p style=\"text-align: justify;\">Rappelons que la Chine et les &#x00C9;tats-Unis repr&#x00E9;sentent &#x00E0; eux seuls pr&#x00E8;s de la moiti&#x00E9; des &#x00E9;missions carbone mondiales contre 7&nbsp;% pour l&#x2019;UE et seulement 1&nbsp;% pour la France.</p><p style=\"text-align: justify;\">&nbsp;</p><p style=\"text-align: justify;\">Il est essentiel que cet objectif de d&#x00E9;carbonation soit subordonn&#x00E9; &#x00E0; l&#x2019;imp&#x00E9;ratif de pr&#x00E9;servation de la souverainet&#x00E9; alimentaire.</p><p style=\"text-align: justify;\">&nbsp;</p><p style=\"text-align: justify;\">Cet amendement vise &#x00E0; pr&#x00E9;ciser cette hi&#x00E9;rarchie dans le premier article du projet de loi.</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2024-04-26", "datePublication": "2024-04-29", "soumisArticle40": "false", "etatDesTraitements": {"etat": {"code": "DI", "libelle": "Discut\u00e9"}, "sousEtat": {"code": "DI", "libelle": "Non soutenu"}}, "dateSort": "2024-05-02T11:04:55+02:00", "sort": "Non soutenu"}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L16PO419610B2436P0D1N002000?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "seanceDiscussionRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "article99": "false", "loiReference": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionCommune": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionIdentique": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "accordGouvernementDepotHorsDelai": "Sans objet"}}