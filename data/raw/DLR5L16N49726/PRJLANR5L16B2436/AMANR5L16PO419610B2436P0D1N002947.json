{"amendement": {"@xmlns": "http://schemas.assemblee-nationale.fr/referentiel", "uid": "AMANR5L16PO419610B2436P0D1N002947", "chronotag": "1714730393705", "legislature": "16", "identification": {"numeroLong": "CE2947", "numeroOrdreDepot": "2947", "prefixeOrganeExamen": "CION-ECO", "numeroRect": "0"}, "examenRef": "EXANR5L16PO419610B2436P0D1", "texteLegislatifRef": "PRJLANR5L16B2436", "triAmendement": "boaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaabmzzw", "cardinaliteAmdtMultiples": "1", "amendementParentRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "signataires": {"auteur": {"typeAuteur": "D\u00e9put\u00e9", "gouvernementRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "acteurRef": "PA720772", "groupePolitiqueRef": "PO800484", "auteurRapporteurOrganeRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "cosignataires": {"acteurRef": ["PA794510", "PA720054", "PA794386", "PA794878", "PA719640", "PA720414"]}, "suffixe": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "libelle": "M.&#160;Bru,  Mme&#160;Decodts, M.&#160;Haury, Mme&#160;M&#233;tayer, M.&#160;Mournet, Mme&#160;Mette et M.&#160;Perrot"}, "pointeurFragmentTexte": {"partieAmendableRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "division": {"titre": "Article 5", "articleDesignationCourte": "ART. 5", "articleDesignation": "ARTICLE 5", "type": "ARTICLE", "avant_A_Apres": "A", "divisionRattachee": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "articleAdditionnel": "false", "chapitreAdditionnel": "false", "urlDivisionTexteVise": "/16/textes/2436.asp#D_Article_5"}, "amendementStandard": {"alinea": {"avant_A_Apres": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "numero": "3", "alineaDesignation": "Alin\u00e9a 3"}}}, "corps": {"cartoucheInformatif": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenuAuteur": {"dispositif": "<p style=\"text-align: justify;\">&#x00C0; la seconde phrase de l&#x2019;alin&#x00E9;a&nbsp;3, substituer aux mots&nbsp;:&nbsp;</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;Bachelor Agro&nbsp;&#x00BB;,</p><p style=\"text-align: justify;\">les mots&nbsp;:&nbsp;.</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;Dipl&#x00F4;me national de premier cycle en sciences et techniques de l&#x2019;agronomie&nbsp;&#x00BB;.<br><br></p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p style=\"text-align: justify;\">Comme le souligne le Conseil d&#x2019;&#x00C9;tat dans son avis n&#x00B0;&nbsp;408136 rendu le 21 mars 2024, le terme &#x00AB;&nbsp;bachelor&nbsp;&#x00BB; ne figure dans aucune loi ou d&#x00E9;cret pris en Conseil d&#x2019;&#x00C9;tat en vigueur, ou encore dans le Vocabulaire de l&#x2019;&#x00E9;ducation et de la recherche. L&#x2019;emploi de ce mot emprunt&#x00E9; &#x00E0; l&#x2019;anglais ne peut s&#x2019;appuyer sur les m&#x00EA;mes justifications que celles prises en consid&#x00E9;ration lors de la cr&#x00E9;ation du grade de &#x00AB;&nbsp;master&nbsp;&#x00BB;, ces derni&#x00E8;res &#x00E9;tant &#x00E0; l&#x2019;&#x00E9;poque tir&#x00E9;es principalement de l&#x2019;objectif d&#x2019;harmonisation des dipl&#x00F4;mes europ&#x00E9;ens et de reconnaissance internationale (Conseil d&#x2019;&#x00C9;tat, 11&nbsp;juin&nbsp;2003, Association &#x00AB;&nbsp;Avenir de la langue fran&#x00E7;aise&nbsp;&#x00BB;, n&#x00B0;&nbsp;246971, 246972 et 246973).</p><p style=\"text-align: justify;\">&nbsp;</p><p style=\"text-align: justify;\">Il est ainsi propos&#x00E9; de s&#x2019;en tenir aux vocables en usage dans les textes et le lexique fran&#x00E7;ais.</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2024-04-26", "datePublication": "2024-05-01", "soumisArticle40": "false", "etatDesTraitements": {"etat": {"code": "DI", "libelle": "Discut\u00e9"}, "sousEtat": {"code": "DI", "libelle": "Tomb\u00e9"}}, "dateSort": "2024-05-03T11:59:31+02:00", "sort": "Tomb\u00e9"}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L16PO419610B2436P0D1N002947?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "seanceDiscussionRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "article99": "false", "loiReference": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionCommune": {"idDiscussion": "24481", "typePosition": "Milieu"}, "discussionIdentique": {"idDiscussion": "69224", "typePosition": "Dernier"}, "accordGouvernementDepotHorsDelai": "Sans objet"}}