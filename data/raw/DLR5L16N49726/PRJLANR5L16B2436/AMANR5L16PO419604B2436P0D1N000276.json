{"amendement": {"@xmlns": "http://schemas.assemblee-nationale.fr/referentiel", "uid": "AMANR5L16PO419604B2436P0D1N000276", "chronotag": "1714419828775", "legislature": "16", "identification": {"numeroLong": "AC276", "numeroOrdreDepot": "276", "prefixeOrganeExamen": "CION-CEDU", "numeroRect": "0"}, "examenRef": "EXANR5L16PO419604B2436P0D1", "texteLegislatifRef": "PRJLANR5L16B2436", "triAmendement": "boaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaadoi", "cardinaliteAmdtMultiples": "1", "amendementParentRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "signataires": {"auteur": {"typeAuteur": "Rapporteur", "gouvernementRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "acteurRef": "PA720190", "groupePolitiqueRef": "PO800538", "auteurRapporteurOrganeRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "cosignataires": {"acteurRef": "PA720256"}, "suffixe": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "libelle": "M.&#160;Sorre, rapporteur et Mme&#160;Bannier, rapporteure"}, "pointeurFragmentTexte": {"partieAmendableRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "division": {"titre": "Article 5", "articleDesignationCourte": "ART. 5", "articleDesignation": "ARTICLE 5", "type": "ARTICLE", "avant_A_Apres": "A", "divisionRattachee": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "articleAdditionnel": "false", "chapitreAdditionnel": "false", "urlDivisionTexteVise": "/16/textes/2436.asp#D_Article_5"}, "amendementStandard": {"alinea": {"avant_A_Apres": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "numero": "3", "alineaDesignation": "Alin\u00e9a 3"}}}, "corps": {"cartoucheInformatif": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenuAuteur": {"dispositif": "<p style=\"text-align: justify;\">&#x00C0; la premi&#x00E8;re phrase de l&#x2019;aline&#x0301;a 3, apre&#x0300;s la premi&#x00E8;re occurrence du mot&nbsp;:</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;agricole&nbsp;&#x00BB;,</p><p style=\"text-align: justify;\">&nbsp;inse&#x0301;rer les mots&nbsp;:</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;, les e&#x0301;tablissements prive&#x0301;s d&#x2019;enseignement supe&#x0301;rieur agricole,&nbsp;&#x00BB;.</p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p style=\"text-align: justify;\">Cet amendement vise &#x00E0; int&#x00E9;grer les &#x00E9;tablissements priv&#x00E9;s d&#x2019;enseignement sup&#x00E9;rieur agricoles dans le dispositif de l&#x2019;article&nbsp;5. En effet, si les &#x00E9;tablissements techniques priv&#x00E9;s sont bien inclus &#x00E0; l&#x2019;article&nbsp;5, en ce qu&#x2019;ils peuvent &#x00EA;tre accr&#x00E9;dit&#x00E9;s conjointement avec un &#x00E9;tablissement d&#x2019;enseignement sup&#x00E9;rieur public &#x00E0; dispenser une formation conduisant au Bachelor Agro, les &#x00E9;tablissements priv&#x00E9;s d&#x2019;enseignement sup&#x00E9;rieur&nbsp; d&#x2019;agricoles ne sont pas mentionn&#x00E9;s. Or, il para&#x00EE;t souhaitable de leur permettre d&#x2019;&#x00EA;tre accr&#x00E9;dit&#x00E9;s pour dispenser des formations relevant du Bachelor Agro, dans l&#x2019;objectif de favoriser la diffusion de cette nouvelle formation.</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2024-04-29", "datePublication": "2024-04-29", "soumisArticle40": "false", "etatDesTraitements": {"etat": {"code": "AC", "libelle": "A discuter"}, "sousEtat": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "dateSort": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "sort": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L16PO419604B2436P0D1N000276?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "seanceDiscussionRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "article99": "false", "loiReference": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionCommune": {"idDiscussion": "24506", "typePosition": "Milieu"}, "discussionIdentique": {"idDiscussion": "69335", "typePosition": "Premier"}, "accordGouvernementDepotHorsDelai": "Sans objet"}}