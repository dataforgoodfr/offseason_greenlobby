{"amendement": {"@xmlns": "http://schemas.assemblee-nationale.fr/referentiel", "uid": "AMANR5L16PO419610B2436P0D1N003335", "chronotag": "1714815473389", "legislature": "16", "identification": {"numeroLong": "CE3335", "numeroOrdreDepot": "3335", "prefixeOrganeExamen": "CION-ECO", "numeroRect": "0"}, "examenRef": "EXANR5L16PO419610B2436P0D1", "texteLegislatifRef": "PRJLANR5L16B2436", "triAmendement": "jiaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaby", "cardinaliteAmdtMultiples": "1", "amendementParentRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "signataires": {"auteur": {"typeAuteur": "D\u00e9put\u00e9", "gouvernementRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "acteurRef": "PA793992", "groupePolitiqueRef": "PO800514", "auteurRapporteurOrganeRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "cosignataires": {"acteurRef": ["PA605782", "PA794278", "PA721522", "PA795278", "PA332228", "PA719032", "PA642868", "PA267780", "PA794234", "PA794470", "PA722070", "PA719778", "PA719052", "PA794818", "PA719382", "PA794426", "PA794838", "PA720230", "PA720908", "PA793398", "PA793572", "PA719528", "PA795270", "PA793306", "PA720552", "PA722336", "PA795354", "PA421348", "PA794914", "PA795026"]}, "suffixe": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "libelle": "M.&#160;Alfandari,  M.&#160;Marcangeli, M.&#160;Albertini, M.&#160;Batut, Mme&#160;Bellamy, M.&#160;Benoit, M.&#160;Bouyx, M.&#160;Christophe, Mme&#160;Firmin&#160;Le&#160;Bodo, M.&#160;Gernigon, Mme&#160;F&#233;licie&#160;G&#233;rard, M.&#160;Henriet, M.&#160;Jolivet, M.&#160;Kervran, Mme&#160;Kochert, M.&#160;Lamirault, Mme&#160;Le&#160;H&#233;nanff, M.&#160;Lemaire, Mme&#160;Magnier, Mme&#160;Moutchou, M.&#160;Patrier-Leitus, M.&#160;Plassard, M.&#160;Portarrieu, Mme&#160;Poussier-Winsback, M.&#160;Pradal, Mme&#160;Rauch, M.&#160;Thi&#233;baut, Mme&#160;Vilgrain, M.&#160;Villiers, M.&#160;Vincendet et Mme&#160;Violland"}, "pointeurFragmentTexte": {"partieAmendableRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "division": {"titre": "Article 14", "articleDesignationCourte": "ART. 14", "articleDesignation": "ARTICLE 14", "type": "ARTICLE", "avant_A_Apres": "A", "divisionRattachee": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "articleAdditionnel": "false", "chapitreAdditionnel": "false", "urlDivisionTexteVise": "/16/textes/2436.asp#D_Article_14"}, "amendementStandard": {"alinea": {"avant_A_Apres": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "numero": "4", "alineaDesignation": "Alin\u00e9a 4"}}}, "corps": {"cartoucheInformatif": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenuAuteur": {"dispositif": "<p style=\"text-align: justify;\">I.&nbsp;&#x2013;&nbsp;Apr&#x00E8;s le mot&nbsp;:&nbsp;</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;ligneuse&nbsp;&#x00BB;,&nbsp;</p><p style=\"text-align: justify;\">la fin de l&#x2019;alin&#x00E9;a 4 est ainsi r&#x00E9;dig&#x00E9;&nbsp;:</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;implant&#x00E9;e &#x00E0; plat, sur talus ou sur creux, avec pr&#x00E9;sence d&#x2019;arbustes et, le cas &#x00E9;ch&#x00E9;ant, pr&#x00E9;sence d&#x2019;arbres ou d&#x2019;autres ligneux tels que les ronces, les gen&#x00EA;ts ou les ajoncs. Ne sont pas inclus dans les haies&nbsp;les alignements d&#x2019;arbres caract&#x00E9;ris&#x00E9;s par la pr&#x00E9;sence d&#x2019;une unit&#x00E9; lin&#x00E9;aire de v&#x00E9;g&#x00E9;tation ligneuse compos&#x00E9;e uniquement d&#x2019;arbres.&nbsp;&#x00BB;</p><p>II.&nbsp;&#x2013;&nbsp;&#x00C0; la fin de la seconde phrase de l&#x2019;alin&#x00E9;a 5, supprimer les mots&nbsp;:&nbsp;</p><p>&#x00AB;&nbsp;usuels en vue de valoriser les produits de la haie, notamment la biomasse.&nbsp;&#x00BB; sont supprim&#x00E9;s.</p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p style=\"text-align: justify;\">La d&#x00E9;finition de la haie, comportant plusieurs esp&#x00E8;ces, &#x00E9;tant trop vague dans cet article et sujette &#x00E0; confusion, portant m&#x00EA;me atteinte &#x00E0; l&#x2019;agroforesterie. Quant &#x00E0; l&#x2019;origine humaine mentionn&#x00E9;e dans cette d&#x00E9;finition, elle entra&#x00EE;ne une instabilit&#x00E9; juridique parce qu&#x2019;il est souvent impossible de d&#x00E9;finir le champ exact de l&#x2019;action humaine sur l&#x2019;existence d&#x2019;une haie<br>Cette r&#x00E9;&#x00E9;criture est bas&#x00E9;e sur le r&#x00E8;glement europ&#x00E9;en qui d&#x00E9;finit les haies.</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2024-04-26", "datePublication": "2024-04-30", "soumisArticle40": "false", "etatDesTraitements": {"etat": {"code": "DI", "libelle": "Discut\u00e9"}, "sousEtat": {"code": "DI", "libelle": "Tomb\u00e9"}}, "dateSort": "2024-05-04T11:37:30+02:00", "sort": "Tomb\u00e9"}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L16PO419610B2436P0D1N003335?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "seanceDiscussionRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "article99": "false", "loiReference": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionCommune": {"idDiscussion": "24599", "typePosition": "Premier"}, "discussionIdentique": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "accordGouvernementDepotHorsDelai": "Sans objet"}}