{"amendement": {"@xmlns": "http://schemas.assemblee-nationale.fr/referentiel", "uid": "AMANR5L16PO419610B2436P0D1N001091", "chronotag": "1714666163500", "legislature": "16", "identification": {"numeroLong": "CE1091", "numeroOrdreDepot": "1091", "prefixeOrganeExamen": "CION-ECO", "numeroRect": "0"}, "examenRef": "EXANR5L16PO419610B2436P0D1", "texteLegislatifRef": "PRJLANR5L16B2436", "triAmendement": "aeaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaafzzzzznzzzzzzwu", "cardinaliteAmdtMultiples": "1", "amendementParentRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "signataires": {"auteur": {"typeAuteur": "D\u00e9put\u00e9", "gouvernementRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "acteurRef": "PA330909", "groupePolitiqueRef": "PO800508", "auteurRapporteurOrganeRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "cosignataires": {"acteurRef": ["PA721410", "PA267901", "PA643134"]}, "suffixe": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "libelle": "M.&#160;Descoeur,  Mme&#160;Bonnivard, M.&#160;Kamardine et Mme&#160;Valentin"}, "pointeurFragmentTexte": {"partieAmendableRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "division": {"titre": "Article PREMIER", "articleDesignationCourte": "ART. PREMIER", "articleDesignation": "ARTICLE PREMIER", "type": "ARTICLE", "avant_A_Apres": "A", "divisionRattachee": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "articleAdditionnel": "false", "chapitreAdditionnel": "false", "urlDivisionTexteVise": "/16/textes/2436.asp#D_Article_1er"}, "amendementStandard": {"alinea": {"avant_A_Apres": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "numero": "19", "alineaDesignation": "Alin\u00e9a 19"}}}, "corps": {"cartoucheInformatif": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenuAuteur": {"dispositif": "<p style=\"text-align: justify;\">&#x00C0; l'alin&#x00E9;a 19, apr&#x00E8;s le mot :&nbsp;</p><p style=\"text-align: justify;\">&#x00AB; essai &#x00BB;,&nbsp;</p><p>ins&#x00E9;rer les mots :&nbsp;</p><p>&#x00AB; d&#x00E9;fini au pr&#x00E9;sent code &#x00BB;.</p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p style=\"text-align: justify;\">L&#x2019;article&nbsp;1<sup>er</sup> modifie substantiellement l&#x2019;article L.&nbsp;1 du code rural et de la p&#x00EA;che maritime. Ce dernier tiendra mieux compte des sujets soci&#x00E9;taires en mati&#x00E8;re d&#x2019;installation et de transmission, en introduisant le &#x00AB;&nbsp;droit &#x00E0; l&#x2019;essai&nbsp;&#x00BB; dans cette politique, qui est une innovation importante, unanimement port&#x00E9;e par les organisations professionnelles agricoles.</p><p style=\"text-align: justify;\">L&#x2019;ajout de cette seule mention appara&#x00EE;t toutefois insuffisant, dans la mesure o&#x00F9; ces derni&#x00E8;res ont identifi&#x00E9; le besoin de fonder le droit &#x00E0; l&#x2019;essai sur un socle l&#x00E9;gislatif n&#x00E9;cessairement plus d&#x00E9;taill&#x00E9;. L&#x2019;ajout pr&#x00E9;vu par le pr&#x00E9;sent amendement constitue donc un compl&#x00E9;ment utile, en ce qu&#x2019;il appelle &#x00E0; d&#x00E9;finir la nature exacte du dispositif dans le code rural, et, par cons&#x00E9;quent, dans la pr&#x00E9;sente loi.</p><p style=\"text-align: justify;\">Cet amendement est propos&#x00E9; en concertation avec la FNSEA.</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2024-04-24", "datePublication": "2024-04-30", "soumisArticle40": "false", "etatDesTraitements": {"etat": {"code": "DI", "libelle": "Discut\u00e9"}, "sousEtat": {"code": "DI", "libelle": "Non soutenu"}}, "dateSort": "2024-05-02T18:09:02+02:00", "sort": "Non soutenu"}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L16PO419610B2436P0D1N001091?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "seanceDiscussionRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "article99": "false", "loiReference": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionCommune": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionIdentique": {"idDiscussion": "69153", "typePosition": "Milieu"}, "accordGouvernementDepotHorsDelai": "Sans objet"}}