tionCourte": "ART. 4", "articleDesignation": "ARTICLE 4", "type": "ARTICLE", "avant_A_Apres": "A", "divisionRattachee": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "articleAdditionnel": "false", "chapitreAdditionnel": "false", "urlDivisionTexteVise": "/16/textes/2436.asp#D_Article_4"}, "amendementStandard": {"alinea": {"avant_A_Apres": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "numero": "6", "alineaDesignation": "Alin\u00e9a 6"}}}, "corps": {"cartoucheInformatif": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenuAuteur": {"dispositif": "<p style=\"text-align: justify;\">Apr&#x00E8;s l&#x2019;alin&#x00E9;a&nbsp;6, ins&#x00E9;rer l&#x2019;alin&#x00E9;a suivant&nbsp;:</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;Il est &#x00E9;tabli pour une dur&#x00E9;e de trois ann&#x00E9;es.&nbsp;&#x00C0; l&#x2019;issue de cette p&#x00E9;riode, il est &#x00E9;valu&#x00E9; par une commission pr&#x00E9;sid&#x00E9;e par le Pr&#x00E9;fet, repr&#x00E9;sentant de l&#x2019;&#x00C9;tat en R&#x00E9;gion, assist&#x00E9; d&#x2019;une commission d&#x2019;&#x00E9;valuation dont la composition sera fix&#x00E9;e par d&#x00E9;cret.&nbsp;&#x00BB;</p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p style=\"text-align: justify;\">L&#x2019;alin&#x00E9;a&nbsp;5 de l&#x2019;article&nbsp;4 pr&#x00E9;voit que soit conclu, s&#x2019;il est pr&#x00E9;vu une augmentation du nombre d&#x2019;&#x00E9;l&#x00E8;ves accueillis dans une section, ou une ouverture de nouvelles sections de formation professionnelle sous statut scolaire, un &#x00AB;&nbsp;contrat territorial&nbsp;&#x00BB; avec les &#x00E9;tablissements concern&#x00E9;s.&nbsp;<br><br></p><p style=\"text-align: justify;\">Or aucune disposition dans ce texte ne fixe la dur&#x00E9;e de mise en &#x0153;uvre de ce contrat.&nbsp;</p><p style=\"text-align: justify;\">Autre lacune&nbsp;: le texte ne pr&#x00E9;voit nulle part de dispositif d&#x2019;&#x00E9;valuation. Il ne saurait pourtant y avoir de contrat entre l&#x2019;&#x00C9;tat ou la R&#x00E9;gion et un op&#x00E9;rateur sans limites temporelles, sans objectifs, sans &#x00E9;valuation terminale.</p><p style=\"text-align: justify;\"><br>Il convient de combler ces lacunes.</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2024-04-16", "datePublication": "2024-04-26", "soumisArticle40": "false", "etatDesTraitements": {"etat": {"code": "AC", "libelle": "A discuter"}, "sousEtat": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "dateSort": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "sort": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L16PO419604B2436P0D1N000005?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "seanceDiscussionRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "article99": "false", "loiReference": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionCommune": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionIdentique": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "accordGouvernementD