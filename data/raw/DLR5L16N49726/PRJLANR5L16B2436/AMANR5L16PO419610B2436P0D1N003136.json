{"amendement": {"@xmlns": "http://schemas.assemblee-nationale.fr/referentiel", "uid": "AMANR5L16PO419610B2436P0D1N003136", "chronotag": "1714644923416", "legislature": "16", "identification": {"numeroLong": "CE3136", "numeroOrdreDepot": "3136", "prefixeOrganeExamen": "CION-ECO", "numeroRect": "0"}, "examenRef": "EXANR5L16PO419610B2436P0D1", "texteLegislatifRef": "PRJLANR5L16B2436", "triAmendement": "aeaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaafzzzzxzzzzzzzxy", "cardinaliteAmdtMultiples": "1", "amendementParentRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "signataires": {"auteur": {"typeAuteur": "D\u00e9put\u00e9", "gouvernementRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "acteurRef": "PA719640", "groupePolitiqueRef": "PO800484", "auteurRapporteurOrganeRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "cosignataires": {"acteurRef": ["PA720162", "PA793122", "PA795100", "PA794810", "PA720092", "PA793560"]}, "suffixe": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "libelle": "Mme&#160;Mette,  M.&#160;Bolo, M.&#160;Daubi&#233;, M.&#160;Martineau, Mme&#160;Morel, M.&#160;Ramos et Mme&#160;Babault"}, "pointeurFragmentTexte": {"partieAmendableRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "division": {"titre": "Article PREMIER", "articleDesignationCourte": "ART. PREMIER", "articleDesignation": "ARTICLE PREMIER", "type": "ARTICLE", "avant_A_Apres": "A", "divisionRattachee": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "articleAdditionnel": "false", "chapitreAdditionnel": "false", "urlDivisionTexteVise": "/16/textes/2436.asp#D_Article_1er"}, "amendementStandard": {"alinea": {"avant_A_Apres": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "numero": "9", "alineaDesignation": "Alin\u00e9a 9"}}}, "corps": {"cartoucheInformatif": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenuAuteur": {"dispositif": "<p style=\"text-align: justify;\">&#x00C0; l&#x2019;alin&#x00E9;a&nbsp;9, substituer aux mots&nbsp;:&nbsp;</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;aquacole et halieutique&nbsp;&#x00BB;&nbsp;</p><p style=\"text-align: justify;\">les mots&nbsp;:</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;aquacole, halieutique et sylvicole&nbsp;&#x00BB;.</p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p style=\"text-align: justify;\">Le projet de loi d&#x2019;orientation retient que l&#x2019;agriculture, la p&#x00EA;che, l&#x2019;aquaculture et l&#x2019;alimentation sont d&#x2019;int&#x00E9;r&#x00EA;t g&#x00E9;n&#x00E9;ral majeur, en tant qu&#x2019;elles garantissent la souverainet&#x00E9; alimentaire. Par ailleurs, la souverainet&#x00E9; agricole est d&#x00E9;finie comme contribuant &#x00E0; la souverainet&#x00E9; alimentaire par une production durable de biomasse, et &#x00E0; la d&#x00E9;carbonation de l&#x2019;&#x00E9;conomie.</p><p style=\"text-align: justify;\">Les for&#x00EA;ts repr&#x00E9;sentent un enjeu majeur dans la d&#x00E9;carbonation de l&#x2019;&#x00E9;conomie, au travers du bois produit (bois mat&#x00E9;riaux, bois &#x00E9;nergie), par la fonction de stockage de carbone qu&#x2019;elles repr&#x00E9;sentent et par les services environnementaux et sociaux qu&#x2019;elles procurent aux citoyens. En cons&#x00E9;quence, elles participent &#x00E0; la souverainet&#x00E9; agricole. A ce titre, la sylviculture doit &#x00EA;tre ajout&#x00E9;e &#x00E0; la liste des activit&#x00E9;s dont la r&#x00E9;silience et le potentiel des facteurs de production doivent &#x00EA;tre pr&#x00E9;serv&#x00E9;s et am&#x00E9;lior&#x00E9;s. La sylviculture doit &#x00E9;galement &#x00EA;tre reconnue d&#x2019;int&#x00E9;r&#x00EA;t g&#x00E9;n&#x00E9;ral majeur.</p><p style=\"text-align: justify;\">Amendement travaill&#x00E9; avec Fransylva.</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2024-04-26", "datePublication": "2024-04-30", "soumisArticle40": "false", "etatDesTraitements": {"etat": {"code": "DI", "libelle": "Discut\u00e9"}, "sousEtat": {"code": "DI", "libelle": "Rejet\u00e9"}}, "dateSort": "2024-05-02T12:15:22+02:00", "sort": "Rejet\u00e9"}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L16PO419610B2436P0D1N003136?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "seanceDiscussionRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "article99": "false", "loiReference": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionCommune": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionIdentique": {"idDiscussion": "69652", "typePosition": "Dernier"}, "accordGouvernementDepotHorsDelai": "Sans objet"}}