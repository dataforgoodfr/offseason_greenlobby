{"amendement": {"@xmlns": "http://schemas.assemblee-nationale.fr/referentiel", "uid": "AMANR5L16PO419610B2436P0D1N003327", "chronotag": "1714767657843", "legislature": "16", "identification": {"numeroLong": "CE3327", "numeroOrdreDepot": "3327", "prefixeOrganeExamen": "CION-ECO", "numeroRect": "0"}, "examenRef": "EXANR5L16PO419610B2436P0D1", "texteLegislatifRef": "PRJLANR5L16B2436", "triAmendement": "faaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaezyi", "cardinaliteAmdtMultiples": "1", "amendementParentRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "signataires": {"auteur": {"typeAuteur": "D\u00e9put\u00e9", "gouvernementRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "acteurRef": "PA718930", "groupePolitiqueRef": "PO800538", "auteurRapporteurOrganeRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "cosignataires": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "suffixe": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "libelle": "Mme&#160;Petel"}, "pointeurFragmentTexte": {"partieAmendableRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "division": {"titre": "Article 10", "articleDesignationCourte": "ART. 10", "articleDesignation": "ARTICLE 10", "type": "ARTICLE", "avant_A_Apres": "A", "divisionRattachee": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "articleAdditionnel": "false", "chapitreAdditionnel": "false", "urlDivisionTexteVise": "/16/textes/2436.asp#D_Article_10"}, "amendementStandard": {"alinea": {"avant_A_Apres": "Apres", "numero": "10", "alineaDesignation": "Apr\u00e8s l'alin\u00e9a 10"}}}, "corps": {"cartoucheInformatif": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenuAuteur": {"dispositif": "<p style=\"text-align: justify;\">Apr&#x00E8;s l&#x2019;alin&#x00E9;a&nbsp;10, ins&#x00E9;rer un alin&#x00E9;a ainsi r&#x00E9;dig&#x00E9;&nbsp;:</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;Le r&#x00E9;seau&nbsp;''&nbsp;France services agriculture&nbsp;''&nbsp;s&#x2019;articule avec les autres outils d&#x00E9;ploy&#x00E9;s sur le territoire, notamment ceux &#x00E9;labor&#x00E9;s dans le cadre des projets alimentaires territoriaux d&#x00E9;finis &#x00E0; l&#x2019;article&nbsp;L.&nbsp;111&#x2011;2&#x2011;2.&nbsp;&#x00BB;</p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p style=\"text-align: justify;\">Cet amendement a &#x00E9;t&#x00E9; propos&#x00E9; par l'association France Urbaine.</p><p style=\"text-align: justify;\">La r&#x00E9;silience de nos syst&#x00E8;mes alimentaires et agricoles n&#x00E9;cessite de penser de mani&#x00E8;re territorialis&#x00E9;e les enjeux de diversification et d&#x00E9;sp&#x00E9;cialisation. Pour soutenir cette d&#x00E9;marche, plusieurs territoires ont d&#x00E9;ploy&#x00E9; des outils de prospective en vue de fixer des objectifs de transformation ou investi dans des outils de diagnostic partenarial.&nbsp; Cela est par exemple le cas du territoire de Rennes m&#x00E9;tropole avec la d&#x00E9;marche de conseil d&#x00E9;ploy&#x00E9;e par la SCIC \"Terres de Source\". La d&#x00E9;finition des d&#x00E9;marches de diagnostic au sein du projet de loi gagnerait ainsi &#x00E0; tirer profit des outils d&#x00E9;j&#x00E0; d&#x00E9;ploy&#x00E9;s en vue de s&#x2019;articuler avec les d&#x00E9;marches en cours.<br>Cet amendement vise donc &#x00E0; ce que le r&#x00E9;seau \"France services agriculture\" s&#x2019;articule le cas &#x00E9;ch&#x00E9;ant les autres outils d&#x00E9;ploy&#x00E9;s sur le territoire, notamment ceux &#x00E9;labor&#x00E9;s dans le cadre des projets alimentaires territoriaux.</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2024-04-26", "datePublication": "2024-04-30", "soumisArticle40": "false", "etatDesTraitements": {"etat": {"code": "DI", "libelle": "Discut\u00e9"}, "sousEtat": {"code": "DI", "libelle": "Tomb\u00e9"}}, "dateSort": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "sort": "Tomb\u00e9"}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L16PO419610B2436P0D1N003327?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "seanceDiscussionRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "article99": "false", "loiReference": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionCommune": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionIdentique": {"idDiscussion": "69497", "typePosition": "Dernier"}, "accordGouvernementDepotHorsDelai": "Sans objet"}}