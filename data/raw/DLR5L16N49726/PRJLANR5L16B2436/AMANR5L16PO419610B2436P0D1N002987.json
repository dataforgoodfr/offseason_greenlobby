{"amendement": {"@xmlns": "http://schemas.assemblee-nationale.fr/referentiel", "uid": "AMANR5L16PO419610B2436P0D1N002987", "chronotag": "1714635953427", "legislature": "16", "identification": {"numeroLong": "CE2987", "numeroOrdreDepot": "2987", "prefixeOrganeExamen": "CION-ECO", "numeroRect": "0"}, "examenRef": "EXANR5L16PO419610B2436P0D1", "texteLegislatifRef": "PRJLANR5L16B2436", "triAmendement": "aeaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaafzzzzvts", "cardinaliteAmdtMultiples": "1", "amendementParentRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "signataires": {"auteur": {"typeAuteur": "D\u00e9put\u00e9", "gouvernementRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "acteurRef": "PA793664", "groupePolitiqueRef": "PO800526", "auteurRapporteurOrganeRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "cosignataires": {"acteurRef": ["PA794008", "PA795454", "PA793780", "PA335999", "PA795362", "PA793980", "PA795010", "PA794022", "PA794146", "PA795636", "PA795794", "PA793708", "PA794154", "PA794778", "PA795076", "PA610002", "PA795808", "PA720952", "PA736201", "PA793948"]}, "suffixe": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "libelle": "Mme&#160;Pochon,  Mme&#160;Chatelain, M.&#160;Ben&#160;Cheikh, Mme&#160;Arrighi, Mme&#160;Batho, Mme&#160;Belluco, M.&#160;Fournier, Mme&#160;Garin, M.&#160;Iordanoff, Mme&#160;Laernoes, M.&#160;Lucas-Lundy, Mme&#160;Pasquini, M.&#160;Peytavie, M.&#160;Raux, Mme&#160;Regol, Mme&#160;Rousseau, Mme&#160;Sas, Mme&#160;Sebaihi, M.&#160;Tach&#233;, Mme&#160;Taill&#233;-Polian et M.&#160;Thierry"}, "pointeurFragmentTexte": {"partieAmendableRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "division": {"titre": "Article PREMIER", "articleDesignationCourte": "ART. PREMIER", "articleDesignation": "ARTICLE PREMIER", "type": "ARTICLE", "avant_A_Apres": "A", "divisionRattachee": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "articleAdditionnel": "false", "chapitreAdditionnel": "false", "urlDivisionTexteVise": "/16/textes/2436.asp#D_Article_1er"}, "amendementStandard": {"alinea": {"avant_A_Apres": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "numero": "5", "alineaDesignation": "Alin\u00e9a 5"}}}, "corps": {"cartoucheInformatif": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenuAuteur": {"dispositif": "<p style=\"text-align: justify;\">&#x00C0; l&#x2019;alin&#x00E9;a&nbsp;5, apr&#x00E8;s le mot&nbsp;:</p><p>&#x00AB;&nbsp;nutritive,&nbsp;&#x00BB;,&nbsp;</p><p>ins&#x00E9;rer les mots&nbsp;:</p><p>&#x00AB;&nbsp;reposant davantage sur les prot&#x00E9;ines v&#x00E9;g&#x00E9;tales,&nbsp;&#x00BB;.</p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p style=\"text-align: justify;\">Cet amendement int&#x00E8;gre la n&#x00E9;cessit&#x00E9; de d&#x00E9;velopper la consommation de prot&#x00E9;ines v&#x00E9;g&#x00E9;tales et les fili&#x00E8;res alimentaires des prot&#x00E9;ines v&#x00E9;g&#x00E9;tales, concourant ainsi aux objectifs fix&#x00E9;s dans la strat&#x00E9;gie nationale sur les prot&#x00E9;ines v&#x00E9;g&#x00E9;tales port&#x00E9;e par le gouvernement, permettant &#x00E0; la fois aux &#x00E9;leveurs d&#x2019;am&#x00E9;liorer leur autonomie pour l&#x2019;alimentation de leurs animaux, et &#x00E0; encourager les Fran&#x00E7;ais &#x00E0; augmenter leur consommation de prot&#x00E9;ines v&#x00E9;g&#x00E9;tales, pour r&#x00E9;pondre aux nouvelles recommandations nutritionnelles. En d&#x00E9;cembre 2020, le Ministre de l&#x2019;Agriculture de l&#x2019;&#x00E9;poque l&#x2019;affirmait : &#x201C;Ma priorite&#x0301; est claire : nous devons impe&#x0301;rativement regagner en souverainete&#x0301; agroalimentaire, et cela ne peut se faire sans le de&#x0301;veloppement d&#x2019;une production franc&#x0327;aise de prote&#x0301;ines ve&#x0301;ge&#x0301;tales.&#x201D; Il est ainsi n&#x00E9;cessaire de mentionner dans ce projet de loi cet imp&#x00E9;ratif. Tel est l&#x2019;objet de cet amendement.&nbsp;</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2024-04-26", "datePublication": "2024-04-30", "soumisArticle40": "false", "etatDesTraitements": {"etat": {"code": "DI", "libelle": "Discut\u00e9"}, "sousEtat": {"code": "DI", "libelle": "Rejet\u00e9"}}, "dateSort": "2024-05-02T09:45:32+02:00", "sort": "Rejet\u00e9"}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L16PO419610B2436P0D1N002987?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "seanceDiscussionRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "article99": "false", "loiReference": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionCommune": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionIdentique": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "accordGouvernementDepotHorsDelai": "Sans objet"}}