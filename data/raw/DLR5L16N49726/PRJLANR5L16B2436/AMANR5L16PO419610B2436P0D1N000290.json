xsi:nil": "true"}, "division": {"titre": "Article 12", "articleDesignationCourte": "ART. 12", "articleDesignation": "ARTICLE 12", "type": "ARTICLE", "avant_A_Apres": "A", "divisionRattachee": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "articleAdditionnel": "false", "chapitreAdditionnel": "false", "urlDivisionTexteVise": "/16/textes/2436.asp#D_Article_12"}, "amendementStandard": {"alinea": {"avant_A_Apres": "Apres", "numero": "15", "alineaDesignation": "Apr\u00e8s l'alin\u00e9a 15"}}}, "corps": {"cartoucheInformatif": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenuAuteur": {"dispositif": "<p style=\"text-align: justify;\">Apr&#x00E8;s l&#x2019;alin&#x00E9;a&nbsp;15, ins&#x00E9;rer les deux alin&#x00E9;as suivants&nbsp;:&nbsp;</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;II <i>bis</i>.&nbsp;&#x2013;&nbsp;Apr&#x00E8;s la premi&#x00E8;re phrase du premier alin&#x00E9;a de l&#x2019;article L.&nbsp;141&#x2011;1&#x2011;1 du code rural et de la p&#x00EA;che maritime est ins&#x00E9;r&#x00E9;e une phrase ainsi r&#x00E9;dig&#x00E9;e&nbsp;:&nbsp;</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;Cette obligation d&#x2019;information vaut pour les cessions de parts ou d&#x2019;actions de groupements fonciers agricoles d&#x2019;investissements.&nbsp;&#x00BB;.</p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p style=\"text-align: justify;\">La SAFER est un organisme de droit priv&#x00E9; charg&#x00E9; assure des missions de service public list&#x00E9;es &#x00E0; l&#x2019;article L 141-1 du code rural et de la p&#x00EA;che maritime. Pour ce faire, elle doit pouvoir disposer d&#x2019;informations lui permettant de jouer pleinement son r&#x00F4;le. C&#x2019;est &#x00E0; ce titre que la loi d&#x2019;avenir du 13 octobre 2014 a &#x00E9;largi le contr&#x00F4;le en dissociant le couplage d&#x2019;obligation de notification et de droit de pr&#x00E9;emption de la SAFER. Ainsi, la SAFER doit &#x00EA;tre inform&#x00E9;e d&#x2019;un ensemble d&#x2019;op&#x00E9;rations, qu&#x2019;elle dispose ou non de son droit de pr&#x00E9;emption. Au regard de l&#x2019;opacit&#x00E9; de certains aspects du GFAI notamment quant &#x00E0; son fonctionnement et sa gouvernance, la SAFER doit &#x00EA;tre inform&#x00E9;e des op&#x00E9;rations de cessions de parts et d&#x2019;actions dans le GFAI. Cet amendement vise &#x00E0; instaurer une obligation de notification &#x00E0; la SAFER de toutes les cessions de parts sociales du GFAI qu&#x2019;elles soient partielles ou totales.</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2024-04-17", "datePublication": "2024-04-30", "soumisArticle40": "false", "etatDesTraitements": {"etat": {"code": "DI", "libelle": "Discut\u00e9"}, "sousEtat": {"code": "DI", "libelle": "Tomb\u00e9"}}, "dateSort": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "sort": "Tomb\u00e9"}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L16PO419610B2436P0D1N000290?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "seanceDiscussionRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "article99": "false", "loiReference": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionCommune": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionIdentique": {"idDiscussion": "69480", "typePosition": "Premier"}, "accordGouvernementDepotHorsDelai": "Sans objet"}}