leAdditionnel": "false", "chapitreAdditionnel": "false", "urlDivisionTexteVise": "/16/textes/2436.asp#D_Article_2"}, "amendementStandard": {"alinea": {"avant_A_Apres": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "numero": "7", "alineaDesignation": "Alin\u00e9a 7"}}}, "corps": {"cartoucheInformatif": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenuAuteur": {"dispositif": "<p style=\"text-align: justify;\">Compl&#x00E9;ter la premi&#x00E8;re phrase de l&#x2019;alin&#x00E9;a&nbsp;7 par les mots&nbsp;:&nbsp;</p><p>&#x00AB;, en associant les professionnels des m&#x00E9;tiers concern&#x00E9;s. &#x00BB;</p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p style=\"text-align: justify;\">Cet amendement vise &#x00E0; modifier l'article 2 pour ins&#x00E9;rer apr&#x00E8;s le mot &#x00AB; vivant &#x00BB; les mots &#x00AB;, en associant les professionnels des m&#x00E9;tiers concern&#x00E9;s &#x00BB;. Cette insertion pr&#x00E9;cise la n&#x00E9;cessit&#x00E9; d'impliquer activement les professionnels des secteurs de l'agriculture, de l'agroalimentaire et des sciences du vivant dans la r&#x00E9;alisation du programme national d'orientation et de d&#x00E9;couverte de ces m&#x00E9;tiers. Ce programme, mis en place par l'&#x00C9;tat et les r&#x00E9;gions, a pour but de promouvoir ces carri&#x00E8;res et de susciter l'int&#x00E9;r&#x00EA;t des jeunes g&#x00E9;n&#x00E9;rations pour ces domaines.</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2024-04-26", "datePublication": "2024-04-30", "soumisArticle40": "false", "etatDesTraitements": {"etat": {"code": "DI", "libelle": "Discut\u00e9"}, "sousEtat": {"code": "DI", "libelle": "Tomb\u00e9"}}, "dateSort": "2024-05-02T22:59:07+02:00", "sort": "Tomb\u00e9"}, "representations": {"representation": {"nom": "