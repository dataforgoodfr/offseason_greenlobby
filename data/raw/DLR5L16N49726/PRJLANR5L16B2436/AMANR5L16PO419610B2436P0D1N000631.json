{"amendement": {"@xmlns": "http://schemas.assemblee-nationale.fr/referentiel", "uid": "AMANR5L16PO419610B2436P0D1N000631", "chronotag": "1714741913568", "legislature": "16", "identification": {"numeroLong": "CE631", "numeroOrdreDepot": "631", "prefixeOrganeExamen": "CION-ECO", "numeroRect": "0"}, "examenRef": "EXANR5L16PO419610B2436P0D1", "texteLegislatifRef": "PRJLANR5L16B2436", "triAmendement": "doaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaabbajt", "cardinaliteAmdtMultiples": "1", "amendementParentRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "signataires": {"auteur": {"typeAuteur": "D\u00e9put\u00e9", "gouvernementRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "acteurRef": "PA607090", "groupePolitiqueRef": "PO800508", "auteurRapporteurOrganeRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "cosignataires": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "suffixe": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "libelle": "M.&#160;Jean-Pierre&#160;Vigier"}, "pointeurFragmentTexte": {"partieAmendableRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "division": {"titre": "Article 8", "articleDesignationCourte": "ART. 8", "articleDesignation": "ARTICLE 8", "type": "ARTICLE", "avant_A_Apres": "A", "divisionRattachee": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "articleAdditionnel": "false", "chapitreAdditionnel": "false", "urlDivisionTexteVise": "/16/textes/2436.asp#D_Article_8"}, "amendementStandard": {"alinea": {"avant_A_Apres": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "numero": "1", "alineaDesignation": "Alin\u00e9a 1"}}}, "corps": {"cartoucheInformatif": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenuAuteur": {"dispositif": "<p style=\"text-align: justify;\">&#x00C0; l&#x2019;alin&#x00E9;a&nbsp;1, substituer &#x00E0; la date&nbsp;:</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;2035&nbsp;&#x00BB;,</p><p style=\"text-align: justify;\">la date&nbsp;:&nbsp;</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;2050&nbsp;&#x00BB;.</p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p style=\"text-align: justify;\">Les&nbsp;politiques d&#x2019;installation et de transmission efficaces ne sont possibles que si des moyens humains et financiers importants sont mobilis&#x00E9;s.&nbsp;Cette ambition pour le renouvellement des g&#x00E9;n&#x00E9;rations en agriculture se traduit par la cr&#x00E9;ation d&#x2019;un guichet unique (FSA), organis&#x00E9; pour l&#x2019;installation, la formation et la transmission, qui doit r&#x00E9;pondre &#x00E0; une vision de long-terme pour notre souverainet&#x00E9;.</p><p style=\"text-align: justify;\">Ainsi, cet amendement&nbsp;propose une politique vis&#x00E9;e pour l&#x2019;horizon 2050&nbsp;et non&nbsp;2035, une date qui&nbsp;semble&nbsp;excessivement courte au regard des enjeux de souverainet&#x00E9; alimentaire et des transitions &#x00E0; mener pour r&#x00E9;pondre aux multiples d&#x00E9;fis auxquels l&#x2019;agriculture fait face.</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2024-04-22", "datePublication": "2024-04-29", "soumisArticle40": "false", "etatDesTraitements": {"etat": {"code": "DI", "libelle": "Discut\u00e9"}, "sousEtat": {"code": "DI", "libelle": "Rejet\u00e9"}}, "dateSort": "2024-05-03T15:11:42+02:00", "sort": "Rejet\u00e9"}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L16PO419610B2436P0D1N000631?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "seanceDiscussionRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "article99": "false", "loiReference": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionCommune": {"idDiscussion": "24345", "typePosition": "Milieu"}, "discussionIdentique": {"idDiscussion": "69160", "typePosition": "Milieu"}, "accordGouvernementDepotHorsDelai": "Sans objet"}}