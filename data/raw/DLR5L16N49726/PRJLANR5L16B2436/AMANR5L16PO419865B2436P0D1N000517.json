{"amendement": {"@xmlns": "http://schemas.assemblee-nationale.fr/referentiel", "uid": "AMANR5L16PO419865B2436P0D1N000517", "chronotag": "1714421475413", "legislature": "16", "identification": {"numeroLong": "CD517", "numeroOrdreDepot": "517", "prefixeOrganeExamen": "CION-DVP", "numeroRect": "0"}, "examenRef": "EXANR5L16PO419865B2436P0D1", "texteLegislatifRef": "PRJLANR5L16B2436", "triAmendement": "auaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaax", "cardinaliteAmdtMultiples": "1", "amendementParentRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "signataires": {"auteur": {"typeAuteur": "D\u00e9put\u00e9", "gouvernementRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "acteurRef": "PA794946", "groupePolitiqueRef": "PO800520", "auteurRapporteurOrganeRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "cosignataires": {"acteurRef": ["PA793238", "PA793158", "PA720668", "PA793182", "PA794222", "PA793964", "PA793146", "PA795540", "PA794530", "PA793314", "PA793852"]}, "suffixe": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "libelle": "M.&#160;Villedieu,  M.&#160;Barth&#232;s, M.&#160;Beaurain, M.&#160;Blairy, M.&#160;Bovet, Mme&#160;Cousin, M.&#160;de&#160;Fournas, M.&#160;Dragon, M.&#160;Grenon, M.&#160;Marchio, Mme&#160;Alexandra&#160;Masson et M.&#160;Meurin"}, "pointeurFragmentTexte": {"partieAmendableRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "division": {"titre": "Article 2", "articleDesignationCourte": "APR\u00c8S ART. 2", "articleDesignation": "APR\u00c8S L'ARTICLE 2, ins\u00e9rer l'article suivant:", "type": "ARTICLE", "avant_A_Apres": "Apr\u00e8s", "divisionRattachee": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "articleAdditionnel": "true", "chapitreAdditionnel": "false", "urlDivisionTexteVise": "/16/textes/2436.asp#D_Article_2"}, "amendementStandard": {"alinea": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "corps": {"cartoucheInformatif": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenuAuteur": {"dispositif": "<p style=\"text-align: justify;\">Dans les six mois suivant la publication de la pr&#x00E9;sente loi, le Gouvernement pr&#x00E9;sente au Parlement un rapport sur la viabilit&#x00E9; technique et &#x00E9;conomique de mat&#x00E9;riels innovants, utilisant une &#x00E9;nergie d&#x00E9;carbon&#x00E9;e, au b&#x00E9;n&#x00E9;fice de tous les acteurs du secteur agricole.</p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p style=\"text-align: justify;\">Alors que le projet insiste sur la transition &#x00E9;cologique &#x00E0; travers la participation de l'agriculture &#x00E0; la d&#x00E9;carbonation de l'&#x00E9;conomie, les efforts demand&#x00E9;s aux agriculteurs ne s'accompagnent que tr&#x00E8;s partiellement de dispositifs destin&#x00E9;s &#x00E0; favoriser les transitions. Malgr&#x00E9; une &#x00E9;tude prospective r&#x00E9;alis&#x00E9;e par le Conseil G&#x00E9;n&#x00E9;ral de l'Alimentation de l'Agriculture et des Espaces Ruraux sur les possibilit&#x00E9;s de d&#x00E9;carbonation de l'&#x00E9;nergie utilis&#x00E9;e par les agriculteurs qui a identifi&#x00E9; plusieurs sc&#x00E9;narios, la viabilit&#x00E9; des nouveaux moyens et des technologies qui sont destin&#x00E9;s &#x00E0; favoriser cette transition &#x00E9;nerg&#x00E9;tique n'est toujours pas assur&#x00E9;e. Cet amendement demande au gouvernement de pr&#x00E9;senter au Parlement un rapport sur la viabilit&#x00E9; technique et &#x00E9;conomique de mat&#x00E9;riels innovants utilisant une &#x00E9;nergie&nbsp;d&#x00E9;carbon&#x00E9;e pour tous les acteurs du monde agricole.&nbsp;</p><p style=\"text-align: justify;\">Cet amendement a &#x00E9;t&#x00E9; travaill&#x00E9; en collaboration avec la Chambre d'agriculture de Haute-Sa&#x00F4;ne.</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2024-04-25", "datePublication": "2024-04-29", "soumisArticle40": "false", "etatDesTraitements": {"etat": {"code": "DI", "libelle": "Discut\u00e9"}, "sousEtat": {"code": "DI", "libelle": "Rejet\u00e9"}}, "dateSort": "2024-04-29T22:10:58+02:00", "sort": "Rejet\u00e9"}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L16PO419865B2436P0D1N000517?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "seanceDiscussionRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "article99": "false", "loiReference": {"codeLoi": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "divisionCodeLoi": "zrapport"}, "discussionCommune": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionIdentique": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "accordGouvernementDepotHorsDelai": "Sans objet"}}