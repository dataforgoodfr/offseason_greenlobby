{"amendement": {"@xmlns": "http://schemas.assemblee-nationale.fr/referentiel", "uid": "AMANR5L16PO419610B2436P0D1N001684", "chronotag": "1714767658070", "legislature": "16", "identification": {"numeroLong": "CE1684", "numeroOrdreDepot": "1684", "prefixeOrganeExamen": "CION-ECO", "numeroRect": "0"}, "examenRef": "EXANR5L16PO419610B2436P0D1", "texteLegislatifRef": "PRJLANR5L16B2436", "triAmendement": "faaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaafbl", "cardinaliteAmdtMultiples": "1", "amendementParentRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "signataires": {"auteur": {"typeAuteur": "D\u00e9put\u00e9", "gouvernementRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "acteurRef": "PA794410", "groupePolitiqueRef": "PO800508", "auteurRapporteurOrganeRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "cosignataires": {"acteurRef": ["PA608416", "PA642847", "PA721474", "PA722390", "PA342240", "PA793476", "PA718884", "PA793508", "PA793094"]}, "suffixe": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "libelle": "M.&#160;Seitlinger,  M.&#160;Hetzel, M.&#160;Bazin, M.&#160;Viry, Mme&#160;Corneloup, M.&#160;Bourgeaux, M.&#160;Dubois, Mme&#160;Bazin-Malgras, M.&#160;Brigand et M.&#160;Ray"}, "pointeurFragmentTexte": {"partieAmendableRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "division": {"titre": "Article 10", "articleDesignationCourte": "ART. 10", "articleDesignation": "ARTICLE 10", "type": "ARTICLE", "avant_A_Apres": "A", "divisionRattachee": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "articleAdditionnel": "false", "chapitreAdditionnel": "false", "urlDivisionTexteVise": "/16/textes/2436.asp#D_Article_10"}, "amendementStandard": {"alinea": {"avant_A_Apres": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "numero": "11", "alineaDesignation": "Alin\u00e9a 11"}}}, "corps": {"cartoucheInformatif": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenuAuteur": {"dispositif": "<p style=\"text-align: justify;\">Apr&#x00E8;s la premi&#x00E8;re phrase de&nbsp;l&#x2019;alin&#x00E9;a&nbsp;11, ins&#x00E9;rer la phrase suivante&nbsp;:</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;Les d&#x00E9;crets d&#x2019;application le concernant clarifient et simplifient l&#x2019;organisation actuelle, en priorisant d&#x2019;une part la mise en place&nbsp;&#x00BB;France services agricultures&#x00AB;&nbsp;comme une plateforme d&#x2019;orientation vers un interlocuteur&nbsp;unique, comme un m&#x00E9;diateur et un facilitateur des &#x00E9;changes entre les agriculteurs et les domaines dont il a la charge ainsi que comme un simplificateur des t&#x00E2;ches administratives que doivent remplir les agriculteurs&nbsp;&#x00BB;.&nbsp;</p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p style=\"text-align: justify;\">Le pr&#x00E9;sent amendement vise &#x00E0; pr&#x00E9;ciser les perspectives par lesquelles le d&#x00E9;cret d'application devra orienter le pilotage de \"France services agricultures\".&nbsp;</p><p style=\"text-align: justify;\">Les jeunes agriculteurs sont en lien avec de nombreux agriculteurs &#x00E0; l'&#x00E9;chelle locale, ce qui complexifie leur parcours d'installation ou de reprise.&nbsp;</p><p style=\"text-align: justify;\">La clarification et la simplification de l'organisation actuelle que devrait permettre \"France services agricultures\" enrayerait l'inflation administrative qui p&#x00E8;se lourdement sur les &#x00E9;paules des repreneurs.&nbsp;</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2024-04-25", "datePublication": "2024-04-29", "soumisArticle40": "false", "etatDesTraitements": {"etat": {"code": "DI", "libelle": "Discut\u00e9"}, "sousEtat": {"code": "DI", "libelle": "Tomb\u00e9"}}, "dateSort": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "sort": "Tomb\u00e9"}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L16PO419610B2436P0D1N001684?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "seanceDiscussionRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "article99": "false", "loiReference": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionCommune": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionIdentique": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "accordGouvernementDepotHorsDelai": "Sans objet"}}