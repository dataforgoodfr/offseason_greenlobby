{"amendement": {"@xmlns": "http://schemas.assemblee-nationale.fr/referentiel", "uid": "AMANR5L16PO419610B2436P0D1N001021", "chronotag": "1714681343532", "legislature": "16", "identification": {"numeroLong": "CE1021", "numeroOrdreDepot": "1021", "prefixeOrganeExamen": "CION-ECO", "numeroRect": "0"}, "examenRef": "EXANR5L16PO419610B2436P0D1", "texteLegislatifRef": "PRJLANR5L16B2436", "triAmendement": "asaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaabgi", "cardinaliteAmdtMultiples": "1", "amendementParentRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "signataires": {"auteur": {"typeAuteur": "D\u00e9put\u00e9", "gouvernementRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "acteurRef": "PA712015", "groupePolitiqueRef": "PO800508", "auteurRapporteurOrganeRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "cosignataires": {"acteurRef": ["PA793476", "PA718838", "PA266793", "PA605991", "PA330909", "PA795406", "PA718884", "PA342240", "PA720586", "PA794038", "PA793094", "PA721442", "PA607090", "PA794178", "PA721474", "PA719318", "PA794410", "PA608416", "PA719272", "PA721314", "PA793508", "PA1327", "PA722390", "PA718850", "PA719968", "PA608826", "PA266808"]}, "suffixe": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "libelle": "M.&#160;Dive,  M.&#160;Dubois, M.&#160;Fabrice&#160;Brun, M.&#160;Bony, Mme&#160;Genevard, M.&#160;Descoeur, Mme&#160;P&#233;rigault, Mme&#160;Bazin-Malgras, M.&#160;Bourgeaux, M.&#160;Vatin, M.&#160;Neuder, M.&#160;Ray, Mme&#160;Petex, M.&#160;Jean-Pierre&#160;Vigier, M.&#160;Taite, M.&#160;Viry, Mme&#160;Anthoine, M.&#160;Seitlinger, M.&#160;Hetzel, Mme&#160;Fr&#233;d&#233;rique&#160;Meunier, M.&#160;Schellenberger, M.&#160;Brigand, M.&#160;Forissier, Mme&#160;Corneloup, M.&#160;Cordier, Mme&#160;Bonnet, Mme&#160;Duby-Muller et M.&#160;Rolland"}, "pointeurFragmentTexte": {"partieAmendableRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "division": {"titre": "Article 2", "articleDesignationCourte": "ART. 2", "articleDesignation": "ARTICLE 2", "type": "ARTICLE", "avant_A_Apres": "A", "divisionRattachee": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "articleAdditionnel": "false", "chapitreAdditionnel": "false", "urlDivisionTexteVise": "/16/textes/2436.asp#D_Article_2"}, "amendementStandard": {"alinea": {"avant_A_Apres": "Apres", "numero": "6", "alineaDesignation": "Apr\u00e8s l'alin\u00e9a 6"}}}, "corps": {"cartoucheInformatif": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenuAuteur": {"dispositif": "<p style=\"text-align: justify;\">Apr&#x00E8;s l&#x2019;alin&#x00E9;a&nbsp;6, ins&#x00E9;rer l&#x2019;alin&#x00E9;a suivant&nbsp;:&nbsp;</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;5&#x00B0;&nbsp;De d&#x00E9;velopper des collaborations entre la recherche publique et les entreprises orient&#x00E9;es vers les besoins qui participent &#x00E0; la transition agro&#x00E9;cologique et climatique de l&#x2019;agriculture et de l&#x2019;alimentation.&nbsp;&#x00BB;</p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p style=\"text-align: justify;\">Ces collaborations offrent un potentiel consid&#x00E9;rable pour r&#x00E9;pondre aux d&#x00E9;fis complexes auxquels est confront&#x00E9; le secteur agricole. Les entreprises, en &#x00E9;tant directement impliqu&#x00E9;es dans les processus de recherche et d'innovation, peuvent apporter leur expertise, leurs ressources et leur connaissance pratique des r&#x00E9;alit&#x00E9;s du terrain. De m&#x00EA;me, la recherche publique d&#x00E9;tient un savoir-faire scientifique ainsi que des infrastructures de recherche pour le d&#x00E9;veloppement de solutions innovantes. Cet amendement vise donc &#x00E0; enrichir le dispositif l&#x00E9;gislatif en reconnaissant l'importance des collaborations entre la recherche publique et les entreprises dans la transition vers une agriculture plus durable et r&#x00E9;siliente. En soutenant et en encourageant ces partenariats, la capacit&#x00E9; collective &#x00E0; relever les d&#x00E9;fis futurs tout en ouvrant de nouvelles perspectives de d&#x00E9;veloppement &#x00E9;conomique et social est renforc&#x00E9;e.</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2024-04-24", "datePublication": "2024-04-29", "soumisArticle40": "false", "etatDesTraitements": {"etat": {"code": "DI", "libelle": "Discut\u00e9"}, "sousEtat": {"code": "DI", "libelle": "Adopt\u00e9"}}, "dateSort": "2024-05-02T22:22:20+02:00", "sort": "Adopt\u00e9"}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L16PO419610B2436P0D1N001021?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "seanceDiscussionRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "article99": "false", "loiReference": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionCommune": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionIdentique": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "accordGouvernementDepotHorsDelai": "Sans objet"}}