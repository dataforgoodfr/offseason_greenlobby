ntTexte": {"partieAmendableRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "division": {"titre": "Article 14", "articleDesignationCourte": "ART. 14", "articleDesignation": "ARTICLE 14", "type": "ARTICLE", "avant_A_Apres": "A", "divisionRattachee": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "articleAdditionnel": "false", "chapitreAdditionnel": "false", "urlDivisionTexteVise": "/16/textes/2436.asp#D_Article_14"}, "amendementStandard": {"alinea": {"avant_A_Apres": "Apres", "numero": "33", "alineaDesignation": "Apr\u00e8s l'alin\u00e9a 33"}}}, "corps": {"cartoucheInformatif": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenuAuteur": {"dispositif": "<p style=\"text-align: justify;\">Apr&#x00E8;s l&#x2019;alin&#x00E9;a&nbsp;33, ins&#x00E9;rer les deux alin&#x00E9;as suivants&nbsp;:&nbsp;</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;I <i>bis</i>.&nbsp;&#x2013;&nbsp;L&#x2019;article L.&nbsp;411&#x2011;1 du code de l&#x2019;environnement est compl&#x00E9;t&#x00E9; par un III ainsi r&#x00E9;dig&#x00E9;&nbsp;:</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;III.&nbsp;&#x2013;&nbsp;Pour l&#x2019;application du pr&#x00E9;sent article relatif &#x00E0; la taille des haies en espaces agricoles, la p&#x00E9;riode d&#x2019;interdiction de perturbation doit tenir compte des sp&#x00E9;cificit&#x00E9;s et des conditions climatiques et p&#x00E9;dologiques du d&#x00E9;partement.&nbsp;&#x00BB;</p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p style=\"text-align: justify;\">Chaque d&#x00E9;partement pr&#x00E9;sente des caract&#x00E9;ristiques g&#x00E9;ographiques, climatiques et p&#x00E9;dologiques uniques qui influent directement sur la croissance et la sant&#x00E9; des haies.&nbsp;En effet, certaines r&#x00E9;gions peuvent &#x00EA;tre soumises &#x00E0; des p&#x00E9;riodes de s&#x00E9;cheresse prolong&#x00E9;e, tandis que d'autres peuvent &#x00EA;tre confront&#x00E9;es &#x00E0; des pr&#x00E9;cipitations abondantes. De m&#x00EA;me, la composition du sol peut varier consid&#x00E9;rablement d'un territoire &#x00E0; l'autre, ce qui influe sur la capacit&#x00E9; des haies &#x00E0; se d&#x00E9;velopper et &#x00E0; remplir leurs fonctions &#x00E9;cologiques. Plut&#x00F4;t que d'imposer une approche uniforme &#x00E0; l'&#x00E9;chelle nationale, cet amendement&nbsp;reconna&#x00EE;t la n&#x00E9;cessit&#x00E9; d'ajuster les p&#x00E9;riodes d'interdiction de perturbation en fonction des conditions sp&#x00E9;cifiques &#x00E0; l'&#x00E9;chelle d&#x00E9;partementale.&nbsp;</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2024-04-24", "datePublication": "2024-05-01", "soumisArticle40": "false", "etatDesTraitements": {"etat": {"code": "DI", "libelle": "Discut\u00e9"}, "sousEtat": {"code": "DI", "libelle": "Adopt\u00e9"}}, "dateSort": "2024-05-04T12:40:55+02:00", "sort": "Adopt\u00e9"}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L16PO419610B2436P0D1N001025?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "seanceDiscussionRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "article99": "false", "loiReference": {"codeLoi": "c environnement", "divisionCodeLoi": "L411-1"}, "discussionCommune": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionIdentique": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "accordGouvernementDepotHorsDelai": "Sans objet"}}