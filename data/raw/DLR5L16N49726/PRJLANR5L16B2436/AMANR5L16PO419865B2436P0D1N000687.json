{"amendement": {"@xmlns": "http://schemas.assemblee-nationale.fr/referentiel", "uid": "AMANR5L16PO419865B2436P0D1N000687", "chronotag": "1714400745422", "legislature": "16", "identification": {"numeroLong": "CD687", "numeroOrdreDepot": "687", "prefixeOrganeExamen": "CION-DVP", "numeroRect": "0"}, "examenRef": "EXANR5L16PO419865B2436P0D1", "texteLegislatifRef": "PRJLANR5L16B2436", "triAmendement": "aeaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaabnzzzr", "cardinaliteAmdtMultiples": "1", "amendementParentRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "signataires": {"auteur": {"typeAuteur": "D\u00e9put\u00e9", "gouvernementRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "acteurRef": "PA795354", "groupePolitiqueRef": "PO800514", "auteurRapporteurOrganeRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "cosignataires": {"acteurRef": ["PA794830", "PA720038"]}, "suffixe": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "libelle": "Mme&#160;Vilgrain,  M.&#160;Ott et Mme&#160;Josso"}, "pointeurFragmentTexte": {"partieAmendableRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "division": {"titre": "Article PREMIER", "articleDesignationCourte": "ART. PREMIER", "articleDesignation": "ARTICLE PREMIER", "type": "ARTICLE", "avant_A_Apres": "A", "divisionRattachee": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "articleAdditionnel": "false", "chapitreAdditionnel": "false", "urlDivisionTexteVise": "/16/textes/2436.asp#D_Article_1er"}, "amendementStandard": {"alinea": {"avant_A_Apres": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "numero": "6", "alineaDesignation": "Alin\u00e9a 6"}}}, "corps": {"cartoucheInformatif": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenuAuteur": {"dispositif": "<p style=\"text-align: justify;\">&#x00C0; l&#x2019;alin&#x00E9;a&nbsp;6, apr&#x00E8;s le mot&nbsp;:</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;capacit&#x00E9;&nbsp;&#x00BB;,&nbsp;</p><p style=\"text-align: justify;\">ins&#x00E9;rer les mots&nbsp;:</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;&#x00E0; anticiper et s&#x2019;adapter aux cons&#x00E9;quences du changement climatique et &#x00E0; en att&#x00E9;nuer les effets ainsi qu&#x2019;&nbsp;&#x00BB;</p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p style=\"text-align: justify;\">Le pr&#x00E9;sent&nbsp;projet de loi&nbsp;entend mobiliser les piliers de l'orientation et de la formation ainsi que celui de l'installation et la transmission afin de pr&#x00E9;parer les futures g&#x00E9;n&#x00E9;rations. Il doit permettre &#x00E0; l'agriculture de r&#x00E9;pondre de mani&#x00E8;re imm&#x00E9;diate et pour l'avenir en r&#x00E9;alisant les transitions indispensables face au changement climatique.</p><p style=\"text-align: justify;\">L'article 1 qui vise &#x00E0; pr&#x00E9;ciser que les politiques publiques concourent &#x00E0; la protection de la souverainet&#x00E9; alimentaire en d&#x00E9;terminant les objectifs qu'elles doivent poursuivre et les actions qu'elles doivent mettre en oeuvre &#x00E0; cette fin. Le pr&#x00E9;sent amendement entend ainsi ajouter &#x00E0; l'alin&#x00E9;a 6 la&nbsp;pr&#x00E9;cision que les politiques publiques concourent &#x00E0; la protection de la souverainet&#x00E9; alimentaire de la France en veillant &#x00E0; pr&#x00E9;server et am&#x00E9;liorer sa capacit&#x00E9; &#x00E0; anticiper et s'adapter aux cons&#x00E9;quences du changement climatique et &#x00E0; en att&#x00E9;nuer le effets afin de r&#x00E9;pondre aux grandes orientations attendues dans ce projet de loi.&nbsp;</p><p style=\"text-align: justify;\">Tel est l'objet de cet amendement</p><p style=\"text-align: justify;\">&nbsp;</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2024-04-25", "datePublication": "2024-04-29", "soumisArticle40": "false", "etatDesTraitements": {"etat": {"code": "DI", "libelle": "Discut\u00e9"}, "sousEtat": {"code": "DI", "libelle": "Tomb\u00e9"}}, "dateSort": "2024-04-29T16:25:17+02:00", "sort": "Tomb\u00e9"}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L16PO419865B2436P0D1N000687?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "seanceDiscussionRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "article99": "false", "loiReference": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionCommune": {"idDiscussion": "24384", "typePosition": "Dernier"}, "discussionIdentique": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "accordGouvernementDepotHorsDelai": "Sans objet"}}