{"amendement": {"@xmlns": "http://schemas.assemblee-nationale.fr/referentiel", "uid": "AMANR5L16PO419610B2436P0D1N001832", "chronotag": "1714687193369", "legislature": "16", "identification": {"numeroLong": "CE1832", "numeroOrdreDepot": "1832", "prefixeOrganeExamen": "CION-ECO", "numeroRect": "0"}, "examenRef": "EXANR5L16PO419610B2436P0D1", "texteLegislatifRef": "PRJLANR5L16B2436", "triAmendement": "asaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaiqzzzzzzw", "cardinaliteAmdtMultiples": "1", "amendementParentRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "signataires": {"auteur": {"typeAuteur": "D\u00e9put\u00e9", "gouvernementRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "acteurRef": "PA793816", "groupePolitiqueRef": "PO830170", "auteurRapporteurOrganeRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "cosignataires": {"acteurRef": ["PA607553", "PA643192", "PA794854", "PA333285", "PA344201"]}, "suffixe": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "libelle": "Mme&#160;Thomin,  M.&#160;Potier, Mme&#160;Jourdan, M.&#160;Echaniz, M.&#160;Garot et M.&#160;Bertrand&#160;Petit"}, "pointeurFragmentTexte": {"partieAmendableRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "division": {"titre": "Article 2", "articleDesignationCourte": "ART. 2", "articleDesignation": "ARTICLE 2", "type": "ARTICLE", "avant_A_Apres": "A", "divisionRattachee": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "articleAdditionnel": "false", "chapitreAdditionnel": "false", "urlDivisionTexteVise": "/16/textes/2436.asp#D_Article_2"}, "amendementStandard": {"alinea": {"avant_A_Apres": "Apres", "numero": "9", "alineaDesignation": "Apr\u00e8s l'alin\u00e9a 9"}}}, "corps": {"cartoucheInformatif": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenuAuteur": {"dispositif": "<p style=\"text-align: justify;\">Compl&#x00E9;ter cet article par l'alin&#x00E9;a suivant&nbsp;:&nbsp;</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;Les politiques d&#x2019;orientation et de formation en mati&#x00E8;re agricole contribuent &#x00E0; la meilleure ma&#x00EE;trise des outils, des logiciels et des dispositifs de gestion administrative des exploitations agricoles chez les futurs agriculteurs en formation.&nbsp;&#x00BB;</p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p style=\"text-align: justify;\">Cet amendement du groupe Socialistes et apparent&#x00E9;s vise &#x00E0; inclure dans les politiques d&#x2019;orientation et de formation en mati&#x00E8;re agricole l'enseignement aux futurs agriculteurs de la meilleure ma&#x00EE;trise des outils, des logiciels et des dispositifs de gestion administrative des exploitations agricoles.</p><p>Dans un contexte de complexification administrative et technique de la profession, une parfaite ma&#x00EE;trise des outils informatiques constitue un r&#x00E9;el avantage pour les futurs agriculteurs pour optimiser leurs t&#x00E2;ches administratives.</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2024-04-26", "datePublication": "2024-04-29", "soumisArticle40": "false", "etatDesTraitements": {"etat": {"code": "DI", "libelle": "Discut\u00e9"}, "sousEtat": {"code": "DI", "libelle": "Rejet\u00e9"}}, "dateSort": "2024-05-02T23:59:39+02:00", "sort": "Rejet\u00e9"}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L16PO419610B2436P0D1N001832?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "seanceDiscussionRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "article99": "false", "loiReference": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionCommune": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionIdentique": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "accordGouvernementDepotHorsDelai": "Sans objet"}}