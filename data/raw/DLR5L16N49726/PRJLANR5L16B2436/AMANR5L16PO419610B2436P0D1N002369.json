{"amendement": {"@xmlns": "http://schemas.assemblee-nationale.fr/referentiel", "uid": "AMANR5L16PO419610B2436P0D1N002369", "chronotag": "1714640873426", "legislature": "16", "identification": {"numeroLong": "CE2369", "numeroOrdreDepot": "2369", "prefixeOrganeExamen": "CION-ECO", "numeroRect": "0"}, "examenRef": "EXANR5L16PO419610B2436P0D1", "texteLegislatifRef": "PRJLANR5L16B2436", "triAmendement": "aeaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaafzzzzxzzzys", "cardinaliteAmdtMultiples": "1", "amendementParentRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "signataires": {"auteur": {"typeAuteur": "D\u00e9put\u00e9", "gouvernementRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "acteurRef": "PA267306", "groupePolitiqueRef": "PO800502", "auteurRapporteurOrganeRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "cosignataires": {"acteurRef": ["PA722202", "PA720838", "PA796106", "PA795868", "PA795192", "PA718926", "PA721896", "PA795998", "PA795184", "PA774962", "PA335612", "PA796010", "PA793174", "PA795820", "PA721270", "PA795240", "PA795876", "PA720692", "PA605745", "PA794650", "PA720992"]}, "suffixe": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "libelle": "M.&#160;Chassaigne,  M.&#160;Jumel, Mme&#160;Bourouaha, M.&#160;B&#233;nard, M.&#160;Castor, M.&#160;Chailloux, M.&#160;Dharr&#233;ville, Mme&#160;Faucillon, Mme&#160;K/Bidi, M.&#160;Le&#160;Gayic, Mme&#160;Lebon, M.&#160;Lecoq, M.&#160;Maillot, M.&#160;Monnet, M.&#160;Nadeau, M.&#160;Peu, Mme&#160;Reid&#160;Arbelot, M.&#160;Rimane, M.&#160;Roussel, M.&#160;Sansu, M.&#160;Tellier et M.&#160;William"}, "pointeurFragmentTexte": {"partieAmendableRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "division": {"titre": "Article PREMIER", "articleDesignationCourte": "ART. PREMIER", "articleDesignation": "ARTICLE PREMIER", "type": "ARTICLE", "avant_A_Apres": "A", "divisionRattachee": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "articleAdditionnel": "false", "chapitreAdditionnel": "false", "urlDivisionTexteVise": "/16/textes/2436.asp#D_Article_1er"}, "amendementStandard": {"alinea": {"avant_A_Apres": "Apres", "numero": "7", "alineaDesignation": "Apr\u00e8s l'alin\u00e9a 7"}}}, "corps": {"cartoucheInformatif": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenuAuteur": {"dispositif": "<p style=\"text-align: justify;\">Apr&#x00E8;s l&#x2019;alin&#x00E9;a&nbsp;7, ins&#x00E9;rer l&#x2019;alin&#x00E9;a suivant&nbsp;:</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;&#x2013;&nbsp;la r&#x00E9;ponse aux besoins alimentaires nationaux et communautaires, en favorisant la diversification de la production, la proximit&#x00E9; et le soutien aux fili&#x00E8;res d&#x00E9;ficitaires.&nbsp;&#x00BB;</p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p style=\"text-align: justify;\">Le projet de loi introduit dans le code rural un nouvel article L.&nbsp;1 A propre aux orientations des politiques publiques associ&#x00E9;es &#x00E0; la protection de la souverainet&#x00E9; alimentaire. La r&#x00E9;daction propos&#x00E9;e est toutefois d&#x00E9;faillante et ne fait en particulier nulle r&#x00E9;f&#x00E9;rence aux implications commerciales, aux droits et aux revenus des agriculteurs. Le pr&#x00E9;sent amendement tend en cons&#x00E9;quence &#x00E0; pr&#x00E9;ciser que la souverainet&#x00E9; alimentaire ne peut &#x00EA;tre conquise sans une diversification de la production, le d&#x00E9;veloppement d&#x2019;une agriculture de proximit&#x00E9; et cons&#x00E9;quemment un soutien accru aux fili&#x00E8;res aujourd&#x2019;hui d&#x00E9;ficitaires.</p><p style=\"text-align: justify;\">Cet amendement s&#x2019;inspire des propositions du collectif Nourrir.</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2024-04-26", "datePublication": "2024-04-30", "soumisArticle40": "false", "etatDesTraitements": {"etat": {"code": "DI", "libelle": "Discut\u00e9"}, "sousEtat": {"code": "DI", "libelle": "Non soutenu"}}, "dateSort": "2024-05-02T11:07:28+02:00", "sort": "Non soutenu"}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L16PO419610B2436P0D1N002369?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "seanceDiscussionRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "article99": "false", "loiReference": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionCommune": {"idDiscussion": "24447", "typePosition": "Dernier"}, "discussionIdentique": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "accordGouvernementDepotHorsDelai": "Sans objet"}}