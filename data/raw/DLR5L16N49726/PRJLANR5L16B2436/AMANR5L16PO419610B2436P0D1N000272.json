{"amendement": {"@xmlns": "http://schemas.assemblee-nationale.fr/referentiel", "uid": "AMANR5L16PO419610B2436P0D1N000272", "chronotag": "1714666223389", "legislature": "16", "identification": {"numeroLong": "CE272", "numeroOrdreDepot": "272", "prefixeOrganeExamen": "CION-ECO", "numeroRect": "0"}, "examenRef": "EXANR5L16PO419610B2436P0D1", "texteLegislatifRef": "PRJLANR5L16B2436", "triAmendement": "aeaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaafzzzzznzzzzzzwi", "cardinaliteAmdtMultiples": "1", "amendementParentRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "signataires": {"auteur": {"typeAuteur": "D\u00e9put\u00e9", "gouvernementRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "acteurRef": "PA712015", "groupePolitiqueRef": "PO800508", "auteurRapporteurOrganeRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "cosignataires": {"acteurRef": ["PA718838", "PA605991", "PA793476", "PA266793", "PA1327", "PA795406", "PA342240", "PA720586", "PA794038", "PA793094", "PA721442", "PA607090", "PA794178", "PA719318", "PA794410", "PA608416", "PA719272", "PA721314", "PA722390", "PA346054", "PA718850", "PA719968", "PA608826", "PA266808"]}, "suffixe": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "libelle": "M.&#160;Dive,  M.&#160;Fabrice&#160;Brun, Mme&#160;Genevard, M.&#160;Dubois, M.&#160;Bony, M.&#160;Forissier, Mme&#160;P&#233;rigault, M.&#160;Bourgeaux, M.&#160;Vatin, M.&#160;Neuder, M.&#160;Ray, Mme&#160;Petex, M.&#160;Jean-Pierre&#160;Vigier, M.&#160;Taite, Mme&#160;Anthoine, M.&#160;Seitlinger, M.&#160;Hetzel, Mme&#160;Fr&#233;d&#233;rique&#160;Meunier, M.&#160;Schellenberger, Mme&#160;Corneloup, M.&#160;Emmanuel&#160;Maquet, M.&#160;Cordier, Mme&#160;Bonnet, Mme&#160;Duby-Muller et M.&#160;Rolland"}, "pointeurFragmentTexte": {"partieAmendableRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "division": {"titre": "Article PREMIER", "articleDesignationCourte": "ART. PREMIER", "articleDesignation": "ARTICLE PREMIER", "type": "ARTICLE", "avant_A_Apres": "A", "divisionRattachee": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "articleAdditionnel": "false", "chapitreAdditionnel": "false", "urlDivisionTexteVise": "/16/textes/2436.asp#D_Article_1er"}, "amendementStandard": {"alinea": {"avant_A_Apres": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "numero": "19", "alineaDesignation": "Alin\u00e9a 19"}}}, "corps": {"cartoucheInformatif": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenuAuteur": {"dispositif": "<p style=\"text-align: justify;\">&#x00C0; l'alin&#x00E9;a 19, apr&#x00E8;s le mot :&nbsp;</p><p style=\"text-align: justify;\">&#x00AB; essai &#x00BB;,&nbsp;</p><p>ins&#x00E9;rer les mots :&nbsp;</p><p>&#x00AB; d&#x00E9;fini au pr&#x00E9;sent code &#x00BB;.</p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p style=\"text-align: justify;\">L&#x2019;article 1er modifie substantiellement l&#x2019;article L. 1 du code rural et de la p&#x00EA;che maritime. Ce dernier tiendra mieux compte des sujets soci&#x00E9;taires en mati&#x00E8;re d&#x2019;installation et de transmission, en introduisant le &#x00AB; droit &#x00E0; l&#x2019;essai &#x00BB; dans cette politique, qui est une innovation importante, unanimement port&#x00E9;e par les organisations professionnelles agricoles. L&#x2019;ajout de cette seule mention appara&#x00EE;t toutefois insuffisant, dans la mesure o&#x00F9; ces derni&#x00E8;res ont identifi&#x00E9; le besoin de fonder le droit &#x00E0; l&#x2019;essai sur un socle l&#x00E9;gislatif n&#x00E9;cessairement plus d&#x00E9;taill&#x00E9;. L&#x2019;ajout pr&#x00E9;vu par le pr&#x00E9;sent amendement constitue donc un compl&#x00E9;ment utile, en ce qu&#x2019;il appelle &#x00E0; d&#x00E9;finir la nature exacte du dispositif dans le code rural, et, par cons&#x00E9;quent, dans la pr&#x00E9;sente loi.</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2024-04-17", "datePublication": "2024-04-30", "soumisArticle40": "false", "etatDesTraitements": {"etat": {"code": "DI", "libelle": "Discut\u00e9"}, "sousEtat": {"code": "DI", "libelle": "Rejet\u00e9"}}, "dateSort": "2024-05-02T18:10:10+02:00", "sort": "Rejet\u00e9"}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L16PO419610B2436P0D1N000272?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "seanceDiscussionRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "article99": "false", "loiReference": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionCommune": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionIdentique": {"idDiscussion": "69153", "typePosition": "Premier"}, "accordGouvernementDepotHorsDelai": "Sans objet"}}