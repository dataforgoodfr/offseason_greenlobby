{"amendement": {"@xmlns": "http://schemas.assemblee-nationale.fr/referentiel", "uid": "AMANR5L16PO419610B2436P0D1N002651", "chronotag": "1714767657881", "legislature": "16", "identification": {"numeroLong": "CE2651", "numeroOrdreDepot": "2651", "prefixeOrganeExamen": "CION-ECO", "numeroRect": "0"}, "examenRef": "EXANR5L16PO419610B2436P0D1", "texteLegislatifRef": "PRJLANR5L16B2436", "triAmendement": "faaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaezyk", "cardinaliteAmdtMultiples": "1", "amendementParentRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "signataires": {"auteur": {"typeAuteur": "D\u00e9put\u00e9", "gouvernementRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "acteurRef": "PA719032", "groupePolitiqueRef": "PO800514", "auteurRapporteurOrganeRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "cosignataires": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "suffixe": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "libelle": "M.&#160;Bouyx"}, "pointeurFragmentTexte": {"partieAmendableRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "division": {"titre": "Article 10", "articleDesignationCourte": "ART. 10", "articleDesignation": "ARTICLE 10", "type": "ARTICLE", "avant_A_Apres": "A", "divisionRattachee": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "articleAdditionnel": "false", "chapitreAdditionnel": "false", "urlDivisionTexteVise": "/16/textes/2436.asp#D_Article_10"}, "amendementStandard": {"alinea": {"avant_A_Apres": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "numero": "11", "alineaDesignation": "Alin\u00e9a 11"}}}, "corps": {"cartoucheInformatif": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenuAuteur": {"dispositif": "<p style=\"text-align: justify;\">&#x00C0; l&#x2019;alin&#x00E9;a&nbsp;11, substituer aux mots&nbsp;:&nbsp;</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;les &#x00E9;tablissements publics locaux d&#x2019;enseignement et de formation professionnelle agricole&nbsp;&#x00BB;</p><p style=\"text-align: justify;\">les mots&nbsp;:</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;l&#x2019;un des &#x00E9;tablissements d&#x2019;enseignement et de formation professionnelle agricole d&#x00E9;finis aux articles L.&nbsp;811&#x2011;1 et L.&nbsp;813&#x2011;1&nbsp;&#x00BB;.</p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p style=\"text-align: justify;\">Cet amendement vise &#x00E0; laisser le choix au porteur de projet de l&#x2019;&#x00E9;tablissement dans lequel il veut suivre sa formation professionnelle.</p><p style=\"text-align: justify;\">L&#x2019;agriculteur ayant besoin de suivre une formation avant de s&#x2019;installer doit pouvoir se former dans l&#x2019;&#x00E9;tablissement de son choix, public ou priv&#x00E9;. Cela laissera un plus grand panel de choix pour l&#x2019;&#x00E9;tudiant qui pourra choisir un &#x00E9;tablissement correspondant au mieux &#x00E0; son projet professionnel. La libert&#x00E9; de choix laiss&#x00E9;e &#x00E0; l&#x2019;&#x00E9;tudiant peut encourager les &#x00E9;tablissements &#x00E0; se sp&#x00E9;cialiser dans certains domaines et ainsi mieux correspondre aux projets sp&#x00E9;cifiques de ceux-ci. Les &#x00E9;tablissements priv&#x00E9;s sous contrat ont toute l&#x00E9;gitimit&#x00E9; &#x00E0; &#x00EA;tre r&#x00E9;f&#x00E9;rents et garants de la m&#x00E9;thode dans l&#x2019;accompagnement du parcours de formation du porteur de projet tout comme un &#x00E9;tablissement public.</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2024-04-26", "datePublication": "2024-04-30", "soumisArticle40": "false", "etatDesTraitements": {"etat": {"code": "DI", "libelle": "Discut\u00e9"}, "sousEtat": {"code": "DI", "libelle": "Tomb\u00e9"}}, "dateSort": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "sort": "Tomb\u00e9"}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L16PO419610B2436P0D1N002651?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "seanceDiscussionRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "article99": "false", "loiReference": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionCommune": {"idDiscussion": "24581", "typePosition": "Premier"}, "discussionIdentique": {"idDiscussion": "69498", "typePosition": "Premier"}, "accordGouvernementDepotHorsDelai": "Sans objet"}}