ignationCourte": "ART. 6", "articleDesignation": "ARTICLE 6", "type": "ARTICLE", "avant_A_Apres": "A", "divisionRattachee": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "articleAdditionnel": "false", "chapitreAdditionnel": "false", "urlDivisionTexteVise": "/16/textes/2600.asp#D_Article_6"}, "amendementStandard": {"alinea": {"avant_A_Apres": "A", "numero": "5", "alineaDesignation": "Alin\u00e9a 5"}}}, "corps": {"cartoucheInformatif": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenuAuteur": {"dispositif": "<p style=\"text-align: justify;\">&#x00C0; la premi&#x00E8;re phrase de l&#x2019;alin&#x00E9;a 5, supprimer les mots :</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;transition agro&#x00E9;cologique et climatique&nbsp;et de &#x00BB;.</p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p style=\"text-align: justify;\">Cet amendement de repli tend &#x00E0; ce que &#x00AB;&nbsp;les plans prioritaires pluriannuels&nbsp;&#x00BB; pr&#x00E9;vus par l&#x2019;article soient &#x00E9;labor&#x00E9;s dans un objectif de souverainet&#x00E9; et non de &#x00AB;&nbsp;transition agro&#x00E9;cologique et climatique&nbsp;&#x00BB;.<br>&nbsp;<br>Si la pr&#x00E9;servation des espaces naturels est essentielle, l&#x2019;objectif de cette loi est de r&#x00E9;pondre aux demandes du monde agricole, parmi lesquelles une meilleure protection contre la concurrence d&#x00E9;loyale &#x00E9;trang&#x00E8;re et le poids des normes. L&#x2019;exemple de l&#x2019;immobilier a d&#x00E9;montr&#x00E9; &#x00E0; quel point l&#x2019;imposition d&#x2019;objectifs &#x00E9;cologiques abstraits, peu clairs et n&#x00E9;anmoins obligatoires avait fortement p&#x00E9;nalis&#x00E9; le secteur.&nbsp;</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2024-05-10", "datePublication": "2024-05-16", "soumisArticle40": "false", "etatDesTraitements": {"etat": {"code": "DI", "libelle": "Discut\u00e9"}, "sousEtat": {"code": "DI", "libelle": "Rejet\u00e9"}}, "dateSort": "2024-05-22T17:30:09+02:00", "sort": "Rejet\u00e9"}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L16PO791932BTC2600P0D1N003262?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "seanceDiscussionRef": "RUANR5L16S2024IDS28367", "article99": "false", "loiReference": {"codeLoi": "1", "divisionCodeLoi": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "discussionCommune": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionIdentique": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "accordGouvernementDepotHorsDelai": "Sans objet"}}