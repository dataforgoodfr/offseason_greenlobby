{"amendement": {"@xmlns": "http://schemas.assemblee-nationale.fr/referentiel", "uid": "AMANR5L16PO791932BTC2600P0D1N003979", "chronotag": "1716389276729", "legislature": "16", "identification": {"numeroLong": "3979", "numeroOrdreDepot": "3979", "prefixeOrganeExamen": "AN", "numeroRect": "0"}, "examenRef": "EXANR5L16PO791932BTC2600P0D1", "texteLegislatifRef": "PRJLANR5L16BTC2600", "triAmendement": "buaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaxr", "cardinaliteAmdtMultiples": "1", "amendementParentRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "signataires": {"auteur": {"typeAuteur": "D\u00e9put\u00e9", "gouvernementRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "acteurRef": "PA793796", "groupePolitiqueRef": "PO800532", "auteurRapporteurOrganeRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "cosignataires": {"acteurRef": ["PA605069", "PA719286", "PA719146", "PA721046", "PA719138", "PA267766", "PA942", "PA720696", "PA267042", "PA822617", "PA795244", "PA720976", "PA607619", "PA266788", "PA721486", "PA333421", "PA793210", "PA794626", "PA2952"]}, "suffixe": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "libelle": "M.&#160;Taupiac,  M.&#160;Jean-Louis&#160;Bricout, M.&#160;Colombani, M.&#160;Acquaviva, Mme&#160;Bassire, M.&#160;Castellani, M.&#160;Guy&#160;Bricout, M.&#160;de&#160;Courson, Mme&#160;Descamps, M.&#160;Favennec-B&#233;cot&#160;, Mme&#160;Froger, M.&#160;Lenormand, M.&#160;Mathiasin, M.&#160;Molac, M.&#160;Morel-&#192;-L&apos;Huissier, M.&#160;Naegelen, M.&#160;Pancher, M.&#160;Panifous, M.&#160;Saint-Huile et M.&#160;Warsmann"}, "pointeurFragmentTexte": {"partieAmendableRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "division": {"titre": "Article 5", "articleDesignationCourte": "ART. 5", "articleDesignation": "ARTICLE 5", "type": "ARTICLE", "avant_A_Apres": "A", "divisionRattachee": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "articleAdditionnel": "false", "chapitreAdditionnel": "false", "urlDivisionTexteVise": "/16/textes/2600.asp#D_Article_5"}, "amendementStandard": {"alinea": {"avant_A_Apres": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "numero": "6", "alineaDesignation": "Alin\u00e9a 6"}}}, "corps": {"cartoucheInformatif": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenuAuteur": {"dispositif": "<p style=\"text-align: justify;\">Compl&#x00E9;ter&nbsp;l&#x2019;alin&#x00E9;a&nbsp;6&nbsp;la phrase suivante&nbsp;:</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;Le ministre charg&#x00E9; de l&#x2019;agriculture veille&nbsp;&#x00E0; ce que le maillage territorial des &#x00E9;tablissements&nbsp;dispensant des formations de l&#x2019;enseignement sup&#x00E9;rieur conduisant &#x00E0; un dipl&#x00F4;me national de premier cycle en sciences et techniques de l&#x2019;agronomie apporte une r&#x00E9;ponse de proximit&#x00E9; aux besoins en mati&#x00E8;re de formation.&nbsp;&#x00BB;</p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p style=\"text-align: justify;\">Cet amendement vise &#x00E0; garantir la mise en place d'une offre de formation au plus pr&#x00E8;s des besoins des territoires. Il pr&#x00E9;voit &#x00E0; ce titre que le Ministre de l'Agriculture veille, dans la proc&#x00E9;dure d'accr&#x00E9;ditation des &#x00E9;tablissements de formation, au maillage territoriale. Il permet ainsi que les &#x00E9;l&#x00E8;ves souhaitant &#x00E9;tudier en vue&nbsp;du dipl&#x00F4;me national de premier cycle en sciences et techniques de l'agronomie puissent b&#x00E9;n&#x00E9;ficier d'une formation de proximit&#x00E9;.&nbsp;</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2024-05-10", "datePublication": "2024-05-17", "soumisArticle40": "false", "etatDesTraitements": {"etat": {"code": "DI", "libelle": "Discut\u00e9"}, "sousEtat": {"code": "DI", "libelle": "Adopt\u00e9"}}, "dateSort": "2024-05-22T16:47:56+02:00", "sort": "Adopt\u00e9"}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L16PO791932BTC2600P0D1N003979?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "seanceDiscussionRef": "RUANR5L16S2024IDS28367", "article99": "false", "loiReference": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionCommune": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionIdentique": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "accordGouvernementDepotHorsDelai": "Sans objet"}}