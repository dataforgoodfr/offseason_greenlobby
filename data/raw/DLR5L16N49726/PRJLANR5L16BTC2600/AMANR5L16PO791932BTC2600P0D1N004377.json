{"amendement": {"@xmlns": "http://schemas.assemblee-nationale.fr/referentiel", "uid": "AMANR5L16PO791932BTC2600P0D1N004377", "chronotag": "1715981151237", "legislature": "16", "identification": {"numeroLong": "4377", "numeroOrdreDepot": "4377", "prefixeOrganeExamen": "AN", "numeroRect": "0"}, "examenRef": "EXANR5L16PO791932BTC2600P0D1", "texteLegislatifRef": "PRJLANR5L16BTC2600", "triAmendement": "biaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaag", "cardinaliteAmdtMultiples": "1", "amendementParentRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "signataires": {"auteur": {"typeAuteur": "D\u00e9put\u00e9", "gouvernementRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "acteurRef": "PA794154", "groupePolitiqueRef": "PO800526", "auteurRapporteurOrganeRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "cosignataires": {"acteurRef": ["PA793664", "PA793780", "PA795362", "PA795454", "PA794008", "PA793980", "PA795010", "PA794022", "PA794146", "PA795636", "PA795794", "PA793708", "PA794778", "PA795076", "PA610002", "PA795808", "PA720952", "PA736201", "PA793948"]}, "suffixe": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "libelle": "M.&#160;Raux,  Mme&#160;Pochon, Mme&#160;Arrighi, Mme&#160;Belluco, M.&#160;Ben&#160;Cheikh, Mme&#160;Chatelain, M.&#160;Fournier, Mme&#160;Garin, M.&#160;Iordanoff, Mme&#160;Laernoes, M.&#160;Lucas-Lundy, Mme&#160;Pasquini, M.&#160;Peytavie, Mme&#160;Regol, Mme&#160;Rousseau, Mme&#160;Sas, Mme&#160;Sebaihi, M.&#160;Tach&#233;, Mme&#160;Taill&#233;-Polian et M.&#160;Thierry"}, "pointeurFragmentTexte": {"partieAmendableRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "division": {"titre": "Article 3", "articleDesignationCourte": "ART. 3", "articleDesignation": "ARTICLE 3", "type": "ARTICLE", "avant_A_Apres": "A", "divisionRattachee": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "articleAdditionnel": "false", "chapitreAdditionnel": "false", "urlDivisionTexteVise": "/16/textes/2600.asp#D_Article_3"}, "amendementStandard": {"alinea": {"avant_A_Apres": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "numero": "6", "alineaDesignation": "Alin\u00e9a 6"}}}, "corps": {"cartoucheInformatif": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenuAuteur": {"dispositif": "<p style=\"text-align: justify;\">I.&nbsp;&#x2013;&nbsp;&#x00C0; la premi&#x00E8;re phrase de l&#x2019;alin&#x00E9;a&nbsp;6, apr&#x00E8;s le mot&nbsp;:</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;alimentaire,&nbsp;&#x00BB;,&nbsp;</p><p style=\"text-align: justify;\">ins&#x00E9;rer les mots&nbsp;:</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;d&#x2019;am&#x00E9;lioration de la r&#x00E9;mun&#x00E9;ration,&nbsp;&#x00BB;.</p><p style=\"text-align: justify;\">II.&nbsp;&#x2013;&nbsp;En cons&#x00E9;quence, &#x00E0; la m&#x00EA;me phrase du m&#x00EA;me alin&#x00E9;a, supprimer les mots&nbsp;:</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;de la diversit&#x00E9;&nbsp;&#x00BB;.&nbsp;</p><p style=\"text-align: justify;\">III.&nbsp;&#x2013;&nbsp;En cons&#x00E9;quence, &#x00E0; ladite phrase dudit alin&#x00E9;a, substituer au mot&nbsp;:</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;agricoles&nbsp;&#x00BB;,&nbsp;</p><p style=\"text-align: justify;\">les mots&nbsp;:</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;agro&#x00E9;cologiques, dont l&#x2019;agriculture biologique, de d&#x00E9;sp&#x00E9;cialisation des territoires, d&#x2019;autonomie de gestion, de diversification et de valorisation de la production,&nbsp;&#x00BB;.&nbsp;</p><p style=\"text-align: justify;\">IV.&nbsp;&#x2013;&nbsp;En cons&#x00E9;quence, compl&#x00E9;ter le m&#x00EA;me alin&#x00E9;a par la phrase suivante&nbsp;:</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;Ils promeuvent le partenariat permettant de nouer des liens entre les &#x00E9;tablissements scolaires g&#x00E9;n&#x00E9;raux et agricoles, l&#x2019;&#x00C9;tat, les r&#x00E9;gions et les autres collectivit&#x00E9;s territoriales et leurs groupements.&nbsp;&#x00BB;</p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p style=\"text-align: justify;\">Cet amendement du groupe &#x00C9;cologiste &#x2013; NUPES vise &#x00E0; renforcer les enjeux de d&#x00E9;veloppement des fili&#x00E8;res de production et de transformation agricole en :&nbsp;</p><ul><li style=\"text-align: justify;\">promouvant les syst&#x00E8;mes de production agro&#x00E9;cologiques ;&nbsp;</li><li style=\"text-align: justify;\">renfor&#x00E7;ant&nbsp;la promotion de l&#x2019;agriculture biologique au sein de l&#x2019;enseignement et de la formation professionnelle aux m&#x00E9;tiers de l&#x2019;agriculture, de la for&#x00EA;t, de la nature et des territoires ;</li><li style=\"text-align: justify;\">int&#x00E9;grant&nbsp;les dimensions de d&#x00E9;sp&#x00E9;cialisation des territoires, l&#x2019;autonomie de gestion, la diversification et la valorisation de la production ;</li><li style=\"text-align: justify;\">concourant&nbsp;&#x00E0; l&#x2019;atteinte d&#x2019;une r&#x00E9;mun&#x00E9;ration juste pour les agriculteurs et agricultrices ;</li><li style=\"text-align: justify;\">promouvant&nbsp;le partenariat entre les &#x00E9;tablissements scolaires g&#x00E9;n&#x00E9;raux et agricoles, l&#x2019;&#x00C9;tat et les collectivit&#x00E9;s territoriales ou leurs groupements.</li></ul>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2024-05-10", "datePublication": "2024-05-15", "soumisArticle40": "true", "etatDesTraitements": {"etat": {"code": "DI", "libelle": "Discut\u00e9"}, "sousEtat": {"code": "DI", "libelle": "Rejet\u00e9"}}, "dateSort": "2024-05-17T23:25:49+02:00", "sort": "Rejet\u00e9"}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L16PO791932BTC2600P0D1N004377?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "seanceDiscussionRef": "RUANR5L16S2024IDS28364", "article99": "false", "loiReference": {"codeLoi": "1", "divisionCodeLoi": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "discussionCommune": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionIdentique": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "accordGouvernementDepotHorsDelai": "Sans objet"}}