{"amendement": {"@xmlns": "http://schemas.assemblee-nationale.fr/referentiel", "uid": "AMANR5L16PO791932BTC2600P0D1N003843", "chronotag": "1716535585260", "legislature": "16", "identification": {"numeroLong": "3843", "numeroOrdreDepot": "3843", "prefixeOrganeExamen": "AN", "numeroRect": "0"}, "examenRef": "EXANR5L16PO791932BTC2600P0D1", "texteLegislatifRef": "PRJLANR5L16BTC2600", "triAmendement": "esaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaadji", "cardinaliteAmdtMultiples": "1", "amendementParentRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "signataires": {"auteur": {"typeAuteur": "D\u00e9put\u00e9", "gouvernementRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "acteurRef": "PA793664", "groupePolitiqueRef": "PO800526", "auteurRapporteurOrganeRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "cosignataires": {"acteurRef": ["PA793780", "PA795362", "PA795454", "PA794008", "PA793980", "PA795010", "PA794022", "PA794146", "PA795636", "PA795794", "PA793708", "PA794154", "PA794778", "PA795076", "PA610002", "PA795808", "PA720952", "PA736201", "PA793948"]}, "suffixe": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "libelle": "Mme&#160;Pochon,  Mme&#160;Arrighi, Mme&#160;Belluco, M.&#160;Ben&#160;Cheikh, Mme&#160;Chatelain, M.&#160;Fournier, Mme&#160;Garin, M.&#160;Iordanoff, Mme&#160;Laernoes, M.&#160;Lucas-Lundy, Mme&#160;Pasquini, M.&#160;Peytavie, M.&#160;Raux, Mme&#160;Regol, Mme&#160;Rousseau, Mme&#160;Sas, Mme&#160;Sebaihi, M.&#160;Tach&#233;, Mme&#160;Taill&#233;-Polian et M.&#160;Thierry"}, "pointeurFragmentTexte": {"partieAmendableRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "division": {"titre": "Article 10", "articleDesignationCourte": "ART. 10", "articleDesignation": "ARTICLE 10", "type": "ARTICLE", "avant_A_Apres": "A", "divisionRattachee": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "articleAdditionnel": "false", "chapitreAdditionnel": "false", "urlDivisionTexteVise": "/16/textes/2600.asp#D_Article_10"}, "amendementStandard": {"alinea": {"avant_A_Apres": "Apres", "numero": "24", "alineaDesignation": "Apr\u00e8s l'alin\u00e9a 24"}}}, "corps": {"cartoucheInformatif": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenuAuteur": {"dispositif": "<p style=\"text-align: justify;\">Apr&#x00E8;s l&#x2019;alin&#x00E9;a&nbsp;24, ins&#x00E9;rer l&#x2019;alin&#x00E9;a suivant&nbsp;:</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;Des temps d&#x2019;&#x00E9;change collectifs, dont certains en non-mixit&#x00E9;, sont propos&#x00E9;s aux porteurs de projets dans le cadre de l&#x2019;accompagnement de leur projet par France Services Agriculture&nbsp;&#x00BB;.&nbsp;</p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p style=\"text-align: justify;\">L&#x2019;objet de cet amendement est d&#x2019;instaurer des temps d&#x2019;&#x00E9;change collectif entre les porteurs de projets. Ces temps d&#x2019;&#x00E9;change permettront aux futur.es install&#x00E9;.es d&#x2019;am&#x00E9;liorer leur projet en le confrontant &#x00E0; celui des autres.&nbsp; Cela permet &#x00E9;galement de cr&#x00E9;er des liens entre futurs responsables d&#x2019;entreprises agricoles. Par ailleurs, les femmes devant faire face &#x00E0; des obstacles en raison de leur genre, des temps d&#x2019;&#x00E9;change en non-mixit&#x00E9; seront &#x00E9;galement propos&#x00E9;s dans le but de favoriser les partages d&#x2019;exp&#x00E9;rience entre femmes et faciliter ainsi leur installation.&nbsp;</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2024-05-10", "datePublication": "2024-05-17", "soumisArticle40": "false", "etatDesTraitements": {"etat": {"code": "DI", "libelle": "Discut\u00e9"}, "sousEtat": {"code": "DI", "libelle": "Rejet\u00e9"}}, "dateSort": "2024-05-24T09:26:00+02:00", "sort": "Rejet\u00e9"}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L16PO791932BTC2600P0D1N003843?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "seanceDiscussionRef": "RUANR5L16S2024IDS28372", "article99": "false", "loiReference": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionCommune": {"idDiscussion": "25075", "typePosition": "Dernier"}, "discussionIdentique": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "accordGouvernementDepotHorsDelai": "Sans objet"}}