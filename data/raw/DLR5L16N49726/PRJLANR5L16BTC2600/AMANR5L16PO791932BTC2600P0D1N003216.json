{"amendement": {"@xmlns": "http://schemas.assemblee-nationale.fr/referentiel", "uid": "AMANR5L16PO791932BTC2600P0D1N003216", "chronotag": "1715929881714", "legislature": "16", "identification": {"numeroLong": "3216", "numeroOrdreDepot": "3216", "prefixeOrganeExamen": "AN", "numeroRect": "0"}, "examenRef": "EXANR5L16PO791932BTC2600P0D1", "texteLegislatifRef": "PRJLANR5L16BTC2600", "triAmendement": "adaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaanzu", "cardinaliteAmdtMultiples": "1", "amendementParentRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "signataires": {"auteur": {"typeAuteur": "D\u00e9put\u00e9", "gouvernementRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "acteurRef": "PA719608", "groupePolitiqueRef": "PO793087", "auteurRapporteurOrganeRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "cosignataires": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "suffixe": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "libelle": "Mme&#160;M&#233;nard"}, "pointeurFragmentTexte": {"partieAmendableRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "division": {"titre": "Article PREMIER", "articleDesignationCourte": "APR\u00c8S ART. PREMIER", "articleDesignation": "APR\u00c8S L'ARTICLE PREMIER, ins\u00e9rer l'article suivant:", "type": "ARTICLE", "avant_A_Apres": "Apr\u00e8s", "divisionRattachee": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "articleAdditionnel": "true", "chapitreAdditionnel": "false", "urlDivisionTexteVise": "/16/textes/2600.asp#D_Article_1er"}, "amendementStandard": {"alinea": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "corps": {"cartoucheInformatif": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenuAuteur": {"dispositif": "<p style=\"text-align: justify;\">L&#x2019;article L.&nbsp;211&#x2011;1&#x2011;1 du code de l&#x2019;environnement est ainsi modifi&#x00E9;&nbsp;:</p><p style=\"text-align: justify;\">1&#x00B0;&nbsp;&#x00C0; la deuxi&#x00E8;me phrase,&nbsp;les mots&nbsp;: &#x00AB;&nbsp;une agriculture,&nbsp;&#x00BB; sont supprim&#x00E9;s&nbsp;;</p><p style=\"text-align: justify;\">2&#x00B0;&nbsp;Apr&#x00E8;s la deuxi&#x00E8;me phrase, est ins&#x00E9;r&#x00E9;e une phrase ainsi r&#x00E9;dig&#x00E9;e&nbsp;: &#x00AB;&nbsp;Toute atteinte port&#x00E9;e &#x00E0; la protection de l&#x2019;agriculture, qui est d&#x2019;int&#x00E9;r&#x00EA;t g&#x00E9;n&#x00E9;ral en application de l&#x2019;article L.&nbsp;1 du code rural et de la p&#x00EA;che maritime, doit &#x00EA;tre n&#x00E9;cessaire et proportionn&#x00E9;e&nbsp;&#x00BB;.&nbsp;</p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p style=\"text-align: justify;\">Amendement propos&#x00E9; par la FNSEA.<br>&nbsp;<br>Au-del&#x00E0; de la reconnaissance de l&#x2019;int&#x00E9;r&#x00EA;t g&#x00E9;n&#x00E9;ral comme principe fondateur du droit et des politiques publiques en faveur de la protection, du d&#x00E9;ploiement et du d&#x00E9;veloppement de l&#x2019;agriculture, des modifications appropri&#x00E9;es du Code de l&#x2019;Environnement sont n&#x00E9;cessaires.<br>L&#x2019;objet de cet amendement est de pr&#x00E9;ciser que toute atteinte port&#x00E9;e &#x00E0; l&#x2019;agriculture dans le cadre de la pr&#x00E9;servation et de la gestion durable des zones humides doit &#x00EA;tre n&#x00E9;cessaire et proportionn&#x00E9;e &#x00E0; sa protection qui est &#x00E9;galement d&#x2019;int&#x00E9;r&#x00EA;t g&#x00E9;n&#x00E9;ral.</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2024-05-10", "datePublication": "2024-05-13", "soumisArticle40": "false", "etatDesTraitements": {"etat": {"code": "DI", "libelle": "Discut\u00e9"}, "sousEtat": {"code": "DI", "libelle": "Non soutenu"}}, "dateSort": "2024-05-17T09:11:13+02:00", "sort": "Non soutenu"}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L16PO791932BTC2600P0D1N003216?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "seanceDiscussionRef": "RUANR5L16S2024IDS28362", "article99": "false", "loiReference": {"codeLoi": "c environnement", "divisionCodeLoi": "L211-1-1"}, "discussionCommune": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionIdentique": {"idDiscussion": "69806", "typePosition": "Milieu"}, "accordGouvernementDepotHorsDelai": "Sans objet"}}