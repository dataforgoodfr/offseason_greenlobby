leDesignation": "ARTICLE 14", "type": "ARTICLE", "avant_A_Apres": "A", "divisionRattachee": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "articleAdditionnel": "false", "chapitreAdditionnel": "false", "urlDivisionTexteVise": "/16/textes/2600.asp#D_Article_14"}, "amendementStandard": {"alinea": {"avant_A_Apres": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "numero": "14", "alineaDesignation": "Alin\u00e9a 14"}}}, "corps": {"cartoucheInformatif": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenuAuteur": {"dispositif": "<p style=\"text-align: justify;\">&#x00C0; l&#x2019;alin&#x00E9;a&nbsp;14,&nbsp;substituer aux mots&nbsp;:&nbsp;</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;des d&#x00E9;cisions &#x00E9;num&#x00E9;r&#x00E9;es &#x00E0;&nbsp;&#x00BB;&nbsp;</p><p style=\"text-align: justify;\">les mots&nbsp;:&nbsp;</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;de la d&#x00E9;cision mentionn&#x00E9;e au 1&#x00B0;&nbsp;de&nbsp;&#x00BB;</p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p style=\"text-align: justify;\">Cet amendement a pour but de limiter la consultation publique dans le cadre de la r&#x00E8;glementation &#x201C;esp&#x00E8;ces prot&#x00E9;g&#x00E9;es&#x201D;. Le m&#x00E9;canisme de d&#x00E9;placement des haies ne relevant ni du r&#x00E9;gime ICPE ni du r&#x00E9;gime IOTA, comme rappel&#x00E9; par le Conseil d&#x2019;&#x00C9;tat dans son avis sur le projet de loi, il n&#x2019;y pas lieu de calquer la consultation publique obligatoire relevant de ces m&#x00EA;mes r&#x00E9;gimes pour l&#x2019;ensemble des r&#x00E8;glementations vis&#x00E9;es par le nouvel article L.412-24.&nbsp;Ainsi, dans la liste des r&#x00E8;glementations prises en compte dans cette autorisation unique, seulement la d&#x00E9;rogation esp&#x00E8;ces prot&#x00E9;g&#x00E9;es n&#x00E9;cessite une consultation publique.&nbsp; &nbsp;</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2024-05-06", "datePublication": "2024-05-17", "soumisArticle40": "false", "etatDesTraitements": {"etat": {"code": "DI", "libelle": "Discut\u00e9"}, "sousEtat": {"code": "DI", "libelle": "Non soutenu"}}, "dateSort": "2024-05-24T19:21:09+02:00", "sort": "Non soutenu"}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L16PO791932BTC2600P0D1N000028?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "seanceDiscussionRef": "RUANR5L16S2024IDS28373", "article99": "false", "loiReference": {"codeLoi": "1", "divisionCodeLoi": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "discussionCommune": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionIdentique": {"idDiscussion": "69843", "typePosition": "Premier"}, "accordGouvernementDepotHorsDelai": "Sans objet"}}