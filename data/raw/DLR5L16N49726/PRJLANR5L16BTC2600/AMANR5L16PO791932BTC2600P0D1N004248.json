{"amendement": {"@xmlns": "http://schemas.assemblee-nationale.fr/referentiel", "uid": "AMANR5L16PO791932BTC2600P0D1N004248", "chronotag": "1715898628304", "legislature": "16", "identification": {"numeroLong": "4248", "numeroOrdreDepot": "4248", "prefixeOrganeExamen": "AN", "numeroRect": "0"}, "examenRef": "EXANR5L16PO791932BTC2600P0D1", "texteLegislatifRef": "PRJLANR5L16BTC2600", "triAmendement": "acaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaamgdo", "cardinaliteAmdtMultiples": "1", "amendementParentRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "signataires": {"auteur": {"typeAuteur": "D\u00e9put\u00e9", "gouvernementRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "acteurRef": "PA794598", "groupePolitiqueRef": "PO800520", "auteurRapporteurOrganeRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "cosignataires": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "suffixe": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "libelle": "M.&#160;Jacobelli"}, "pointeurFragmentTexte": {"partieAmendableRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "division": {"titre": "Article PREMIER", "articleDesignationCourte": "ART. PREMIER", "articleDesignation": "ARTICLE PREMIER", "type": "ARTICLE", "avant_A_Apres": "A", "divisionRattachee": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "articleAdditionnel": "false", "chapitreAdditionnel": "false", "urlDivisionTexteVise": "/16/textes/2600.asp#D_Article_1er"}, "amendementStandard": {"alinea": {"avant_A_Apres": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "numero": "7", "alineaDesignation": "Alin\u00e9a 7"}}}, "corps": {"cartoucheInformatif": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenuAuteur": {"dispositif": "<p style=\"text-align: justify;\">&#x00C0; l&#x2019;alin&#x00E9;a&nbsp;7, supprimer les mots&nbsp;:</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;en valorisant les solutions fond&#x00E9;es sur la nature, compte tenu de la trajectoire du r&#x00E9;chauffement de r&#x00E9;f&#x00E9;rence,&nbsp;&#x00BB;</p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p style=\"text-align: justify;\">La n&#x00E9;cessaire adaptation de notre agriculture aux ph&#x00E9;nom&#x00E8;nes climatiques extr&#x00EA;mes doit se faire de mani&#x00E8;re pragmatique, par &#x00E9;tape et ne doit pas &#x00EA;tre soumise &#x00E0; une logique id&#x00E9;ologue. Notre priorit&#x00E9; doit &#x00EA;tre la protection de notre souverainet&#x00E9; alimentaire. La pr&#x00E9;servation de la nature est un objectif essentiel, de m&#x00EA;me que d'inscrire notre agriculture dans un plus grand respect des processus naturels, mais ne doit pas &#x00EA;tre exclusif de solutions fond&#x00E9;es sur la recherche scientifique, ni faire obstacle &#x00E0; la souverainet&#x00E9; alimentaire.</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2024-05-10", "datePublication": "2024-05-13", "soumisArticle40": "false", "etatDesTraitements": {"etat": {"code": "DI", "libelle": "Discut\u00e9"}, "sousEtat": {"code": "DI", "libelle": "Tomb\u00e9"}}, "dateSort": "2024-05-17T00:28:23+02:00", "sort": "Tomb\u00e9"}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "