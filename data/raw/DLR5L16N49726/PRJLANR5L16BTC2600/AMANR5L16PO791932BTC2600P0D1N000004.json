{"amendement": {"@xmlns": "http://schemas.assemblee-nationale.fr/referentiel", "uid": "AMANR5L16PO791932BTC2600P0D1N000004", "chronotag": "1715898891873", "legislature": "16", "identification": {"numeroLong": "4", "numeroOrdreDepot": "4", "prefixeOrganeExamen": "AN", "numeroRect": "0"}, "examenRef": "EXANR5L16PO791932BTC2600P0D1", "texteLegislatifRef": "PRJLANR5L16BTC2600", "triAmendement": "adaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaapg", "cardinaliteAmdtMultiples": "1", "amendementParentRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "signataires": {"auteur": {"typeAuteur": "D\u00e9put\u00e9", "gouvernementRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "acteurRef": "PA712015", "groupePolitiqueRef": "PO800508", "auteurRapporteurOrganeRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "cosignataires": {"acteurRef": ["PA605991", "PA720630", "PA720644", "PA720100", "PA342240", "PA1327", "PA722308", "PA608416", "PA794038", "PA721474", "PA718850", "PA719968", "PA794058", "PA332523", "PA721314", "PA718784", "PA794178", "PA721816", "PA721442", "PA783457", "PA330240", "PA794118"]}, "suffixe": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "libelle": "M.&#160;Dive,  Mme&#160;Genevard, M.&#160;Minot, M.&#160;Nury, M.&#160;Pradi&#233;, M.&#160;Bourgeaux, M.&#160;Forissier, Mme&#160;Serre, M.&#160;Hetzel, M.&#160;Neuder, M.&#160;Viry, M.&#160;Cordier, Mme&#160;Bonnet, Mme&#160;Gruet, Mme&#160;Dalloz, M.&#160;Schellenberger, M.&#160;Pauget, M.&#160;Taite, M.&#160;Boucard, Mme&#160;Petex, M.&#160;Habert-Dassault, M.&#160;Ciotti et M.&#160;Vermorel-Marques"}, "pointeurFragmentTexte": {"partieAmendableRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "division": {"titre": "Article PREMIER", "articleDesignationCourte": "APR\u00c8S ART. PREMIER", "articleDesignation": "APR\u00c8S L'ARTICLE PREMIER, ins\u00e9rer l'article suivant:", "type": "ARTICLE", "avant_A_Apres": "Apr\u00e8s", "divisionRattachee": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "articleAdditionnel": "true", "chapitreAdditionnel": "false", "urlDivisionTexteVise": "/16/textes/2600.asp#D_Article_1er"}, "amendementStandard": {"alinea": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "corps": {"cartoucheInformatif": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenuAuteur": {"dispositif": "<p style=\"text-align: justify;\">Apr&#x00E8;s le 12&#x00B0;&nbsp;du I de l&#x2019;article L.&nbsp;1 du code rural et de la p&#x00EA;che maritime, il est ins&#x00E9;r&#x00E9; un 12&#x00B0;&nbsp;<i>bis</i> ainsi r&#x00E9;dig&#x00E9;&nbsp;:</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;12&#x00B0;&nbsp;<i>bis</i> De pr&#x00E9;server et d&#x2019;am&#x00E9;liorer la souverainet&#x00E9; agricole du pays, li&#x00E9;e &#x00E0; la production durable de biomasse sur le territoire et &#x00E0; la contribution du secteur &#x00E0; la d&#x00E9;carbonation de l&#x2019;&#x00E9;conomie via notamment ses productions &#x00E9;nerg&#x00E9;tiques actuelles et en d&#x00E9;veloppement&nbsp;;&nbsp;&#x00BB;</p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p style=\"text-align: justify;\">La protection de la souverainet&#x00E9; agricole de notre pays vise avant tout &#x00E0; prot&#x00E9;ger notre souverainet&#x00E9; alimentaire mais &#x00E9;galement la contribution de l&#x2019;agriculture dans la production d&#x2019;&#x00E9;nergie renouvelable : Biocarburants de 1&#x00E8;re g&#x00E9;n&#x00E9;ration et avanc&#x00E9;s, biogaz, &#x00E9;lectricit&#x00E9; via la cog&#x00E9;n&#x00E9;ration. Ces productions contribuent significativement au mix-&#x00E9;nerg&#x00E9;tique national et permettent de produire &#x00E9;galement des mati&#x00E8;res riches en prot&#x00E9;ines (tourteaux, dr&#x00E8;ches, &#x2026;) &#x00E0; destination de l&#x2019;&#x00E9;levage et limitent notre d&#x00E9;pendance aux importations.</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2024-05-06", "datePublication": "2024-05-13", "soumisArticle40": "false", "etatDesTraitements": {"etat": {"code": "DI", "libelle": "Discut\u00e9"}, "sousEtat": {"code": "DI", "libelle": "Tomb\u00e9"}}, "dateSort": "2024-05-17T00:34:44+02:00", "sort": "Tomb\u00e9"}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L16PO791932BTC2600P0D1N000004?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "seanceDiscussionRef": "RUANR5L16S2024IDS28361", "article99": "false", "loiReference": {"codeLoi": "c rural", "divisionCodeLoi": "L1"}, "discussionCommune": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionIdentique": {"idDiscussion": "69828", "typePosition": "Premier"}, "accordGouvernementDepotHorsDelai": "Sans objet"}}