{"amendement": {"@xmlns": "http://schemas.assemblee-nationale.fr/referentiel", "uid": "AMANR5L16PO791932BTC2600P0D1N005103", "chronotag": "1715897751079", "legislature": "16", "identification": {"numeroLong": "5103", "numeroOrdreDepot": "5103", "prefixeOrganeExamen": "AN", "numeroRect": "0"}, "examenRef": "EXANR5L16PO791932BTC2600P0D1", "texteLegislatifRef": "PRJLANR5L16BTC2600", "triAmendement": "acaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaabzlnzxchm", "cardinaliteAmdtMultiples": "1", "amendementParentRef": "AMANR5L16PO791932BTC2600P0D1N003952", "signataires": {"auteur": {"typeAuteur": "D\u00e9put\u00e9", "gouvernementRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "acteurRef": "PA793664", "groupePolitiqueRef": "PO800526", "auteurRapporteurOrganeRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "cosignataires": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "suffixe": "et les membres du groupe \u00c9cologiste - NUPES", "libelle": "Mme&#160;Pochon et les membres du groupe &#201;cologiste - NUPES"}, "pointeurFragmentTexte": {"partieAmendableRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "division": {"titre": "Article PREMIER", "articleDesignationCourte": "ART. PREMIER", "articleDesignation": "ARTICLE PREMIER", "type": "ARTICLE", "avant_A_Apres": "A", "divisionRattachee": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "articleAdditionnel": "false", "chapitreAdditionnel": "false", "urlDivisionTexteVise": "/16/textes/2600.asp#D_Article_1er"}, "amendementStandard": {"alinea": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "corps": {"cartoucheInformatif": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenuAuteur": {"dispositif": "<p style=\"text-align: justify;\">R&#x00E9;diger ainsi l&#x2019;alin&#x00E9;a&nbsp;38&nbsp;:&nbsp;</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;18&#x00B0;&nbsp;De r&#x00E9;&#x00E9;quilibrer les termes des &#x00E9;changes entre pays dans un cadre europ&#x00E9;en et de coop&#x00E9;ration internationale, en se fondant sur les principes du commerce &#x00E9;quitable et le respect de la souverainet&#x00E9; alimentaire des pays tiers, en coh&#x00E9;rence avec les orientations de la politique fran&#x00E7;aise de d&#x00E9;veloppement et de solidarit&#x00E9; internationale telles que d&#x00E9;finies &#x00E0; l&#x2019;article premier de la loi n&#x00B0;&nbsp;2021&#x2011;1031 du 4 ao&#x00FB;t 2021 de programmation relative au d&#x00E9;veloppement solidaire et &#x00E0; la lutte contre les in&#x00E9;galit&#x00E9;s mondiales et de ses engagements internationaux relatifs aux droits humains et &#x00E0; l&#x2019;environnement&nbsp;;&nbsp;&#x00BB;.</p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p style=\"text-align: justify;\">Ce sous-amendement vise &#x00E0; pr&#x00E9;ciser que le r&#x00E9;&#x00E9;quilibrage des &#x00E9;changes doit se faire dans le sens du juste &#x00E9;change en se fondant sur les principes du commerce &#x00E9;quitable et en respectant la souverainet&#x00E9; alimentaire des pays tiers.&nbsp;</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2024-05-15", "datePublication": "2024-05-15", "soumisArticle40": "false", "etatDesTraitements": {"etat": {"code": "DI", "libelle": "Discut\u00e9"}, "sousEtat": {"code": "DI", "libelle": "Rejet\u00e9"}}, "dateSort": "2024-05-17T00:15:43+02:00", "sort": "Rejet\u00e9"}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L16PO791932BTC2600P0D1N005103?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "seanceDiscussionRef": "RUANR5L16S2024IDS28361", "article99": "false", "loiReference": {"codeLoi": "38", "divisionCodeLoi": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "discussionCommune": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionIdentique": {"idDiscussion": "70595", "typePosition": "Premier"}, "accordGouvernementDepotHorsDelai": "Sans objet"}}