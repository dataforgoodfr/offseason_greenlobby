{"amendement": {"@xmlns": "http://schemas.assemblee-nationale.fr/referentiel", "uid": "AMANR5L16PO791932BTC2600P0D1N000016", "chronotag": "1716388136662", "legislature": "16", "identification": {"numeroLong": "16", "numeroOrdreDepot": "16", "prefixeOrganeExamen": "AN", "numeroRect": "0"}, "examenRef": "EXANR5L16PO791932BTC2600P0D1", "texteLegislatifRef": "PRJLANR5L16BTC2600", "triAmendement": "buaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaxqyoai", "cardinaliteAmdtMultiples": "1", "amendementParentRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "signataires": {"auteur": {"typeAuteur": "D\u00e9put\u00e9", "gouvernementRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "acteurRef": "PA712015", "groupePolitiqueRef": "PO800508", "auteurRapporteurOrganeRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "cosignataires": {"acteurRef": ["PA720644", "PA720100", "PA342240", "PA722308", "PA608416", "PA794038", "PA721474", "PA718850", "PA719968", "PA794058", "PA332523", "PA721314", "PA718784", "PA794178", "PA721816", "PA721442", "PA783457", "PA330240", "PA794118"]}, "suffixe": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "libelle": "M.&#160;Dive,  M.&#160;Nury, M.&#160;Pradi&#233;, M.&#160;Bourgeaux, Mme&#160;Serre, M.&#160;Hetzel, M.&#160;Neuder, M.&#160;Viry, M.&#160;Cordier, Mme&#160;Bonnet, Mme&#160;Gruet, Mme&#160;Dalloz, M.&#160;Schellenberger, M.&#160;Pauget, M.&#160;Taite, M.&#160;Boucard, Mme&#160;Petex, M.&#160;Habert-Dassault, M.&#160;Ciotti et M.&#160;Vermorel-Marques"}, "pointeurFragmentTexte": {"partieAmendableRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "division": {"titre": "Article 5", "articleDesignationCourte": "ART. 5", "articleDesignation": "ARTICLE 5", "type": "ARTICLE", "avant_A_Apres": "A", "divisionRattachee": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "articleAdditionnel": "false", "chapitreAdditionnel": "false", "urlDivisionTexteVise": "/16/textes/2600.asp#D_Article_5"}, "amendementStandard": {"alinea": {"avant_A_Apres": "Apres", "numero": "5", "alineaDesignation": "Apr\u00e8s l'alin\u00e9a 5"}}}, "corps": {"cartoucheInformatif": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenuAuteur": {"dispositif": "<p style=\"text-align: justify;\">Apr&#x00E8;s l&#x2019;alin&#x00E9;a&nbsp;5, ins&#x00E9;rer l&#x2019;alin&#x00E9;a suivant&nbsp;:</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;Le r&#x00E9;f&#x00E9;rentiel de ce dipl&#x00F4;me national de premier cycle fait l&#x2019;objet d&#x2019;un travail de concertation associant les organisations professionnelles d&#x2019;employeurs repr&#x00E9;sentatives, en vertu de l&#x2019;article L.&nbsp;6113&#x2011;3 du code du travail et selon les modalit&#x00E9;s d&#x00E9;finies au 1&#x00B0;&nbsp;de l&#x2019;article D. 6113&#x2011;27 du m&#x00EA;me code.&nbsp;&#x00BB;</p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p style=\"text-align: justify;\">Le dipl&#x00F4;me national de premier cycle en sciences et techniques de l&#x2019;agronomie &#x00E0; vis&#x00E9;e professionnalisante est appel&#x00E9; &#x00E0; devenir une r&#x00E9;f&#x00E9;rence importante en termes de niveau de qualification, comme le pr&#x00E9;cise l&#x2019;expos&#x00E9; des motifs du projet de loi. Comme pour tous les dipl&#x00F4;mes &#x00E9;labor&#x00E9;s au niveau national, les repr&#x00E9;sentants de la profession doivent pouvoir concourir &#x00E0; la d&#x00E9;finition du r&#x00E9;f&#x00E9;rentiel de ce dipl&#x00F4;me, dans le cadre des instances consultatives au sein desquelles ils si&#x00E8;gent, en l&#x2019;occurrence au niveau du Comit&#x00E9; de suivi des cycles licence, master et doctorat (CSLMD).</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2024-05-06", "datePublication": "2024-05-16", "soumisArticle40": "false", "etatDesTraitements": {"etat": {"code": "DI", "libelle": "Discut\u00e9"}, "sousEtat": {"code": "DI", "libelle": "Rejet\u00e9"}}, "dateSort": "2024-05-22T16:28:52+02:00", "sort": "Rejet\u00e9"}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L16PO791932BTC2600P0D1N000016?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "seanceDiscussionRef": "RUANR5L16S2024IDS28367", "article99": "false", "loiReference": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionCommune": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionIdentique": {"idDiscussion": "70160", "typePosition": "Premier"}, "accordGouvernementDepotHorsDelai": "Sans objet"}}