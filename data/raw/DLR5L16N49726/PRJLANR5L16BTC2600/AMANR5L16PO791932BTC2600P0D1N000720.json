{"amendement": {"@xmlns": "http://schemas.assemblee-nationale.fr/referentiel", "uid": "AMANR5L16PO791932BTC2600P0D1N000720", "chronotag": "1716406406618", "legislature": "16", "identification": {"numeroLong": "720", "numeroOrdreDepot": "720", "prefixeOrganeExamen": "AN", "numeroRect": "0"}, "examenRef": "EXANR5L16PO791932BTC2600P0D1", "texteLegislatifRef": "PRJLANR5L16BTC2600", "triAmendement": "dsaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaawf", "cardinaliteAmdtMultiples": "1", "amendementParentRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "signataires": {"auteur": {"typeAuteur": "D\u00e9put\u00e9", "gouvernementRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "acteurRef": "PA642847", "groupePolitiqueRef": "PO800508", "auteurRapporteurOrganeRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "cosignataires": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "suffixe": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "libelle": "M.&#160;Bazin"}, "pointeurFragmentTexte": {"partieAmendableRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "division": {"titre": "Article 8", "articleDesignationCourte": "ART. 8", "articleDesignation": "ARTICLE 8", "type": "ARTICLE", "avant_A_Apres": "A", "divisionRattachee": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "articleAdditionnel": "false", "chapitreAdditionnel": "false", "urlDivisionTexteVise": "/16/textes/2600.asp#D_Article_8"}, "amendementStandard": {"alinea": {"avant_A_Apres": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "numero": "1", "alineaDesignation": "Alin\u00e9a 1"}}}, "corps": {"cartoucheInformatif": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenuAuteur": {"dispositif": "<p style=\"text-align: justify;\">&#x00C0; l&#x2019;alin&#x00E9;a&nbsp;1, substituer &#x00E0; l&#x2019;ann&#x00E9;e&nbsp;:</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;2035&nbsp;&#x00BB;</p><p style=\"text-align: justify;\">l&#x2019;ann&#x00E9;e&nbsp;:&nbsp;</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;2050&nbsp;&#x00BB;.</p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p style=\"text-align: justify;\">L&#x2019;&#x00E9;laboration de politiques publiques relatives aux enjeux que conna&#x00EE;t l&#x2019;agriculture fran&#x00E7;aise actuellement et dans les prochaines d&#x00E9;cennies sont tels qu&#x2019;ils n&#x00E9;cessitent un travail consciencieux et en profondeur. <br>Ces politiques publiques doivent, temporellement parlant, avoir mati&#x00E8;re pour &#x00EA;tre effectives, d&#x2019;autant que les enjeux identifi&#x00E9;s doivent &#x00EA;tre travaill&#x00E9;s sur la dur&#x00E9;e, sans essoufflement &#x00E0; &#x00E9;ch&#x00E9;ance.&nbsp; <br>Face &#x00E0; la contenance-m&#x00EA;me de certaines de ces probl&#x00E9;matiques, ainsi qu&#x2019;au d&#x00E9;lai que n&#x00E9;cessiterait leur progressive r&#x00E9;solution, une &#x00E9;ch&#x00E9;ance de dix ann&#x00E9;es ne semble pas r&#x00E9;aliste&nbsp;; ce qui justifie la proposition d&#x2019;une &#x00E9;ch&#x00E9;ance plus lointaine en mati&#x00E8;re de d&#x00E9;clinaison de politiques publiques, passant de 2035 &#x00E0; 2050.</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2024-05-07", "datePublication": "2024-05-16", "soumisArticle40": "false", "etatDesTraitements": {"etat": {"code": "DI", "libelle": "Discut\u00e9"}, "sousEtat": {"code": "DI", "libelle": "Non soutenu"}}, "dateSort": "2024-05-22T21:33:04+02:00", "sort": "Non soutenu"}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L16PO791932BTC2600P0D1N000720?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "seanceDiscussionRef": "RUANR5L16S2024IDS28368", "article99": "false", "loiReference": {"codeLoi": "1", "divisionCodeLoi": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "discussionCommune": {"idDiscussion": "25035", "typePosition": "Milieu"}, "discussionIdentique": {"idDiscussion": "69801", "typePosition": "Milieu"}, "accordGouvernementDepotHorsDelai": "Sans objet"}}