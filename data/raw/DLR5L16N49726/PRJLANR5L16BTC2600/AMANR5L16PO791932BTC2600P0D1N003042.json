{"amendement": {"@xmlns": "http://schemas.assemblee-nationale.fr/referentiel", "uid": "AMANR5L16PO791932BTC2600P0D1N003042", "chronotag": "1716546775393", "legislature": "16", "identification": {"numeroLong": "3042", "numeroOrdreDepot": "3042", "prefixeOrganeExamen": "AN", "numeroRect": "0"}, "examenRef": "EXANR5L16PO791932BTC2600P0D1", "texteLegislatifRef": "PRJLANR5L16BTC2600", "triAmendement": "iiaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaji", "cardinaliteAmdtMultiples": "1", "amendementParentRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "signataires": {"auteur": {"typeAuteur": "D\u00e9put\u00e9", "gouvernementRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "acteurRef": "PA1874", "groupePolitiqueRef": "PO800508", "auteurRapporteurOrganeRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "cosignataires": {"acteurRef": ["PA642847", "PA721410", "PA267901", "PA608416", "PA718884", "PA266797", "PA721474", "PA722390"]}, "suffixe": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "libelle": "M.&#160;Le&#160;Fur,  M.&#160;Bazin, Mme&#160;Bonnivard, M.&#160;Kamardine, M.&#160;Hetzel, Mme&#160;Bazin-Malgras, M.&#160;Gosselin, M.&#160;Viry et Mme&#160;Corneloup"}, "pointeurFragmentTexte": {"partieAmendableRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "division": {"titre": "Article 12 bis", "articleDesignationCourte": "ART. 12 BIS", "articleDesignation": "ARTICLE 12 BIS", "type": "ARTICLE", "avant_A_Apres": "A", "divisionRattachee": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "articleAdditionnel": "false", "chapitreAdditionnel": "false", "urlDivisionTexteVise": "/16/textes/2600.asp#D_Article_12_bis"}, "amendementStandard": {"alinea": {"avant_A_Apres": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "numero": "2", "alineaDesignation": "Alin\u00e9a 2"}}}, "corps": {"cartoucheInformatif": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenuAuteur": {"dispositif": "<p style=\"text-align: justify;\">I. &#x2013; &#x00C0; l&#x2019;alin&#x00E9;a&nbsp;2, substituer au montant&nbsp;:</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;10 000&nbsp;euros&nbsp;&#x00BB;&nbsp;&#x00BB;,</p><p style=\"text-align: justify;\">le montant&nbsp;:</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;50 000&nbsp;euros&nbsp;&#x00BB;.</p><p style=\"text-align: justify;\">II. &#x2013; En cons&#x00E9;quence, proc&#x00E9;der &#x00E0; la m&#x00EA;me substitution &#x00E0; l&#x2019;alin&#x00E9;a&nbsp;4.&nbsp;</p><p style=\"text-align: justify;\">&nbsp;</p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p style=\"text-align: justify;\">L&#x2019;article&nbsp;12 <i>bis</i> du pr&#x00E9;sent projet de loi vise &#x00E0; autoriser les soci&#x00E9;t&#x00E9;s civiles agricoles &#x00E0; pratiquer, &#x00E0; la marge, des activit&#x00E9;s commerciales accessoires &#x00E0; hauteur de 10 000&nbsp;euros dans la limite de 50&nbsp;% du chiffre d&#x2019;affaires, avec application de la transparence GAEC pour le seuil de 10 000&nbsp;euros.</p><p style=\"text-align: justify;\">Le pr&#x00E9;sent amendement propose de passer ce seuil de 10 000&nbsp;euros &#x00E0; 50 000&nbsp;euros.</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2024-05-10", "datePublication": "2024-05-16", "soumisArticle40": "false", "etatDesTraitements": {"etat": {"code": "DI", "libelle": "Discut\u00e9"}, "sousEtat": {"code": "DI", "libelle": "Tomb\u00e9"}}, "dateSort": "2024-05-24T12:32:47+02:00", "sort": "Tomb\u00e9"}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L16PO791932BTC2600P0D1N003042?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "seanceDiscussionRef": "RUANR5L16S2024IDS28372", "article99": "false", "loiReference": {"codeLoi": "1", "divisionCodeLoi": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "discussionCommune": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionIdentique": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "accordGouvernementDepotHorsDelai": "Sans objet"}}