{"amendement": {"@xmlns": "http://schemas.assemblee-nationale.fr/referentiel", "uid": "AMANR5L16PO791932BTC2600P0D1N000117", "chronotag": "1716593887412", "legislature": "16", "identification": {"numeroLong": "117", "numeroOrdreDepot": "117", "prefixeOrganeExamen": "AN", "numeroRect": "0"}, "examenRef": "EXANR5L16PO791932BTC2600P0D1", "texteLegislatifRef": "PRJLANR5L16BTC2600", "triAmendement": "poaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaawo", "cardinaliteAmdtMultiples": "1", "amendementParentRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "signataires": {"auteur": {"typeAuteur": "D\u00e9put\u00e9", "gouvernementRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "acteurRef": "PA774954", "groupePolitiqueRef": "PO800508", "auteurRapporteurOrganeRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "cosignataires": {"acteurRef": ["PA719318", "PA642847", "PA721410", "PA266793", "PA342240", "PA793508", "PA722390", "PA330909", "PA793476", "PA608826", "PA266797", "PA794058", "PA608416", "PA608016", "PA795406", "PA793094", "PA794410", "PA794178", "PA721474"]}, "suffixe": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "libelle": "Mme&#160;Blin,  Mme&#160;Anthoine, M.&#160;Bazin, Mme&#160;Bonnivard, M.&#160;Bony, M.&#160;Bourgeaux, M.&#160;Brigand, Mme&#160;Corneloup, M.&#160;Descoeur, M.&#160;Dubois, Mme&#160;Duby-Muller, M.&#160;Gosselin, Mme&#160;Gruet, M.&#160;Hetzel, Mme&#160;Louwagie, Mme&#160;P&#233;rigault, M.&#160;Ray, M.&#160;Seitlinger, M.&#160;Taite et M.&#160;Viry"}, "pointeurFragmentTexte": {"partieAmendableRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "division": {"titre": "Article 20", "articleDesignationCourte": "APR\u00c8S ART. 20", "articleDesignation": "APR\u00c8S L'ARTICLE 20, ins\u00e9rer l'article suivant:", "type": "ARTICLE", "avant_A_Apres": "Apr\u00e8s", "divisionRattachee": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "articleAdditionnel": "true", "chapitreAdditionnel": "false", "urlDivisionTexteVise": "/16/textes/2600.asp#D_Article_20"}, "amendementStandard": {"alinea": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "corps": {"cartoucheInformatif": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenuAuteur": {"dispositif": "<p style=\"text-align: justify;\">Dans un d&#x00E9;lai de six mois &#x00E0; compter de la promulgation de la pr&#x00E9;sente loi, le Gouvernement remet au Parlement un rapport au sujet de la suppression des aides de la politique agricole commune pour les agriculteurs de plus de 67 ans. Il doit &#x00E9;valuer si la proc&#x00E9;dure a permis une bonne information des agriculteurs qui ont vu leurs aides supprim&#x00E9;es, ainsi que mesurer les cons&#x00E9;quences de cette d&#x00E9;cision.</p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p style=\"text-align: justify;\">La nouvelle PAC ne permet plus de b&#x00E9;n&#x00E9;ficier des aides europ&#x00E9;ennes pour un agriculteur de plus de 67 ans. Or, selon le minist&#x00E8;re de l&#x2019;Agriculture en 2021, 10% des b&#x00E9;n&#x00E9;ficiaires des aides PAC &#x00E9;taient &#x00E2;g&#x00E9;s de plus de 67 ans. Outre le fait que cette d&#x00E9;cision met en difficult&#x00E9; des agriculteurs qui se trouvaient parfois dans des situations pr&#x00E9;caires, rien ne d&#x00E9;montre que cela va contribuer &#x00E0; faciliter la transmission des exploitations.</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2024-05-06", "datePublication": "2024-05-21", "soumisArticle40": "false", "etatDesTraitements": {"etat": {"code": "DI", "libelle": "Discut\u00e9"}, "sousEtat": {"code": "DI", "libelle": "Rejet\u00e9"}}, "dateSort": "2024-05-25T01:37:45+02:00", "sort": "Rejet\u00e9"}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L16PO791932BTC2600P0D1N000117?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "seanceDiscussionRef": "RUANR5L16S2024IDS28374", "article99": "false", "loiReference": {"codeLoi": "Z3", "divisionCodeLoi": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "discussionCommune": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionIdentique": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "accordGouvernementDepotHorsDelai": "Sans objet"}}