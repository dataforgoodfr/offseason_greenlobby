icleDesignationCourte": "ART. PREMIER", "articleDesignation": "ARTICLE PREMIER", "type": "ARTICLE", "avant_A_Apres": "A", "divisionRattachee": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "articleAdditionnel": "false", "chapitreAdditionnel": "false", "urlDivisionTexteVise": "/16/textes/2600.asp#D_Article_1er"}, "amendementStandard": {"alinea": {"avant_A_Apres": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "numero": "22", "alineaDesignation": "Alin\u00e9a 22"}}}, "corps": {"cartoucheInformatif": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenuAuteur": {"dispositif": "<p style=\"text-align: justify;\">&#x00C0;&nbsp;l&#x2019;alin&#x00E9;a&nbsp;22, apr&#x00E8;s le mot&nbsp;:</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;aquacole&nbsp;&#x00BB;,</p><p style=\"text-align: justify;\">ins&#x00E9;rer le mot&nbsp;:</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;, viticole&nbsp;&#x00BB;.</p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p style=\"text-align: justify;\">Cet article vise &#x00E0; int&#x00E9;grer la souverainet&#x00E9; alimentaire &#x00E0; la liste des int&#x00E9;r&#x00EA;ts fondamentaux de la Nation, au sein du code rural et de la p&#x00EA;che maritime.</p><p>A ce titre, il est essentiel d&#x2019;ajouter dans cette liste &#x00E9;galement la viticulture, fleuron national fran&#x00E7;ais. La souverainet&#x00E9; alimentaire concerne nos verres comme nos assiettes, dans un domaine vins et spiritueux fortement exportateur, au 3e rang des contributeurs positifs &#x00E0; la balance commerciale de notre pays.</p><p>Tel est le cens de cet amendement.</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2024-05-06", "datePublication": "2024-05-13", "soumisArticle40": "false", "etatDesTraitements"