{"amendement": {"@xmlns": "http://schemas.assemblee-nationale.fr/referentiel", "uid": "AMANR5L16PO791932BTC2600P0D1N000638", "chronotag": "1715981871124", "legislature": "16", "identification": {"numeroLong": "638", "numeroOrdreDepot": "638", "prefixeOrganeExamen": "AN", "numeroRect": "0"}, "examenRef": "EXANR5L16PO791932BTC2600P0D1", "texteLegislatifRef": "PRJLANR5L16BTC2600", "triAmendement": "biaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaagxc", "cardinaliteAmdtMultiples": "1", "amendementParentRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "signataires": {"auteur": {"typeAuteur": "D\u00e9put\u00e9", "gouvernementRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "acteurRef": "PA722202", "groupePolitiqueRef": "PO800502", "auteurRapporteurOrganeRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "cosignataires": {"acteurRef": ["PA267306", "PA720838", "PA796106", "PA795868", "PA795192", "PA718926", "PA721896", "PA795998", "PA795184", "PA774962", "PA335612", "PA796010", "PA793174", "PA795820", "PA721270", "PA795240", "PA795876", "PA720692", "PA605745", "PA794650", "PA720992"]}, "suffixe": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "libelle": "M.&#160;Jumel,  M.&#160;Chassaigne, Mme&#160;Bourouaha, M.&#160;B&#233;nard, M.&#160;Castor, M.&#160;Chailloux, M.&#160;Dharr&#233;ville, Mme&#160;Faucillon, Mme&#160;K/Bidi, M.&#160;Le&#160;Gayic, Mme&#160;Lebon, M.&#160;Lecoq, M.&#160;Maillot, M.&#160;Monnet, M.&#160;Nadeau, M.&#160;Peu, Mme&#160;Reid&#160;Arbelot, M.&#160;Rimane, M.&#160;Roussel, M.&#160;Sansu, M.&#160;Tellier et M.&#160;William"}, "pointeurFragmentTexte": {"partieAmendableRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "division": {"titre": "Article 3", "articleDesignationCourte": "ART. 3", "articleDesignation": "ARTICLE 3", "type": "ARTICLE", "avant_A_Apres": "A", "divisionRattachee": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "articleAdditionnel": "false", "chapitreAdditionnel": "false", "urlDivisionTexteVise": "/16/textes/2600.asp#D_Article_3"}, "amendementStandard": {"alinea": {"avant_A_Apres": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "numero": "6", "alineaDesignation": "Alin\u00e9a 6"}}}, "corps": {"cartoucheInformatif": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenuAuteur": {"dispositif": "<p style=\"text-align: justify;\">&#x00C0; la premi&#x00E8;re phrase de l&#x2019;alin&#x00E9;a&nbsp;6, apr&#x00E8;s le mot&nbsp;:&nbsp;</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;agricoles&nbsp;&#x00BB;</p><p style=\"text-align: justify;\">ins&#x00E9;rer les mots&nbsp;:&nbsp;</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;, dont l&#x2019;agriculture biologique&nbsp;&#x00BB;.&nbsp;</p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p style=\"text-align: justify;\">Le pr&#x00E9;sent amendement vise &#x00E0; garantir que l'agriculture biologique puisse &#x00EA;tre une composante directe du contenu des enseignements visant &#x00E0; pr&#x00E9;senter les diff&#x00E9;rents syst&#x00E8;mes de production agricoles.&nbsp;</p><p style=\"text-align: justify;\">L&#x2019;&#x00E9;tude de l&#x2019;agriculture biologique ne peut se concevoir sur l&#x2019;unique volont&#x00E9; d&#x2019;enseignants et formateurs. Les formations doivent pr&#x00E9;senter, au m&#x00EA;me titre que les syst&#x00E8;mes de production traditionnels, les techniques agro&#x00E9;cologiques, en vue de promouvoir une agriculture r&#x00E9;siliente, locale et durable, et redonner ainsi l&#x2019;autonomie et les capacit&#x00E9;s de choix aux futurs paysans. L&#x2019;agriculture biologique est consid&#x00E9;r&#x00E9;e comme le mod&#x00E8;le le plus abouti (en terme d&#x2019;agro&#x00E9;cologie). Sa promotion correspond aux aspirations des jeunes et moins jeunes form&#x00E9;s accueillis dans les &#x00E9;tablissements de formation agricole.</p><p style=\"text-align: justify;\">Le pr&#x00E9;sent amendement a &#x00E9;t&#x00E9; travaill&#x00E9; avec le SENTAP-FSU.&nbsp;</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2024-05-07", "datePublication": "2024-05-14", "soumisArticle40": "false", "etatDesTraitements": {"etat": {"code": "DI", "libelle": "Discut\u00e9"}, "sousEtat": {"code": "DI", "libelle": "Rejet\u00e9"}}, "dateSort": "2024-05-17T23:37:36+02:00", "sort": "Rejet\u00e9"}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L16PO791932BTC2600P0D1N000638?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "seanceDiscussionRef": "RUANR5L16S2024IDS28364", "article99": "false", "loiReference": {"codeLoi": "1", "divisionCodeLoi": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "discussionCommune": {"idDiscussion": "24975", "typePosition": "Milieu"}, "discussionIdentique": {"idDiscussion": "69902", "typePosition": "Premier"}, "accordGouvernementDepotHorsDelai": "Sans objet"}}