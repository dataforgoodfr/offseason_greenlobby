{"amendement": {"@xmlns": "http://schemas.assemblee-nationale.fr/referentiel", "uid": "AMANR5L16PO791932BTC2600P0D1N003984", "chronotag": "1716461996815", "legislature": "16", "identification": {"numeroLong": "3984", "numeroOrdreDepot": "3984", "prefixeOrganeExamen": "AN", "numeroRect": "0"}, "examenRef": "EXANR5L16PO791932BTC2600P0D1", "texteLegislatifRef": "PRJLANR5L16BTC2600", "triAmendement": "dsaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaajxhwu", "cardinaliteAmdtMultiples": "1", "amendementParentRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "signataires": {"auteur": {"typeAuteur": "D\u00e9put\u00e9", "gouvernementRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "acteurRef": "PA942", "groupePolitiqueRef": "PO800532", "auteurRapporteurOrganeRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "cosignataires": {"acteurRef": ["PA605069", "PA793796", "PA719286", "PA719146", "PA721046", "PA719138", "PA267766", "PA720696", "PA267042", "PA822617", "PA795244", "PA720976", "PA607619", "PA266788", "PA721486", "PA333421", "PA793210", "PA794626", "PA2952"]}, "suffixe": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "libelle": "M.&#160;de&#160;Courson,  M.&#160;Jean-Louis&#160;Bricout, M.&#160;Taupiac, M.&#160;Colombani, M.&#160;Acquaviva, Mme&#160;Bassire, M.&#160;Castellani, M.&#160;Guy&#160;Bricout, Mme&#160;Descamps, M.&#160;Favennec-B&#233;cot&#160;, Mme&#160;Froger, M.&#160;Lenormand, M.&#160;Mathiasin, M.&#160;Molac, M.&#160;Morel-&#192;-L&apos;Huissier, M.&#160;Naegelen, M.&#160;Pancher, M.&#160;Panifous, M.&#160;Saint-Huile et M.&#160;Warsmann"}, "pointeurFragmentTexte": {"partieAmendableRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "division": {"titre": "Article 8", "articleDesignationCourte": "ART. 8", "articleDesignation": "ARTICLE 8", "type": "ARTICLE", "avant_A_Apres": "A", "divisionRattachee": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "articleAdditionnel": "false", "chapitreAdditionnel": "false", "urlDivisionTexteVise": "/16/textes/2600.asp#D_Article_8"}, "amendementStandard": {"alinea": {"avant_A_Apres": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "numero": "5", "alineaDesignation": "Alin\u00e9a 5"}}}, "corps": {"cartoucheInformatif": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenuAuteur": {"dispositif": "<p style=\"text-align: justify;\">A l&#x2019;alin&#x00E9;a&nbsp;5,&nbsp;substituer aux mots&nbsp;:</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;d&#x2019;investisseurs priv&#x00E9;s&nbsp;&#x00BB;,</p><p style=\"text-align: justify;\">les mots&nbsp;:</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;personnes physiques&nbsp;&#x00BB;.</p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p style=\"text-align: justify;\">L&#x2019;article 8 fixe les grands objectifs des politiques publiques agricoles pour 2025-2035&nbsp;: celles-ci doivent favoriser le maintien et le d&#x00E9;veloppement, la cr&#x00E9;ation, l&#x2019;adaptation et la transmission des exploitations agricoles.</p><p style=\"text-align: justify;\">Il pr&#x00E9;voit qu'afin de&nbsp;favoriser l&#x2019;installation de nouveaux exploitants agricoles et l&#x2019;adaptation des exploitations agricoles au changement climatique, l&#x2019;&#x00C9;tat se donne comme objectif, aux c&#x00F4;t&#x00E9;s des collectivit&#x00E9;s territoriales volontaires ainsi que d&#x2019;investisseurs priv&#x00E9;s d'accroitre la mobilisation de fonds.&nbsp;</p><p style=\"text-align: justify;\">Cet amendement vise &#x00E0; limiter la sp&#x00E9;culation fonci&#x00E8;re en pr&#x00E9;voyant que seules les personnes physiques, et non pas l'ensemble des investisseurs priv&#x00E9;s, pourront investir dans le portage du foncier agricole.&nbsp;</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2024-05-10", "datePublication": "2024-05-16", "soumisArticle40": "false", "etatDesTraitements": {"etat": {"code": "DI", "libelle": "Discut\u00e9"}, "sousEtat": {"code": "DI", "libelle": "Tomb\u00e9"}}, "dateSort": "2024-05-23T12:59:29+02:00", "sort": "Tomb\u00e9"}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L16PO791932BTC2600P0D1N003984?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "seanceDiscussionRef": "RUANR5L16S2024IDS28369", "article99": "false", "loiReference": {"codeLoi": "1", "divisionCodeLoi": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "discussionCommune": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionIdentique": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "accordGouvernementDepotHorsDelai": "Sans objet"}}