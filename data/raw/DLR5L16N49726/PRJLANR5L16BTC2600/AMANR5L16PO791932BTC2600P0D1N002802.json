{"amendement": {"@xmlns": "http://schemas.assemblee-nationale.fr/referentiel", "uid": "AMANR5L16PO791932BTC2600P0D1N002802", "chronotag": "1716475165063", "legislature": "16", "identification": {"numeroLong": "2802", "numeroOrdreDepot": "2802", "prefixeOrganeExamen": "AN", "numeroRect": "0"}, "examenRef": "EXANR5L16PO791932BTC2600P0D1", "texteLegislatifRef": "PRJLANR5L16BTC2600", "triAmendement": "dsaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaoanww", "cardinaliteAmdtMultiples": "1", "amendementParentRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "signataires": {"auteur": {"typeAuteur": "D\u00e9put\u00e9", "gouvernementRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "acteurRef": "PA720560", "groupePolitiqueRef": "PO800484", "auteurRapporteurOrganeRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "cosignataires": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "suffixe": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "libelle": "Mme&#160;Perrine&#160;Goulet"}, "pointeurFragmentTexte": {"partieAmendableRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "division": {"titre": "Article 8", "articleDesignationCourte": "ART. 8", "articleDesignation": "ARTICLE 8", "type": "ARTICLE", "avant_A_Apres": "A", "divisionRattachee": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "articleAdditionnel": "false", "chapitreAdditionnel": "false", "urlDivisionTexteVise": "/16/textes/2600.asp#D_Article_8"}, "amendementStandard": {"alinea": {"avant_A_Apres": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "numero": "6", "alineaDesignation": "Alin\u00e9a 6"}}}, "corps": {"cartoucheInformatif": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenuAuteur": {"dispositif": "<p style=\"text-align: justify;\">&nbsp;</p><p style=\"text-align: justify;\">Compl&#x00E9;ter l&#x2019;alin&#x00E9;a&nbsp;6 par la phrase suivante&nbsp;:</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;Afin de limiter la sp&#x00E9;culation sur le foncier agricole, l&#x2019;&#x00C9;tat organise la r&#x00E9;gulation de l&#x2019;installation d&#x2019;&#x00E9;quipements photovolta&#x00EF;ques au sol.&nbsp;&#x00BB;</p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p style=\"text-align: justify;\">Le pr&#x00E9;sent amendement d'appel vise &#x00E0; ce que l'installation d'&#x00E9;quipement photovolta&#x00EF;ques ne puissent constituer un outil de sp&#x00E9;culation trop fort. il conviendrait que dans un d&#x00E9;lai post-installation et pr&#x00E9;-retraite il ne puisse &#x00EA;tre possible d'installer des dispositifs photovolta&#x00EF;ques que sur des b&#x00E2;timents agricoles et non au sol.&nbsp;</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2024-05-10", "datePublication": "2024-05-23", "soumisArticle40": "false", "etatDesTraitements": {"etat": {"code": "DI", "libelle": "Discut\u00e9"}, "sousEtat": {"code": "DI", "libelle": "Rejet\u00e9"}}, "dateSort": "2024-05-23T16:39:04+02:00", "sort": "Rejet\u00e9"}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L16PO791932BTC2600P0D1N002802?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "seanceDiscussionRef": "RUANR5L16S2024IDS28370", "article99": "false", "loiReference": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionCommune": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionIdentique": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "accordGouvernementDepotHorsDelai": "Sans objet"}}