{"amendement": {"@xmlns": "http://schemas.assemblee-nationale.fr/referentiel", "uid": "AMANR5L16PO791932BTC2600P0D1N000216", "chronotag": "1715898622526", "legislature": "16", "identification": {"numeroLong": "216", "numeroOrdreDepot": "216", "prefixeOrganeExamen": "AN", "numeroRect": "0"}, "examenRef": "EXANR5L16PO791932BTC2600P0D1", "texteLegislatifRef": "PRJLANR5L16BTC2600", "triAmendement": "acaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaavdqzzzzg", "cardinaliteAmdtMultiples": "1", "amendementParentRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "signataires": {"auteur": {"typeAuteur": "D\u00e9put\u00e9", "gouvernementRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "acteurRef": "PA608016", "groupePolitiqueRef": "PO800508", "auteurRapporteurOrganeRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "cosignataires": {"acteurRef": ["PA794410", "PA643134", "PA793322", "PA795406", "PA722390", "PA346054"]}, "suffixe": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "libelle": "Mme&#160;Louwagie,  M.&#160;Seitlinger, Mme&#160;Valentin, Mme&#160;D&apos;Intorni, Mme&#160;P&#233;rigault, Mme&#160;Corneloup et M.&#160;Emmanuel&#160;Maquet"}, "pointeurFragmentTexte": {"partieAmendableRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "division": {"titre": "Article PREMIER", "articleDesignationCourte": "ART. PREMIER", "articleDesignation": "ARTICLE PREMIER", "type": "ARTICLE", "avant_A_Apres": "A", "divisionRattachee": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "articleAdditionnel": "false", "chapitreAdditionnel": "false", "urlDivisionTexteVise": "/16/textes/2600.asp#D_Article_1er"}, "amendementStandard": {"alinea": {"avant_A_Apres": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "numero": "33", "alineaDesignation": "Alin\u00e9a 33"}}}, "corps": {"cartoucheInformatif": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenuAuteur": {"dispositif": "<p style=\"text-align: justify;\">R&#x00E9;diger ainsi l&#x2019;alin&#x00E9;a&nbsp;33&nbsp;:</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;&#x00C0; ce titre, elle oriente l&#x2019;installation en agriculture aux fins de pr&#x00E9;server et d&#x2019;am&#x00E9;liorer la souverainet&#x00E9; alimentaire et &#x00E9;nerg&#x00E9;tique, de mani&#x00E8;re adapt&#x00E9;e aux enjeux de chaque territoire, et vers des syst&#x00E8;mes de production diversifi&#x00E9;s et viables humainement, &#x00E9;conomiquement et &#x00E9;cologiquement, &#x00E0; travers des mesures visant &#x00E0;&nbsp;:&nbsp;&#x00BB;</p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p style=\"text-align: justify;\">La r&#x00E9;daction actuelle de l&#x2019;alin&#x00E9;a 14 de l&#x2019;article 1 introduit la notion d&#x2019;orientation &#x201C;prioritaire&#x201D; de la politique d&#x2019;installation et de transmission au profit de &#x201C;secteurs strat&#x00E9;giques pour la souverainet&#x00E9; alimentaire&#x201D;. Une telle d&#x00E9;marche de priorisation pourrait aboutir &#x00E0; des situations de blocage pour des exploitation pourtant viable &#x00E9;conomiquement. De plus, le march&#x00E9; ne permet pas de prioriser un secteur agricole au d&#x00E9;triment d&#x2019;un autre. Cet amendement revient donc sur cette notion, tout en confortant le lien entre la politique d&#x2019;installation et la n&#x00E9;cessaire pr&#x00E9;servation et am&#x00E9;lioration de la souverainet&#x00E9; alimentaire et &#x00E9;nerg&#x00E9;tique.</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2024-05-07", "datePublication": "2024-05-13", "soumisArticle40": "false", "etatDesTraitements": {"etat": {"code": "DI", "libelle": "Discut\u00e9"}, "sousEtat": {"code": "DI", "libelle": "Tomb\u00e9"}}, "dateSort": "2024-05-17T00:28:18+02:00", "sort": "Tomb\u00e9"}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L16PO791932BTC2600P0D1N000216?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "seanceDiscussionRef": "RUANR5L16S2024IDS28361", "article99": "false", "loiReference": {"codeLoi": "1", "divisionCodeLoi": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "discussionCommune": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionIdentique": {"idDiscussion": "70060", "typePosition": "Milieu"}, "accordGouvernementDepotHorsDelai": "Sans objet"}}