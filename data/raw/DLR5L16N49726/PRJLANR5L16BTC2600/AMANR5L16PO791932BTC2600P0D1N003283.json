articleDesignation": "ARTICLE 10", "type": "ARTICLE", "avant_A_Apres": "A", "divisionRattachee": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "articleAdditionnel": "false", "chapitreAdditionnel": "false", "urlDivisionTexteVise": "/16/textes/2600.asp#D_Article_10"}, "amendementStandard": {"alinea": {"avant_A_Apres": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "numero": "8", "alineaDesignation": "Alin\u00e9a 8"}}}, "corps": {"cartoucheInformatif": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenuAuteur": {"dispositif": "<p style=\"text-align: justify;\">&#x00C0; l&#x2019;alin&#x00E9;a&nbsp;8, substituer aux mots&nbsp;:</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;cinq ans&nbsp;&#x00BB;</p><p style=\"text-align: justify;\">le&nbsp;mot&nbsp;:</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;un an&nbsp;&#x00BB;.</p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p style=\"text-align: justify;\">Cet amendement tend &#x00E0; r&#x00E9;duire le temps en amont duquel un agriculteur ayant pour projet de partir &#x00E0; la retraite doit signaler ce d&#x00E9;part &#x00E0; la retraite au point d&#x2019;accueil d&#x00E9;partemental unique de 5 &#x00E0; 1 an.<br>&nbsp;<br>Cette disposition, d&#x00E9;j&#x00E0; attentatoire &#x00AB;&nbsp;&#x00E0; la libert&#x00E9; d&#x2019;entreprendre et &#x00E0; la libert&#x00E9; contractuelle qui d&#x00E9;coulent de l&#x2019;article 4 de la D&#x00E9;claration des droits de l&#x2019;homme et du citoyen&nbsp;&#x00BB; comme l&#x2019;a soulign&#x00E9;e le Conseil d&#x2019;&#x00C9;tat dans son avis, impose en outre de se signaler tr&#x00E8;s en amont du d&#x00E9;part &#x00E0; la retraite. Pour beaucoup d&#x2019;agriculteurs, il difficile de savoir de mani&#x00E8;re certaine s&#x2019;ils partiront &#x00E0; la retraite dans 5 ans si bien que la disposition serait difficilement applicable et engendrerait de nouveaux probl&#x00E8;mes administratifs.<br>&nbsp;<br>C&#x2019;est l&#x00E0; encore rajouter des obligations et de la complexit&#x00E9;.</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2024-05-10", "datePublication": "2024-05-17", "soumisArticle40": "false", "etatDesTraitements": {"etat": {"code": "DI", "libelle": "Discut\u00e9"}, "sousEtat": {"code": "DI", "libelle": "Rejet\u00e9"}}, "dateSort": "2024-05-23T22:43:05+02:00", "sort": "Rejet\u00e9"}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L16PO791932BTC2600P0D1N003283?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "seanceDiscussionRef": "RUANR5L16S2024IDS28371", "article99": "false", "loiReference": {"codeLoi": "1", "divisionCodeLoi": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "discussionCommune": {"idDiscussion": "25065", "typePosition": "Milieu"}, "discussionIdentique": {"idDiscussion": "70565", "typePosition": "Milieu"}, "accordGouvernementDepotHorsDelai": "Sans objet"}}