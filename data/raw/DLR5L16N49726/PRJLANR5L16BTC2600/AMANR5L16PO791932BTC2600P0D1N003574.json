{"amendement": {"@xmlns": "http://schemas.assemblee-nationale.fr/referentiel", "uid": "AMANR5L16PO791932BTC2600P0D1N003574", "chronotag": "1716537085142", "legislature": "16", "identification": {"numeroLong": "3574", "numeroOrdreDepot": "3574", "prefixeOrganeExamen": "AN", "numeroRect": "0"}, "examenRef": "EXANR5L16PO791932BTC2600P0D1", "texteLegislatifRef": "PRJLANR5L16BTC2600", "triAmendement": "esaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaadwb", "cardinaliteAmdtMultiples": "1", "amendementParentRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "signataires": {"auteur": {"typeAuteur": "D\u00e9put\u00e9", "gouvernementRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "acteurRef": "PA712015", "groupePolitiqueRef": "PO800508", "auteurRapporteurOrganeRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "cosignataires": {"acteurRef": ["PA793476", "PA330909", "PA718884", "PA330240", "PA1327", "PA605991", "PA266797", "PA783457", "PA794038", "PA794118", "PA721474", "PA794058"]}, "suffixe": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "libelle": "M.&#160;Dive,  M.&#160;Dubois, M.&#160;Descoeur, Mme&#160;Bazin-Malgras, M.&#160;Ciotti, M.&#160;Forissier, Mme&#160;Genevard, M.&#160;Gosselin, M.&#160;Habert-Dassault, M.&#160;Neuder, M.&#160;Vermorel-Marques, M.&#160;Viry et Mme&#160;Gruet"}, "pointeurFragmentTexte": {"partieAmendableRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "division": {"titre": "Article 10", "articleDesignationCourte": "ART. 10", "articleDesignation": "ARTICLE 10", "type": "ARTICLE", "avant_A_Apres": "A", "divisionRattachee": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "articleAdditionnel": "false", "chapitreAdditionnel": "false", "urlDivisionTexteVise": "/16/textes/2600.asp#D_Article_10"}, "amendementStandard": {"alinea": {"avant_A_Apres": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "numero": "26", "alineaDesignation": "Alin\u00e9a 26"}}}, "corps": {"cartoucheInformatif": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenuAuteur": {"dispositif": "<p style=\"text-align: justify;\">I.&nbsp;&#x2013;&nbsp;Supprimer la premi&#x00E8;re phrase de l&#x2019;alin&#x00E9;a 26.</p><p>II.&nbsp;&#x2013;&nbsp;En cons&#x00E9;quence, supprimer l&#x2019;alin&#x00E9;a 36.</p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p style=\"text-align: justify;\">Le r&#x00E9;pertoire unique d&#x00E9;partemental qu&#x2019;il est pr&#x00E9;vu de constituer dans le cadre de la cr&#x00E9;ation de &#x00AB;&nbsp;France services agriculture&nbsp;&#x00BB; enregistrera toutes les personnes ayant b&#x00E9;n&#x00E9;fici&#x00E9; d&#x2019;un conseil ou d&#x2019;un accompagnement au sein du r&#x00E9;seau &#x00AB;&nbsp;France services agriculture&nbsp;&#x00BB; et, &#x00E9;ventuellement, suivi les formations prescrites. L&#x2019;autorit&#x00E9; administrative, qui aura acc&#x00E8;s &#x00E0; ce r&#x00E9;pertoire, sera donc en mesure de v&#x00E9;rifier qui aura &#x00E9;t&#x00E9; b&#x00E9;n&#x00E9;ficiaire du service, si n&#x00E9;cessaire. Il est donc propos&#x00E9; de supprimer la mention suivante&nbsp;: &#x00AB;&nbsp;Les personnes ayant eu recours au dispositif de conseil et d&#x2019;accompagnement cr&#x00E9;&#x00E9; par le pr&#x00E9;sent article re&#x00E7;oivent une attestation qu&#x2019;elles doivent &#x00EA;tre en mesure de pr&#x00E9;senter sur demande de l&#x2019;autorit&#x00E9; administrative. &#x00BB;, ainsi que l&#x2019;alin&#x00E9;a 36, qui cr&#x00E9;ent une obligation particuli&#x00E8;re, &#x00E0; la charge des agriculteurs.</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2024-05-10", "datePublication": "2024-05-24", "soumisArticle40": "false", "etatDesTraitements": {"etat": {"code": "DI", "libelle": "Discut\u00e9"}, "sousEtat": {"code": "DI", "libelle": "Adopt\u00e9"}}, "dateSort": "2024-05-24T09:51:18+02:00", "sort": "Adopt\u00e9"}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L16PO791932BTC2600P0D1N003574?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "seanceDiscussionRef": "RUANR5L16S2024IDS28372", "article99": "false", "loiReference": {"codeLoi": "1", "divisionCodeLoi": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "discussionCommune": {"idDiscussion": "25060", "typePosition": "Premier"}, "discussionIdentique": {"idDiscussion": "70308", "typePosition": "Premier"}, "accordGouvernementDepotHorsDelai": "Sans objet"}}