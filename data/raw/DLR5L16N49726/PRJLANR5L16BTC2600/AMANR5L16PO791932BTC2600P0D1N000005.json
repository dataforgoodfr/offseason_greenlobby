{"amendement": {"@xmlns": "http://schemas.assemblee-nationale.fr/referentiel", "uid": "AMANR5L16PO791932BTC2600P0D1N000005", "chronotag": "1715898621062", "legislature": "16", "identification": {"numeroLong": "5", "numeroOrdreDepot": "5", "prefixeOrganeExamen": "AN", "numeroRect": "0"}, "examenRef": "EXANR5L16PO791932BTC2600P0D1", "texteLegislatifRef": "PRJLANR5L16BTC2600", "triAmendement": "acaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaavdqzzzze", "cardinaliteAmdtMultiples": "1", "amendementParentRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "signataires": {"auteur": {"typeAuteur": "D\u00e9put\u00e9", "gouvernementRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "acteurRef": "PA712015", "groupePolitiqueRef": "PO800508", "auteurRapporteurOrganeRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "cosignataires": {"acteurRef": ["PA605991", "PA720644", "PA720100", "PA342240", "PA1327", "PA722308", "PA608416", "PA794038", "PA721474", "PA718850", "PA719968", "PA794058", "PA332523", "PA721314", "PA718784", "PA794178", "PA721816", "PA721442", "PA718884", "PA783457", "PA330240", "PA794118"]}, "suffixe": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "libelle": "M.&#160;Dive,  Mme&#160;Genevard, M.&#160;Nury, M.&#160;Pradi&#233;, M.&#160;Bourgeaux, M.&#160;Forissier, Mme&#160;Serre, M.&#160;Hetzel, M.&#160;Neuder, M.&#160;Viry, M.&#160;Cordier, Mme&#160;Bonnet, Mme&#160;Gruet, Mme&#160;Dalloz, M.&#160;Schellenberger, M.&#160;Pauget, M.&#160;Taite, M.&#160;Boucard, Mme&#160;Petex, Mme&#160;Bazin-Malgras, M.&#160;Habert-Dassault, M.&#160;Ciotti et M.&#160;Vermorel-Marques"}, "pointeurFragmentTexte": {"partieAmendableRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "division": {"titre": "Article PREMIER", "articleDesignationCourte": "ART. PREMIER", "articleDesignation": "ARTICLE PREMIER", "type": "ARTICLE", "avant_A_Apres": "A", "divisionRattachee": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "articleAdditionnel": "false", "chapitreAdditionnel": "false", "urlDivisionTexteVise": "/16/textes/2600.asp#D_Article_1er"}, "amendementStandard": {"alinea": {"avant_A_Apres": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "numero": "33", "alineaDesignation": "Alin\u00e9a 33"}}}, "corps": {"cartoucheInformatif": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenuAuteur": {"dispositif": "<p style=\"text-align: justify;\">R&#x00E9;diger ainsi l&#x2019;alin&#x00E9;a&nbsp;33&nbsp;:</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;&#x00C0; ce titre, elle oriente l&#x2019;installation en agriculture aux fins de pr&#x00E9;server et d&#x2019;am&#x00E9;liorer la souverainet&#x00E9; alimentaire et &#x00E9;nerg&#x00E9;tique, de mani&#x00E8;re adapt&#x00E9;e aux enjeux de chaque territoire, et vers des syst&#x00E8;mes de production diversifi&#x00E9;s et viables humainement, &#x00E9;conomiquement et &#x00E9;cologiquement, &#x00E0; travers des mesures visant &#x00E0;&nbsp;:&nbsp;&#x00BB;</p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p style=\"text-align: justify;\">La r&#x00E9;daction actuelle de l&#x2019;alin&#x00E9;a 14 de l&#x2019;article 1 introduit la notion d&#x2019;orientation &#x201C;prioritaire&#x201D; de la politique d&#x2019;installation et de transmission au profit de &#x201C;secteurs strat&#x00E9;giques pour la souverainet&#x00E9; alimentaire&#x201D;. Une telle d&#x00E9;marche de priorisation pourrait aboutir &#x00E0; des situations de blocage pour des exploitation pourtant viable &#x00E9;conomiquement. De plus, le march&#x00E9; ne permet pas de prioriser un secteur agricole au d&#x00E9;triment d&#x2019;un autre. Cet amendement revient donc sur cette notion, tout en confortant le lien entre la politique d&#x2019;installation et la n&#x00E9;cessaire pr&#x00E9;servation et am&#x00E9;lioration de la souverainet&#x00E9; alimentaire et &#x00E9;nerg&#x00E9;tique.</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2024-05-06", "datePublication": "2024-05-13", "soumisArticle40": "false", "etatDesTraitements": {"etat": {"code": "DI", "libelle": "Discut\u00e9"}, "sousEtat": {"code": "DI", "libelle": "Tomb\u00e9"}}, "dateSort": "2024-05-17T00:28:17+02:00", "sort": "Tomb\u00e9"}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L16PO791932BTC2600P0D1N000005?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "seanceDiscussionRef": "RUANR5L16S2024IDS28361", "article99": "false", "loiReference": {"codeLoi": "0.1", "divisionCodeLoi": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "discussionCommune": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionIdentique": {"idDiscussion": "70060", "typePosition": "Premier"}, "accordGouvernementDepotHorsDelai": "Sans objet"}}