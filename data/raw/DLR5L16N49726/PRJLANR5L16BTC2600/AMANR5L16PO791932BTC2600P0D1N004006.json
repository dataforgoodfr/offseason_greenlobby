{"amendement": {"@xmlns": "http://schemas.assemblee-nationale.fr/referentiel", "uid": "AMANR5L16PO791932BTC2600P0D1N004006", "chronotag": "1715898626858", "legislature": "16", "identification": {"numeroLong": "4006", "numeroOrdreDepot": "4006", "prefixeOrganeExamen": "AN", "numeroRect": "0"}, "examenRef": "EXANR5L16PO791932BTC2600P0D1", "texteLegislatifRef": "PRJLANR5L16BTC2600", "triAmendement": "acaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaapfvo", "cardinaliteAmdtMultiples": "1", "amendementParentRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "signataires": {"auteur": {"typeAuteur": "D\u00e9put\u00e9", "gouvernementRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "acteurRef": "PA266788", "groupePolitiqueRef": "PO800532", "auteurRapporteurOrganeRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "cosignataires": {"acteurRef": ["PA605069", "PA793796", "PA719286", "PA719146", "PA721046", "PA719138", "PA267766", "PA720696", "PA267042", "PA822617", "PA795244", "PA720976", "PA607619", "PA721486", "PA333421", "PA793210", "PA794626", "PA2952"]}, "suffixe": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "libelle": "M.&#160;Morel-&#192;-L&apos;Huissier,  M.&#160;Jean-Louis&#160;Bricout, M.&#160;Taupiac, M.&#160;Colombani, M.&#160;Acquaviva, Mme&#160;Bassire, M.&#160;Castellani, M.&#160;Guy&#160;Bricout, Mme&#160;Descamps, M.&#160;Favennec-B&#233;cot&#160;, Mme&#160;Froger, M.&#160;Lenormand, M.&#160;Mathiasin, M.&#160;Molac, M.&#160;Naegelen, M.&#160;Pancher, M.&#160;Panifous, M.&#160;Saint-Huile et M.&#160;Warsmann"}, "pointeurFragmentTexte": {"partieAmendableRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "division": {"titre": "Article PREMIER", "articleDesignationCourte": "ART. PREMIER", "articleDesignation": "ARTICLE PREMIER", "type": "ARTICLE", "avant_A_Apres": "A", "divisionRattachee": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "articleAdditionnel": "false", "chapitreAdditionnel": "false", "urlDivisionTexteVise": "/16/textes/2600.asp#D_Article_1er"}, "amendementStandard": {"alinea": {"avant_A_Apres": "Apres", "numero": "24", "alineaDesignation": "Apr\u00e8s l'alin\u00e9a 24"}}}, "corps": {"cartoucheInformatif": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenuAuteur": {"dispositif": "<p style=\"text-align: justify;\">Apr&#x00E8;s l&#x2019;alin&#x00E9;a&nbsp;24, ins&#x00E9;rer&nbsp;l&#x2019;alin&#x00E9;a suivant&nbsp;:</p><p style=\"text-align: justify;\"><i>&#x00AB;&nbsp;<i>d&nbsp;<i>bis</i>)</i> </i>Soutenir le d&#x00E9;veloppement de l&#x2019;&#x00E9;levage en p&#x00E2;turage, et garantir les moyens de sa protection&nbsp;;&nbsp;&#x00BB;</p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p style=\"text-align: justify;\">Cet amendement vise &#x00E0; soutenir l'&#x00E9;levage en p&#x00E2;turage. Ce mode d'&#x00E9;levage,&nbsp;respectueux de l'environnement, pr&#x00E9;sente &#x00E9;galement des b&#x00E9;n&#x00E9;fice sur le bien-&#x00EA;tre animal et qui joue un r&#x00F4;le d&#x00E9;terminant d'am&#x00E9;nagement dans la ruralit&#x00E9;.&nbsp;</p><p style=\"text-align: justify;\">Les auteurs de cet amendement appellent notamment &#x00E0; :&nbsp;</p><p style=\"text-align: justify;\">&nbsp;- ouvrir&nbsp;le champ des esp&#x00E8;ces admises au p&#x00E2;turage, pour y inclure les caprins, les lamas et les alpaga;&nbsp;</p><p style=\"text-align: justify;\">- Mieux prot&#x00E9;ger le b&#x00E9;tails et donc d&#x2019;am&#x00E9;liorer la lutte contre la pr&#x00E9;dation des loups, en&nbsp;renfor&#x00E7;ant les lieutenants de louverie et en g&#x00E9;n&#x00E9;ralisant les brigades loups.</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2024-05-10", "datePublication": "2024-05-15", "soumisArticle40": "false", "etatDesTraitements": {"etat": {"code": "DI", "libelle": "Discut\u00e9"}, "sousEtat": {"code": "DI", "libelle": "Tomb\u00e9"}}, "dateSort": "2024-05-17T00:28:48+02:00", "sort": "Tomb\u00e9"}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L16PO791932BTC2600P0D1N004006?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "seanceDiscussionRef": "RUANR5L16S2024IDS28361", "article99": "false", "loiReference": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionCommune": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionIdentique": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "accordGouvernementDepotHorsDelai": "Sans objet"}}