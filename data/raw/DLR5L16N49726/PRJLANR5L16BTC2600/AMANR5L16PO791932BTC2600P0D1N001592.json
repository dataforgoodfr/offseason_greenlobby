{"amendement": {"@xmlns": "http://schemas.assemblee-nationale.fr/referentiel", "uid": "AMANR5L16PO791932BTC2600P0D1N001592", "chronotag": "1716499285167", "legislature": "16", "identification": {"numeroLong": "1592", "numeroOrdreDepot": "1592", "prefixeOrganeExamen": "AN", "numeroRect": "0"}, "examenRef": "EXANR5L16PO791932BTC2600P0D1", "texteLegislatifRef": "PRJLANR5L16BTC2600", "triAmendement": "esaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaadcoo", "cardinaliteAmdtMultiples": "1", "amendementParentRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "signataires": {"auteur": {"typeAuteur": "D\u00e9put\u00e9", "gouvernementRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "acteurRef": "PA793980", "groupePolitiqueRef": "PO800526", "auteurRapporteurOrganeRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "cosignataires": {"acteurRef": ["PA793664", "PA793780", "PA795362", "PA795454", "PA794008", "PA795010", "PA794022", "PA794146", "PA795636", "PA795794", "PA793708", "PA794154", "PA794778", "PA795076", "PA610002", "PA795808", "PA720952", "PA736201", "PA793948"]}, "suffixe": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "libelle": "M.&#160;Fournier,  Mme&#160;Pochon, Mme&#160;Arrighi, Mme&#160;Belluco, M.&#160;Ben&#160;Cheikh, Mme&#160;Chatelain, Mme&#160;Garin, M.&#160;Iordanoff, Mme&#160;Laernoes, M.&#160;Lucas-Lundy, Mme&#160;Pasquini, M.&#160;Peytavie, M.&#160;Raux, Mme&#160;Regol, Mme&#160;Rousseau, Mme&#160;Sas, Mme&#160;Sebaihi, M.&#160;Tach&#233;, Mme&#160;Taill&#233;-Polian et M.&#160;Thierry"}, "pointeurFragmentTexte": {"partieAmendableRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "division": {"titre": "Article 10", "articleDesignationCourte": "ART. 10", "articleDesignation": "ARTICLE 10", "type": "ARTICLE", "avant_A_Apres": "A", "divisionRattachee": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "articleAdditionnel": "false", "chapitreAdditionnel": "false", "urlDivisionTexteVise": "/16/textes/2600.asp#D_Article_10"}, "amendementStandard": {"alinea": {"avant_A_Apres": "Apres", "numero": "9", "alineaDesignation": "Apr\u00e8s l'alin\u00e9a 9"}}}, "corps": {"cartoucheInformatif": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenuAuteur": {"dispositif": "<p style=\"text-align: justify;\">Apr&#x00E8;s l'alin&#x00E9;a&nbsp;9, ins&#x00E9;rer l'alin&#x00E9;a suivant&nbsp;:</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;Ces informations sont communiqu&#x00E9;es &#x00E0; la soci&#x00E9;t&#x00E9; d&#x2019;am&#x00E9;nagement foncier et d&#x2019;&#x00E9;tablissement rural qui les met imm&#x00E9;diatement &#x00E0; disposition du public.&nbsp;&#x00BB;</p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p style=\"text-align: justify;\">Le manque d&#x2019;informations sur la disponibilit&#x00E9; du foncier est un frein bien identifi&#x00E9; depuis de nombreuses ann&#x00E9;es &#x00E0; la transmission des exploitations. Face &#x00E0; ce constat, cet amendement issu d&#x2019;une proposition de Terre de Liens et AGTER vise &#x00E0; indiquer clairement que les d&#x00E9;clarations d&#x2019;intention de cessation d&#x2019;activit&#x00E9; doivent non seulement &#x00EA;tre enregistr&#x00E9;es dans un registre d&#x00E9;partemental unique, comme le pr&#x00E9;voit le texte, mais aussi &#x00EA;tre transmises &#x00E0; la soci&#x00E9;t&#x00E9; d&#x2019;am&#x00E9;nagement foncier et d&#x2019;&#x00E9;tablissement rural qui devra ensuite les mettre &#x00E0; disposition du public, afin que ces d&#x00E9;clarations soient connues de tous ceux qui souhaitent s&#x2019;installer ou s&#x2019;agrandir.</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2024-05-07", "datePublication": "2024-05-17", "soumisArticle40": "false", "etatDesTraitements": {"etat": {"code": "DI", "libelle": "Discut\u00e9"}, "sousEtat": {"code": "DI", "libelle": "Rejet\u00e9"}}, "dateSort": "2024-05-23T23:21:20+02:00", "sort": "Rejet\u00e9"}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L16PO791932BTC2600P0D1N001592?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "seanceDiscussionRef": "RUANR5L16S2024IDS28371", "article99": "false", "loiReference": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionCommune": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionIdentique": {"idDiscussion": "70569", "typePosition": "Milieu"}, "accordGouvernementDepotHorsDelai": "Sans objet"}}