{"amendement": {"@xmlns": "http://schemas.assemblee-nationale.fr/referentiel", "uid": "AMANR5L16PO791932BTC2600P0D1N000107", "chronotag": "1715982290908", "legislature": "16", "identification": {"numeroLong": "107", "numeroOrdreDepot": "107", "prefixeOrganeExamen": "AN", "numeroRect": "0"}, "examenRef": "EXANR5L16PO791932BTC2600P0D1", "texteLegislatifRef": "PRJLANR5L16BTC2600", "triAmendement": "biaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaah", "cardinaliteAmdtMultiples": "1", "amendementParentRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "signataires": {"auteur": {"typeAuteur": "D\u00e9put\u00e9", "gouvernementRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "acteurRef": "PA774954", "groupePolitiqueRef": "PO800508", "auteurRapporteurOrganeRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "cosignataires": {"acteurRef": ["PA642847", "PA721410", "PA266793", "PA342240", "PA793508", "PA722390", "PA330909", "PA793476", "PA608826", "PA266797", "PA794058", "PA608416", "PA608016", "PA795406", "PA793094", "PA794410", "PA721474"]}, "suffixe": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "libelle": "Mme&#160;Blin,  M.&#160;Bazin, Mme&#160;Bonnivard, M.&#160;Bony, M.&#160;Bourgeaux, M.&#160;Brigand, Mme&#160;Corneloup, M.&#160;Descoeur, M.&#160;Dubois, Mme&#160;Duby-Muller, M.&#160;Gosselin, Mme&#160;Gruet, M.&#160;Hetzel, Mme&#160;Louwagie, Mme&#160;P&#233;rigault, M.&#160;Ray, M.&#160;Seitlinger et M.&#160;Viry"}, "pointeurFragmentTexte": {"partieAmendableRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "division": {"titre": "Article 3", "articleDesignationCourte": "ART. 3", "articleDesignation": "ARTICLE 3", "type": "ARTICLE", "avant_A_Apres": "A", "divisionRattachee": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "articleAdditionnel": "false", "chapitreAdditionnel": "false", "urlDivisionTexteVise": "/16/textes/2600.asp#D_Article_3"}, "amendementStandard": {"alinea": {"avant_A_Apres": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "numero": "6", "alineaDesignation": "Alin\u00e9a 6"}}}, "corps": {"cartoucheInformatif": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenuAuteur": {"dispositif": "<p style=\"text-align: justify;\">&#x00C0; la fin de la premi&#x00E8;re phrase de l&#x2019;alin&#x00E9;a&nbsp;6, supprimer les mots&nbsp;:&nbsp;</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;et de sensibilisation au bien-&#x00EA;tre animal&nbsp;&#x00BB;.</p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p style=\"text-align: justify;\">Quelle est la finalit&#x00E9; de cette disposition?&nbsp;</p><p style=\"text-align: justify;\">Serait-ce l'id&#x00E9;e sous-jacente qu'agriculture et bien-&#x00EA;tre animal ne sont pas compatibles comme le pr&#x00E9;tendent les militants animalistes et antisp&#x00E9;cistes?&nbsp;</p><p style=\"text-align: justify;\">Intrins&#x00E8;quement, et bien &#x00E9;videmment, les &#x00E9;leveurs sont les premiers &#x00E0; aimer et prendre soin de leurs b&#x00EA;tes. Cette disposition, si elle n'est pas militante, est a minima provocatrice et n'a donc pas sa place dans ce projet de loi.</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2024-05-06", "datePublication": "2024-05-14", "soumisArticle40": "false", "etatDesTraitements": {"etat": {"code": "DI", "libelle": "Discut\u00e9"}, "sousEtat": {"code": "DI", "libelle": "Rejet\u00e9"}}, "dateSort": "2024-05-17T23:44:43+02:00", "sort": "Rejet\u00e9"}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L16PO791932BTC2600P0D1N000107?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "seanceDiscussionRef": "RUANR5L16S2024IDS28364", "article99": "false", "loiReference": {"codeLoi": "1", "divisionCodeLoi": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "discussionCommune": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionIdentique": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "accordGouvernementDepotHorsDelai": "Sans objet"}}