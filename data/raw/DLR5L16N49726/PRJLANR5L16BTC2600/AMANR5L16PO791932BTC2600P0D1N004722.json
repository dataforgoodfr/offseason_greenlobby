{"amendement": {"@xmlns": "http://schemas.assemblee-nationale.fr/referentiel", "uid": "AMANR5L16PO791932BTC2600P0D1N004722", "chronotag": "1716499915024", "legislature": "16", "identification": {"numeroLong": "4722", "numeroOrdreDepot": "4722", "prefixeOrganeExamen": "AN", "numeroRect": "0"}, "examenRef": "EXANR5L16PO791932BTC2600P0D1", "texteLegislatifRef": "PRJLANR5L16BTC2600", "triAmendement": "esaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaddcg", "cardinaliteAmdtMultiples": "1", "amendementParentRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "signataires": {"auteur": {"typeAuteur": "D\u00e9put\u00e9", "gouvernementRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "acteurRef": "PA722398", "groupePolitiqueRef": "PO800538", "auteurRapporteurOrganeRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "cosignataires": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "suffixe": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "libelle": "M.&#160;Rebeyrotte"}, "pointeurFragmentTexte": {"partieAmendableRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "division": {"titre": "Article 10", "articleDesignationCourte": "ART. 10", "articleDesignation": "ARTICLE 10", "type": "ARTICLE", "avant_A_Apres": "A", "divisionRattachee": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "articleAdditionnel": "false", "chapitreAdditionnel": "false", "urlDivisionTexteVise": "/16/textes/2600.asp#D_Article_10"}, "amendementStandard": {"alinea": {"avant_A_Apres": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "numero": "12", "alineaDesignation": "Alin\u00e9a 12"}}}, "corps": {"cartoucheInformatif": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenuAuteur": {"dispositif": "<p style=\"text-align: justify;\">R&#x00E9;diger ainsi l&#x2019;alin&#x00E9;a&nbsp;12&nbsp;:</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;<i>Art.&nbsp;L.&nbsp;330&#x2011;6</i>. &#x2013; I. &#x2013; Tout porteur d&#x2019;un projet d&#x2019;installation en vue d&#x2019;exercer une activit&#x00E9; agricole,&nbsp;au sens de l&#x2019;article L.&nbsp;311&#x2011;1, ou d&#x2019;un projet de cession d&#x2019;une exploitation agricole ou &#x00E0; un moment cl&#x00E9; de sa vie entrepreneuriale prend contact avec le point d&#x2019;accueil pr&#x00E9;vu au 4&#x00B0;&nbsp;de&nbsp;l&#x2019;article L.&nbsp;511&#x2011;4 comp&#x00E9;tent pour le d&#x00E9;partement dans lequel il envisage son installation, ou dans lequel se situe le si&#x00E8;ge de l&#x2019;exploitation &#x00E0; c&#x00E9;der.&nbsp;&#x00BB;</p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p style=\"text-align: justify;\">Cet amendement vise &#x00E0; proposer un accompagnement aussi qualitatif lors de l&#x2019;installation,</p><p>que de la transmission ou de moments cl&#x00E9;s de la carri&#x00E8;re de l&#x2019;agriculteur. La qualit&#x00E9; des</p><p>services offerts par France Services Agriculture doivent profiter &#x00E0; l&#x2019;ensemble des agriculteurs</p><p>lors des &#x00E9;tapes strat&#x00E9;giques de leur vie d&#x2019;entrepreneur</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2024-05-10", "datePublication": "2024-05-17", "soumisArticle40": "false", "etatDesTraitements": {"etat": {"code": "DI", "libelle": "Discut\u00e9"}, "sousEtat": {"code": "DI", "libelle": "Retir\u00e9"}}, "dateSort": "2024-05-23T23:31:26+02:00", "sort": "Retir\u00e9"}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L16PO791932BTC2600P0D1N004722?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "seanceDiscussionRef": "RUANR5L16S2024IDS28371", "article99": "false", "loiReference": {"codeLoi": "c rural", "divisionCodeLoi": "L330-6"}, "discussionCommune": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionIdentique": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "accordGouvernementDepotHorsDelai": "Sans objet"}}