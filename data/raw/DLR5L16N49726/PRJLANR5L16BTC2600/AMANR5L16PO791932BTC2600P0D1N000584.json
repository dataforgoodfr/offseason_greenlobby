{"amendement": {"@xmlns": "http://schemas.assemblee-nationale.fr/referentiel", "uid": "AMANR5L16PO791932BTC2600P0D1N000584", "chronotag": "1715898625176", "legislature": "16", "identification": {"numeroLong": "584", "numeroOrdreDepot": "584", "prefixeOrganeExamen": "AN", "numeroRect": "0"}, "examenRef": "EXANR5L16PO791932BTC2600P0D1", "texteLegislatifRef": "PRJLANR5L16BTC2600", "triAmendement": "acaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaacai", "cardinaliteAmdtMultiples": "1", "amendementParentRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "signataires": {"auteur": {"typeAuteur": "D\u00e9put\u00e9", "gouvernementRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "acteurRef": "PA330909", "groupePolitiqueRef": "PO800508", "auteurRapporteurOrganeRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "cosignataires": {"acteurRef": ["PA266793", "PA721410", "PA267901", "PA720586", "PA794178", "PA793508", "PA795406", "PA721442", "PA793094", "PA607090", "PA719272", "PA608416", "PA608826", "PA1327", "PA721816", "PA722308"]}, "suffixe": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "libelle": "M.&#160;Descoeur,  M.&#160;Bony, Mme&#160;Bonnivard, M.&#160;Kamardine, M.&#160;Vatin, M.&#160;Taite, M.&#160;Brigand, Mme&#160;P&#233;rigault, Mme&#160;Petex, M.&#160;Ray, M.&#160;Jean-Pierre&#160;Vigier, Mme&#160;Fr&#233;d&#233;rique&#160;Meunier, M.&#160;Hetzel, Mme&#160;Duby-Muller, M.&#160;Forissier, M.&#160;Boucard et Mme&#160;Serre"}, "pointeurFragmentTexte": {"partieAmendableRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "division": {"titre": "Article PREMIER", "articleDesignationCourte": "ART. PREMIER", "articleDesignation": "ARTICLE PREMIER", "type": "ARTICLE", "avant_A_Apres": "A", "divisionRattachee": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "articleAdditionnel": "false", "chapitreAdditionnel": "false", "urlDivisionTexteVise": "/16/textes/2600.asp#D_Article_1er"}, "amendementStandard": {"alinea": {"avant_A_Apres": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "numero": "3", "alineaDesignation": "Alin\u00e9a 3"}}}, "corps": {"cartoucheInformatif": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenuAuteur": {"dispositif": "<p style=\"text-align: justify;\">&#x00C0; l&#x2019;alin&#x00E9;a&nbsp;3, apr&#x00E8;s le mot&nbsp;:&nbsp;</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;agriculture,&nbsp;&#x00BB;,</p><p style=\"text-align: justify;\">ins&#x00E9;rer les mots&nbsp;:</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;de l&#x2019;&#x00E9;levage,&nbsp;&#x00BB;.</p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p style=\"text-align: justify;\">Il convient &#x00E0; travers cet amendement d&#x2019;&#x00E9;voquer l&#x2019;&#x00E9;levage, le grand oubli&#x00E9; du projet de loi. Rien n&#x2019;est pr&#x00E9;vu dans ce texte pour encourager l&#x2019;investissement et la modernisation des &#x00E9;levages. Alors qu&#x2019;aujourd&#x2019;hui, on assiste &#x00E0; un basculement g&#x00E9;n&#x00E9;rationnel tr&#x00E8;s important, qui va entra&#x00EE;ner une baisse de la production, et que le revenu des &#x00E9;leveurs s&#x2019;effrite, rien n&#x2019;est propos&#x00E9; pour soutenir le secteur de l&#x2019;&#x00E9;levage. Alors que le Ministre nous avait assur&#x00E9; le 7 juin dernier que l&#x2019;&#x00E9;levage figurerait bien parmi les sujets importants du Pacte et de la Loi d&#x2019;orientation et d&#x2019;avenir agricoles (PLOA), aucune mesure sp&#x00E9;cifique n&#x2019;est propos&#x00E9;e pour cette fili&#x00E8;re, pourtant essentielle pour l&#x2019;am&#x00E9;nagement et l&#x2019;occupation de notre territoire, l&#x2019;&#x00E9;conomie agricole et la souverainet&#x00E9; alimentaire de notre pays.</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2024-05-07", "datePublication": "2024-05-13", "soumisArticle40": "false", "etatDesTraitements": {"etat": {"code": "DI", "libelle": "Discut\u00e9"}, "sousEtat": {"code": "DI", "libelle": "Tomb\u00e9"}}, "dateSort": "2024-05-17T00:28:20+02:00", "sort": "Tomb\u00e9"}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L16PO791932BTC2600P0D1N000584?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "seanceDiscussionRef": "RUANR5L16S2024IDS28361", "article99": "false", "loiReference": {"codeLoi": "1", "divisionCodeLoi": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "discussionCommune": {"idDiscussion": "24939", "typePosition": "Premier"}, "discussionIdentique": {"idDiscussion": "70373", "typePosition": "Premier"}, "accordGouvernementDepotHorsDelai": "Sans objet"}}