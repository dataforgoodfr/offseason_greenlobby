{"amendement": {"@xmlns": "http://schemas.assemblee-nationale.fr/referentiel", "uid": "AMANR5L16PO791932BTC2600P0D1N001722", "chronotag": "1715930180900", "legislature": "16", "identification": {"numeroLong": "1722", "numeroOrdreDepot": "1722", "prefixeOrganeExamen": "AN", "numeroRect": "0"}, "examenRef": "EXANR5L16PO791932BTC2600P0D1", "texteLegislatifRef": "PRJLANR5L16BTC2600", "triAmendement": "adaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaarbfavbzaaaaaahi", "cardinaliteAmdtMultiples": "1", "amendementParentRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "signataires": {"auteur": {"typeAuteur": "D\u00e9put\u00e9", "gouvernementRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "acteurRef": "PA722390", "groupePolitiqueRef": "PO800508", "auteurRapporteurOrganeRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "cosignataires": {"acteurRef": ["PA608416", "PA643134", "PA721474", "PA720586", "PA718884", "PA642847", "PA793094", "PA330909", "PA795406", "PA607090", "PA793508", "PA722308"]}, "suffixe": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "libelle": "Mme&#160;Corneloup,  M.&#160;Hetzel, Mme&#160;Valentin, M.&#160;Viry, M.&#160;Vatin, Mme&#160;Bazin-Malgras, M.&#160;Bazin, M.&#160;Ray, M.&#160;Descoeur, Mme&#160;P&#233;rigault, M.&#160;Jean-Pierre&#160;Vigier, M.&#160;Brigand et Mme&#160;Serre"}, "pointeurFragmentTexte": {"partieAmendableRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "division": {"titre": "Article PREMIER", "articleDesignationCourte": "APR\u00c8S ART. PREMIER", "articleDesignation": "APR\u00c8S L'ARTICLE PREMIER, ins\u00e9rer l'article suivant:", "type": "ARTICLE", "avant_A_Apres": "Apr\u00e8s", "divisionRattachee": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "articleAdditionnel": "true", "chapitreAdditionnel": "false", "urlDivisionTexteVise": "/16/textes/2600.asp#D_Article_1er"}, "amendementStandard": {"alinea": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "corps": {"cartoucheInformatif": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenuAuteur": {"dispositif": "<p style=\"text-align: justify;\">Apr&#x00E8;s l&#x2019;article L.&nbsp;1 du code rural et de la p&#x00EA;che maritime, il est ins&#x00E9;r&#x00E9;&nbsp;un article L.&nbsp;1&#x2011;1&nbsp;ainsi r&#x00E9;dig&#x00E9;&nbsp;:<br><br>&#x00AB;&nbsp;<i>Art.&nbsp;L.&nbsp;1&#x2011;1.</i> &#x2013; Les politiques publiques concourent &#x00E0; la promotion active de la souverainet&#x00E9; alimentaire. Tout projet de loi ou toute proposition de loi int&#x00E9;ressant l&#x2019;agriculture doit s&#x2019;ins&#x00E9;rer dans l&#x2019;objectif g&#x00E9;n&#x00E9;ral de production locale et durable.&nbsp;&#x00BB;</p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p style=\"text-align: justify;\">Cet amendement a pour objectif d&#x2019;assurer que l&#x2019;ensemble du projet de loi ainsi que des projets et propositions de loi futurs int&#x00E8;grent la n&#x00E9;cessit&#x00E9; d&#x2019;une production agricole locale et durable. L&#x2019;article 1 B du code rural et de la p&#x00EA;che maritime a pour ambition d&#x2019;assurer que la reconnaissance de la souverainet&#x00E9; alimentaire, impliquant une production locale et durable, ne soit pas seulement un texte d&#x2019;affichage mais qu&#x2019;il soit contraignant dans les initiatives futures.</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2024-05-08", "datePublication": "2024-05-13", "soumisArticle40": "false", "etatDesTraitements": {"etat": {"code": "DI", "libelle": "Discut\u00e9"}, "sousEtat": {"code": "DI", "libelle": "Retir\u00e9"}}, "dateSort": "2024-05-17T09:16:04+02:00", "sort": "Retir\u00e9"}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L16PO791932BTC2600P0D1N001722?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "seanceDiscussionRef": "RUANR5L16S2024IDS28362", "article99": "false", "loiReference": {"codeLoi": "c rural", "divisionCodeLoi": "L1-1"}, "discussionCommune": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionIdentique": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "accordGouvernementDepotHorsDelai": "Sans objet"}}