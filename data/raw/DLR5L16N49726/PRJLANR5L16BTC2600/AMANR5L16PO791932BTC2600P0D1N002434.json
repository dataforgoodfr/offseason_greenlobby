t_A_Apres": "A", "divisionRattachee": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "articleAdditionnel": "false", "chapitreAdditionnel": "false", "urlDivisionTexteVise": "/16/textes/2600.asp#D_Article_1er"}, "amendementStandard": {"alinea": {"avant_A_Apres": null, "numero": "29", "alineaDesignation": "Alin\u00e9a 29"}}}, "corps": {"cartoucheInformatif": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenuAuteur": {"dispositif": "<p style=\"text-align: justify;\">R&#x00E9;diger ainsi l&#x2019;alin&#x00E9;a&nbsp;29&nbsp;:</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;<i>i)</i> R&#x00E9;duire notre d&#x00E9;pendance en mati&#x00E8;re d&#x2019;importations&nbsp;;&nbsp;&#x00BB;.</p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p style=\"text-align: justify;\">Les auteurs de cet amendement souhaitent que les actions des politiques publiques en mati&#x00E8;re agricole, de la p&#x00EA;che et de l&#x2019;aquaculture ne vise pas seulement &#x00E0; &#x00AB; ma&#x00EE;triser &#x00BB; mais bien &#x00E0; &#x00AB;&nbsp;r&#x00E9;durie &#x00BB; les d&#x00E9;pendances en mati&#x00E8;re d&#x2019;importations.<br><br></p><p>La croissance vertigineuse des importations de produits agricoles, aquacoles et de la p&#x00EA;che a fait l&#x2019;objet de nombreux rapports et analyses qui doivent aujourd&#x2019;hui d&#x00E9;boucher sur la d&#x00E9;clinaison de politiques publiques concr&#x00E8;tes fixant de v&#x00E9;ritables objectifs pour diminuer le recours aux produits import&#x00E9;s, en priorisant les productions et fili&#x00E8;res les plus impact&#x00E9;es ou d&#x00E9;ficitaires, et les importations les plus probl&#x00E9;matiques en mati&#x00E8;re de qualit&#x00E9; sanitaire, sociale et environnementale des productions dans les pays tiers.</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2024-05-09", "datePublication": "2024-05-13", "soumisArticle40": "false", "etatDesTraitements": {"etat": {"code": "DI", "libelle": "Discut\u00e9"}, "sousEtat": {"code": "DI", "libelle": "Tomb\u00e9"}}, "dateSort": "2024-05-17T00:28:40+02:00", "sort": "Tomb\u00e9"}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L16PO791932BTC2600P0D1N002434?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "seanceDiscussionRef": "RUANR5L16S2024IDS28361", "article99": "false", "loiReference": {"codeLoi": "0.1", "divisionCodeLoi": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "discussionCommune": {"idDiscussion": "24891", "typePosition": "Dernier"}, "discussionIdentique": {"idDiscussion": "70393", "typePosition": "Dernier"}, "accordGouvernementDepotHorsDelai": "Sans objet"}}