{"amendement": {"@xmlns": "http://schemas.assemblee-nationale.fr/referentiel", "uid": "AMANR5L16PO791932BTC2600P0D1N003193", "chronotag": "1716570385036", "legislature": "16", "identification": {"numeroLong": "3193", "numeroOrdreDepot": "3193", "prefixeOrganeExamen": "AN", "numeroRect": "0"}, "examenRef": "EXANR5L16PO791932BTC2600P0D1", "texteLegislatifRef": "PRJLANR5L16BTC2600", "triAmendement": "koaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaalux", "cardinaliteAmdtMultiples": "1", "amendementParentRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "signataires": {"auteur": {"typeAuteur": "D\u00e9put\u00e9", "gouvernementRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "acteurRef": "PA795362", "groupePolitiqueRef": "PO800526", "auteurRapporteurOrganeRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "cosignataires": {"acteurRef": ["PA793664", "PA793780", "PA335999", "PA795454", "PA794008", "PA793980", "PA795010", "PA794022", "PA794146", "PA795636", "PA795794", "PA793708", "PA794154", "PA794778", "PA795076", "PA610002", "PA795808", "PA720952", "PA736201", "PA793948"]}, "suffixe": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "libelle": "Mme&#160;Belluco,  Mme&#160;Pochon, Mme&#160;Arrighi, Mme&#160;Batho, M.&#160;Ben&#160;Cheikh, Mme&#160;Chatelain, M.&#160;Fournier, Mme&#160;Garin, M.&#160;Iordanoff, Mme&#160;Laernoes, M.&#160;Lucas-Lundy, Mme&#160;Pasquini, M.&#160;Peytavie, M.&#160;Raux, Mme&#160;Regol, Mme&#160;Rousseau, Mme&#160;Sas, Mme&#160;Sebaihi, M.&#160;Tach&#233;, Mme&#160;Taill&#233;-Polian et M.&#160;Thierry"}, "pointeurFragmentTexte": {"partieAmendableRef": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "division": {"titre": "Article 14", "articleDesignationCourte": "ART. 14", "articleDesignation": "ARTICLE 14", "type": "ARTICLE", "avant_A_Apres": "A", "divisionRattachee": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "articleAdditionnel": "false", "chapitreAdditionnel": "false", "urlDivisionTexteVise": "/16/textes/2600.asp#D_Article_14"}, "amendementStandard": {"alinea": {"avant_A_Apres": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "numero": "9", "alineaDesignation": "Alin\u00e9a 9"}}}, "corps": {"cartoucheInformatif": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenuAuteur": {"dispositif": "<p style=\"text-align: justify;\">I.&nbsp;&#x2013;&nbsp;&#x00C0; la premi&#x00E8;re phrase de l&#x2019;alin&#x00E9;a&nbsp;9, substituer au mot&nbsp;:</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;s&#x2019;opposer&nbsp;&#x00BB;</p><p style=\"text-align: justify;\">le mot&nbsp;:</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;autoriser&nbsp;&#x00BB;.</p><p style=\"text-align: justify;\">II.&nbsp;&#x2013;&nbsp;En cons&#x00E9;quence,&nbsp;substituer &#x00E0; la derni&#x00E8;re phrase du m&#x00EA;me alin&#x00E9;a la phrase suivante&nbsp;:</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;L&#x2019;absence d&#x2019;autorisation vaut opposition au projet de destruction au titre des l&#x00E9;gislations applicables au projet.&nbsp;&#x00BB;</p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p style=\"text-align: justify;\">L'objet de cet amendement est de garantir une instruction des dossiers conduisant &#x00E0; une meilleure protection des haies.</p><p>&#x00C9;tant donn&#x00E9;es l'importance &#x00E9;cologique, &#x00E9;conomique et agronomique des haies, et leur disparition, une logique de protection par d&#x00E9;faut est plus appropri&#x00E9;e qu'une autorisation par d&#x00E9;faut de destruction. D'o&#x00F9; le renversement de la logique propos&#x00E9; par cet amendement, avec un silence vaut refus plut&#x00F4;t qu'acceptation.</p><p>&nbsp;</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2024-05-10", "datePublication": "2024-05-17", "soumisArticle40": "false", "etatDesTraitements": {"etat": {"code": "DI", "libelle": "Discut\u00e9"}, "sousEtat": {"code": "DI", "libelle": "Rejet\u00e9"}}, "dateSort": "2024-05-24T19:06:24+02:00", "sort": "Rejet\u00e9"}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L16PO791932BTC2600P0D1N003193?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "seanceDiscussionRef": "RUANR5L16S2024IDS28373", "article99": "false", "loiReference": {"codeLoi": "1", "divisionCodeLoi": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "discussionCommune": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionIdentique": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "accordGouvernementDepotHorsDelai": "Sans objet"}}