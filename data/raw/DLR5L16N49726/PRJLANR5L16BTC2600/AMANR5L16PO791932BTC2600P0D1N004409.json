signationCourte": "ART. 14", "articleDesignation": "ARTICLE 14", "type": "ARTICLE", "avant_A_Apres": "A", "divisionRattachee": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "articleAdditionnel": "false", "chapitreAdditionnel": "false", "urlDivisionTexteVise": "/16/textes/2600.asp#D_Article_14"}, "amendementStandard": {"alinea": {"avant_A_Apres": "Apres", "numero": "29", "alineaDesignation": "Apr\u00e8s l'alin\u00e9a 29"}}}, "corps": {"cartoucheInformatif": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenuAuteur": {"dispositif": "<p style=\"text-align: justify;\">Apr&#x00E8;s l&#x2019;alin&#x00E9;a&nbsp;29,&nbsp;ins&#x00E9;rer l&#x2019;alin&#x00E9;a suivant&nbsp;:&nbsp;</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;Le pr&#x00E9;sent article ne s&#x2019;applique pas dans les cas de n&#x00E9;cessit&#x00E9; de d&#x00E9;broussaillement ou de maintien de l&#x2019;&#x00E9;tat d&#x00E9;broussaill&#x00E9; relevant de l&#x2019;article L.&nbsp;134&#x2011;10 du code forestier, et ou dans un cadre sanitaire relevant de l&#x2019;article&nbsp;350&#x2011;3 alin&#x00E9;a&nbsp;3 du code de l&#x2019;environnement&nbsp;&#x00BB;.&nbsp;</p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p style=\"text-align: justify;\">Le pr&#x00E9;sent article souhaite int&#x00E9;grer deux exceptions au nouvel article L.412-24 du code de l'environnement:</p><p style=\"text-align: justify;\">- l'int&#x00E9;r&#x00EA;t de s&#x00E9;curit&#x00E9; dans les cas de n&#x00E9;cessit&#x00E9; de d&#x00E9;broussaillements ou de maintien d'&#x00E9;tat d&#x00E9;broussaill&#x00E9;,&nbsp;</p><p style=\"text-align: justify;\">- l'int&#x00E9;r&#x00EA;t sanitaire &#x00E9;nonc&#x00E9; &#x00E0; l'alin&#x00E9;a 3 de l'article L350-3 du code de l'environnement, soumis &#x00E0; son propre syst&#x00E8;me de d&#x00E9;claration et d'autorisation.&nbsp;</p><p style=\"text-align: justify;\">Tel est le sens du pr&#x00E9;sent amendement.&nbsp;</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2024-05-10", "datePublication": "2024-05-17", "soumisArticle40": "false", "etatDesTraitements": {"etat": {"code": "DI", "libelle": "Discut\u00e9"}, "sousEtat": {"code": "DI", "libelle": "Rejet\u00e9"}}, "dateSort": "2024-05-24T19:29:43+02:00", "sort": "Rejet\u00e9"}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L16PO791932BTC2600P0D1N004409?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "seanceDiscussionRef": "RUANR5L16S2024IDS28373", "article99": "false", "loiReference": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionCommune": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "discussionIdentique": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "accordGouvernementDepotHorsDelai": "Sans objet"}}