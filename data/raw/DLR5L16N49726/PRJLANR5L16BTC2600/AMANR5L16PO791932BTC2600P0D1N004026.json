chee": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "articleAdditionnel": "false", "chapitreAdditionnel": "false", "urlDivisionTexteVise": "/16/textes/2600.asp#D_Article_12_bis"}, "amendementStandard": {"alinea": {"avant_A_Apres": "A", "numero": "2", "alineaDesignation": "Alin\u00e9a 2"}}}, "corps": {"cartoucheInformatif": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenuAuteur": {"dispositif": "<p style=\"text-align: justify;\">I.&nbsp;&#x2013;&nbsp;&#x00C0; l&#x2019;alin&#x00E9;a&nbsp;2, substituer aux mots&nbsp;:&nbsp;</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;dans la limite de 10 000&nbsp;euros&nbsp;&#x00BB;</p><p style=\"text-align: justify;\">les mots&nbsp;:&nbsp;</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;, dont la liste est d&#x00E9;finie par d&#x00E9;cret, dans la limite de 20 000&nbsp;euros&nbsp;&#x00BB;.</p><p style=\"text-align: justify;\">II.&nbsp;&#x2013;&nbsp;En cons&#x00E9;quence, &#x00E0; l&#x2019;alin&#x00E9;a&nbsp;4, substituer aux mots&nbsp;:&nbsp;</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;&#x00E0; hauteur de 10 000&nbsp;&#x00BB;&nbsp;</p><p style=\"text-align: justify;\">les mots&nbsp;:&nbsp;</p><p style=\"text-align: justify;\">&#x00AB;&nbsp;dont la liste est d&#x00E9;finie par d&#x00E9;cret, dans la limite de 20 000&nbsp;&#x00BB;.</p>", "avantAppel": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "exposeSommaire": "<p style=\"text-align: justify;\">L&#x2019;article 12 bis du pr&#x00E9;sent projet de loi vise &#x00E0; autoriser les soci&#x00E9;t&#x00E9;s civiles agricoles &#x00E0; pratiquer, &#x00E0; la marge, des activit&#x00E9;s commerciales accessoires &#x00E0; hauteur de 10 000 euros dans la limite de 50 % du chiffre d&#x2019;affaires, avec application de la transparence GAEC pour le seuil de 10 000 euros.&nbsp;</p><p>Le pr&#x00E9;sent amendement propose de passer ce seuil de 10 000 euros &#x00E0; 50 000 euros.</p><p>Cet amendement a &#x00E9;t&#x00E9; travaill&#x00E9; avec les Jeunes Agriculteurs.&nbsp;</p>", "annexeExposeSommaire": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "cycleDeVie": {"dateDepot": "2024-05-10", "datePublication": "2024-05-21", "soumisArticle40": "false", "etatDesTraitements": {"etat": {"code": "DI", "libelle": "Discut\u00e9"}, "sousEtat": {"code": "DI", "libelle": "Adopt\u00e9"}}, "dateSort": "2024-05-24T12:32:33+02:00", "sort": "Adopt\u00e9"}, "representations": {"representation": {"nom": "PDF", "typeMime": {"type": "application", "subType": "PDF"}, "statutRepresentation": {"verbatim": "true", "canonique": "true", "officielle": "true", "transcription": "false", "enregistrement": "false"}, "repSource": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "offset": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}, "contenu": {"documentURI": "/base/AMANR5L16PO791932BTC2600P0D1N004026?format=pdf"}, "dateDispoRepresentation": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}}, "seanceDiscussionRef": "RUANR5L16S2024IDS28372", "article99": "false", "loiReference": {"codeLoi": "1", "divisionCodeLoi": {"@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance", "@xsi:nil": "true"}}, "discussionCommune": {"idDiscussion": "25034", "typePosition": "Milieu"}, "discussionIdentique": {"idDiscussion": "70641", "typePosition": "Dernier"}, "accordGouvernementDepotHorsDelai": "Sans objet"}}